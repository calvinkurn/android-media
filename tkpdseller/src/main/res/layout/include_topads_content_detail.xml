<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_marginBottom="@dimen/topads_detail_padding_item"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/max_bid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/label_title_max_bid_topads"/>

    <include layout="@layout/include_separator_horizontal"/>

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/avg_cost"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/label_title_avg_cost_topads"/>

    <include layout="@layout/include_separator_horizontal"/>

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/start"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/label_title_start_topads"/>

    <include layout="@layout/include_separator_horizontal"/>

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/end"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/label_title_end_topads"/>

    <include layout="@layout/include_separator_horizontal"/>

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/daily_budget"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/label_title_daily_budget_topads"/>

    <include layout="@layout/include_separator_horizontal"/>

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/sent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/label_title_sent_topads"/>

    <include layout="@layout/include_separator_horizontal"/>

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/impr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/label_title_impr_topads"/>

    <include layout="@layout/include_separator_horizontal"/>

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/click"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/label_title_click_topads"/>

    <include layout="@layout/include_separator_horizontal"/>

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/ctr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:title="@string/label_title_ctr_topads"/>

    <include layout="@layout/include_separator_horizontal"/>

    <com.tokopedia.seller.topads.view.widget.TopAdsLabelView
        android:id="@+id/favorite"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/topads_detail_padding_item"
        app:title="@string/label_title_favorite_topads"/>

</merge>