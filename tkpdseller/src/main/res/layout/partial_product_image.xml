<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/view_group_product_image"
    style="@style/LabelLayout"
    android:layout_marginTop="@dimen/dp_20">

    <com.tokopedia.seller.product.edit.view.widget.ImagesSelectView
        android:id="@+id/image_select_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:addPictureSrc="@drawable/ic_add_product"
        app:imageLimit="5"
        app:primaryImageString="@string/prompt_main_picture"
        app:recyclerviewPadding="0dp"
        app:titleString="@string/product_upload_product_image"/>

</FrameLayout>