<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:showIn="@layout/include_topads_content_detail">

    <com.tokopedia.seller.lib.widget.LabelView
        android:id="@+id/max_bid"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:content_small="true"
        app:title="@string/label_top_ads_max_bid" />

    <com.tokopedia.seller.lib.widget.LabelView
        android:id="@+id/avg_cost"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:content_small="true"
        app:title="@string/label_top_ads_avg_cost" />
</merge>