<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/ic_shop_score_limit"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:src="@drawable/icon_picker_gold_badge" />

    <com.tokopedia.seller.shopscore.view.widget.ShopScoreMainProgressBar
        android:id="@+id/shop_score_progress_bar"
        android:layout_width="match_parent"
        android:layout_height="13dp"
        android:layout_below="@id/ic_shop_score_limit"
        app:rcBackgroundColor="@color/grey_300"
        app:rcMax="100"
        app:rcRadius="20dp" />

    <View
        android:id="@+id/shop_score_view_limit"
        android:layout_width="1dp"
        android:layout_height="0dp"
        android:layout_alignBottom="@id/shop_score_progress_bar"
        android:layout_alignTop="@id/shop_score_progress_bar"
        android:layout_below="@id/ic_shop_score_limit"
        android:background="@android:color/white" />

</merge>


