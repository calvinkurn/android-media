<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DatePickerLabelView">
        <attr name="date_picker_title" format="string"/>
        <attr name="date_picker_content" format="string"/>
        <attr name="date_picker_content_color" format="color"/>
    </declare-styleable>
    <declare-styleable name="LabelView">
        <attr name="title" format="string"/>
        <attr name="content" format="string"/>
        <attr name="content_color" format="color"/>
        <attr name="title_textStyle">
            <flag name="normal" value="0"/>
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="content_textStyle">
            <flag name="normal" value="0" />
            <flag name="bold" value="1" />
            <flag name="italic" value="2" />
        </attr>
        <attr name="content_small" format="boolean"/>
        <attr name="switch_enable" format="boolean"/>
        <attr name="label_show_arrow" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="loader_view">
        <attr name="width_weight" format="float" />
        <attr name="height_weight" format="float" />
        <attr name="use_gradient" format="boolean" />
        <attr name="gravity_">
            <flag name="bottom" value="80" />
            <flag name="center" value="17" />
            <flag name="center_horizontal" value="1" />
            <flag name="clip_horizontal" value="8" />
            <flag name="clip_vertical" value="128" />
            <flag name="end" value="8388613" />
            <flag name="fill" value="119" />
            <flag name="fill_horizontal" value="7" />
            <flag name="fill_vertical" value="112" />
            <flag name="left" value="3" />
            <flag name="right" value="5" />
            <flag name="start" value="8388611" />
            <flag name="top" value="48" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CircleProgress">
        <attr name="circle_progress" format="integer"/>
        <attr name="circle_max" format="integer"/>

        <attr name="circle_unfinished_color" format="color"/>
        <attr name="circle_finished_color" format="color"/>

        <attr name="circle_text_size" format="dimension"/>
        <attr name="circle_text_color" format="color"/>

        <attr name="circle_prefix_text" format="string"/>
        <attr name="circle_suffix_text" format="string"/>
    </declare-styleable>

    <declare-styleable name="DonutProgress">
        <attr name="donut_progress" format="float"/>
        <attr name="donut_max" format="integer"/>

        <attr name="donut_unfinished_color" format="color"/>
        <attr name="donut_finished_color" format="color"/>

        <attr name="donut_finished_stroke_width" format="dimension"/>
        <attr name="donut_unfinished_stroke_width" format="dimension"/>

        <attr name="donut_text_size" format="dimension"/>
        <attr name="donut_text_color" format="color"/>

        <attr name="donut_prefix_text" format="string"/>
        <attr name="donut_suffix_text" format="string"/>
        <attr name="donut_text" format="string"/>

        <attr name="donut_background_color" format="color"/>

        <attr name="donut_inner_bottom_text" format="string"/>
        <attr name="donut_inner_bottom_text_size" format="dimension"/>
        <attr name="donut_inner_bottom_text_color" format="color"/>

        <attr name="donut_circle_starting_degree" format="integer" />
        <attr name="donut_show_text" format="boolean"/>
        <attr name="donut_inner_drawable" format="reference"/>
    </declare-styleable>

    <declare-styleable name="ArcProgress">
        <attr name="arc_progress" format="integer"/>
        <attr name="arc_angle" format="float"/>
        <attr name="arc_stroke_width" format="dimension"/>
        <attr name="arc_max" format="integer"/>

        <attr name="arc_unfinished_color" format="color"/>
        <attr name="arc_finished_color" format="color"/>

        <attr name="arc_text_size" format="dimension"/>
        <attr name="arc_text_color" format="color"/>

        <attr name="arc_suffix_text" format="string"/>
        <attr name="arc_suffix_text_size" format="dimension"/>
        <attr name="arc_suffix_text_padding" format="dimension"/>

        <attr name="arc_bottom_text" format="string"/>
        <attr name="arc_bottom_text_size" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="Themes">
        <attr name="circleProgressStyle" format="reference"/>
        <attr name="donutProgressStyle" format="reference"/>
        <attr name="arcProgressStyle" format="reference"/>
    </declare-styleable>

    <!-- william chart attr -->
    <declare-styleable name="ChartAttrs">

        <attr name="chart_axisThickness" format="dimension"/>
        <attr name="chart_axisColor" format="reference|color"/>
        <attr name="chart_axisBorderSpacing" format="dimension"/>
        <attr name="chart_axisTopSpacing" format="dimension"/>

        <attr name="chart_labels" format="boolean"/>
        <attr name="chart_labelColor" format = "reference|color"/>
        <attr name="chart_fontSize" format = "dimension"/>
        <attr name="chart_typeface" format="string"/>

        <attr name="chart_shadowColor" format="reference|color"/>
        <attr name="chart_shadowDx" format = "dimension"/>
        <attr name="chart_shadowDy" format = "dimension"/>
        <attr name="chart_shadowRadius" format = "dimension"/>
    </declare-styleable>

    <!-- william chart attr -->
    <declare-styleable name="BarChartAttrs">
        <attr name="chart_barSpacing" format="dimension"/>
        <attr name="chart_setSpacing" format="dimension"/>
    </declare-styleable>

    <!-- Top ads widget radio expand view-->
    <declare-styleable name="TopAdsRadioExpand" >
        <attr name="title_radio" format="reference|string" />
        <attr name="checked" format="boolean"/>
        <attr name="radio_id" format="reference" />
	</declare-styleable>
    <declare-styleable name="ShopScoreLimitProgressBar">
        <attr name="default_limit" format="float" />
    </declare-styleable>
    <declare-styleable name="DecimalInputView">
        <attr name="decimal_input_hint" format="string"/>
        <attr name="decimal_input_text" format="string"/>
    </declare-styleable>
    <declare-styleable name="CounterInputView">
        <attr name="counter_hint" format="string"/>
        <attr name="counter_text" format="string"/>
        <attr name="counter_show_counter_button" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="SpinnerTextView">
        <attr name="spinner_hint" format="string"/>
        <attr name="spinner_selection" format="integer"/>
        <attr name="spinner_entries" format="reference"/>
        <attr name="spinner_values" format="reference"/>
    </declare-styleable>

    <declare-styleable name="SpinnerCounterInputView">
        <attr name="spinner_decimal_entries" format="reference"/>
        <attr name="spinner_decimal_values" format="reference"/>
        <attr name="spinner_decimal_selection" format="integer"/>
        <attr name="spinner_decimal_hint" format="string"/>
        <attr name="spinner_decimal_show_counter_button" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="ReputationView">
        <attr name="default_layout" format="integer" />
    </declare-styleable>
</resources>