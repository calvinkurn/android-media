            android:screenOrientation="portrait" />
        <activity
            android:name=".product.edit.view.activity.ProductScoringDetailActivity"
            android:label="@string/product_title_activity_product_score"
            android:screenOrientation="portrait" />
        <activity
            android:name=".product.category.view.activity.CategoryPickerActivity"
            android:label="@string/title_activity_category_picker"
            android:screenOrientation="portrait" />
        <activity
            android:name=".product.etalase.view.activity.EtalasePickerActivity"
            android:label="@string/product_title_activity_etalase_picker"
            android:screenOrientation="portrait" />
        <activity
            android:name=".product.edit.view.activity.CatalogPickerActivity"
            android:exported="true"
            android:label="@string/title_activity_catalog_picker"
            android:screenOrientation="portrait" />
        <activity
            android:name=".product.edit.view.activity.YoutubeAddVideoActivity"
            android:label="@string/product_label_video"
            android:screenOrientation="portrait" />
        <activity
            android:name=".product.variant.view.activity.ProductVariantDashboardActivity"
            android:label="@string/product_variant_label_title"
            android:screenOrientation="portrait" />
        <activity
            android:name=".product.variant.view.activity.ProductVariantDetailActivity"
            android:exported="true"
            android:label="@string/product_variant_label_title"
            android:screenOrientation="portrait" />
