<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.seller">

    <application
        android:allowBackup="true"
        android:label="@string/app_name">
        <activity android:name=".seller.info.view.activity.SellerCenterActivity"
            android:theme="@style/Theme.White"
            android:label="@string/title_activity_seller_center"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".purchase.detail.activity.OrderHistoryActivity"
            android:label="@string/transaction_title_activity_order_history"
            android:theme="@style/Theme.White">

            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:scheme="@string/internal_scheme"
                    android:host="@string/host_order"
                    android:path="@string/path_track"/>
            </intent-filter>
        </activity>
    </application>

</manifest>

