            android:name=".shopsettings.shipping.EditShippingActivity"
            android:label="@string/shop_shipping_setting"
            android:screenOrientation="portrait">
            <intent-filter tools:ignore="GoogleAppIndexingWarning">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="ANDROID.INTENT.CATEGORY.BROWSABLE" />

                <data
                    android:host="com.tokopedia.tkpd"
                    android:pathPrefix="/myshop-editor.pl"
                    android:scheme="https" />
            </intent-filter>
        </activity>

        <activity
            android:name=".shopsettings.shipping.OpenShopEditShipping"
            android:label="@string/title_open_shop" />

        <activity
            android:name=".shopsettings.notes.activity.ManageShopNotesActivity"
            android:label="@string/title_activity_manage_shop_notes"
            android:screenOrientation="portrait" />

        <activity
            android:name=".product.picker.view.ProductListPickerActivity"
            android:label="@string/title_activity_product_list_picker"
            android:windowSoftInputMode="stateHidden"
            android:screenOrientation="portrait" />

        <service
            android:name=".selling.SellingService"
            android:exported="false" />
        <service
            android:name=".shop.ShopEditService"
            android:exported="false" />

        <service
            android:name=".product.edit.view.service.UploadProductService"
            android:exported="false" />
