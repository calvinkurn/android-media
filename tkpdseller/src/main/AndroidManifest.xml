<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.tokopedia.seller">

    <application
        android:allowBackup="true"
        android:label="@string/app_name">
        <activity
            android:name=".fintech.mitratoppers.view.activity.MitraToppersActivity"
            android:label="@string/drawer_title_mitra_toppers"
            android:screenOrientation="portrait" />
        <activity
            android:name=".selling.orderReject.ConfirmRejectOrderActivity"
            android:label="@string/title_reject_confirm"
            android:screenOrientation="portrait" />
        <activity
            android:name=".selling.view.activity.SellingDetailActivity"
            android:label="@string/title_activity_people_tx_details"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Tokopedia3" />
        <activity
            android:name=".selling.view.activity.ActivitySellingTransaction"
            android:label="@string/title_activity_shop_transaction_new"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity
            android:name=".OrderHistoryView"
            android:label="@string/title_activity_order_history_view"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ShippingConfirmationDetail"
            android:label="@string/title_activity_shipping_confirmation_detail"
            android:screenOrientation="portrait" />
        <activity
            android:name=".instoped.InstopedActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name=".common.datepicker.view.activity.DatePickerActivity"
            android:label="@string/title_date_setting"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Tokopedia3" />

        <activity
            android:name=".shop.ShopEditorActivity"
            android:label="@string/shop_setting"
            android:theme="@style/ShopAppTheme" />

        <activity
            android:name=".myproduct.ManageProductSeller"
            android:label="@string/title_activity_manage_product"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />

        <activity
            android:name=".product.edit.view.activity.ProductAddActivity"
            android:label="@string/title_activity_add_product"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="tkpdevel.com"
                    android:scheme="http" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="image/*" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.SEND_MULTIPLE" />

                <category android:name="android.intent.category.DEFAULT" />

                <data android:mimeType="image/*" />
            </intent-filter>
        </activity>
        <activity
            android:name=".product.edit.view.activity.ProductEditActivity"
            android:label="@string/title_activity_edit_product"
            android:screenOrientation="portrait" />
        <activity
            android:name=".product.variant.view.activity.ProductVariantPickerActivity"
            android:label="@string/product_variant_label_title"
            android:screenOrientation="portrait" />
        <activity
            android:name=".product.draft.view.activity.ProductDraftListActivity"
            android:exported="true"
            android:label="@string/title_activity_draft_product"
            android:screenOrientation="portrait" />

        <activity
            android:name=".opportunity.activity.OpportunitySortActivity"
            android:label="@string/title_sort_but"
            android:screenOrientation="portrait" />

        <activity
            android:name=".opportunity.activity.OpportunityFilterActivity"
            android:label="@string/title_filter_but"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustPan" />

        <activity
            android:name=".opportunity.activity.OpportunityDetailActivity"
            android:label="@string/title_opportunity_detail"
            android:screenOrientation="portrait" />

        <activity
            android:name=".product.edit.view.activity.ProductDraftAddActivity"
            android:label="@string/title_activity_add_product"
            android:screenOrientation="portrait" />
        <activity
            android:name=".product.edit.view.activity.ProductDraftEditActivity"
            android:label="@string/title_activity_edit_product"
