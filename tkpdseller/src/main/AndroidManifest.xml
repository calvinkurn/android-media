        <activity
            android:name=".reputation.view.activity.SellerReputationActivity"
            android:label="@string/title_activity_reputation"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".reputation.view.activity.SellerReputationInfoActivity"
            android:label="@string/reputation_point_calculation"
            android:theme="@style/AppTheme" />

        <activity
            android:name=".shop.open.view.activity.ShopOpenDomainActivity"
            android:label="@string/title_open_shop"
            android:exported="true"
            android:theme="@style/ShopAppTheme" />
            
		<activity
            android:name=".shop.open.view.activity.ShopOpenMandatoryActivity"
            android:label="@string/title_shop_setting"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Tokopedia3" />
            
        <activity
            android:name=".shopsettings.ManageShopActivity"
            android:label="@string/title_activity_manage_shop"
            android:screenOrientation="portrait" />
        <activity
            android:name=".shopsettings.payment.PaymentEditor"
            android:label="@string/title_activity_payment_editor"
            android:screenOrientation="portrait" />
