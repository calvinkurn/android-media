query GETORDERINFO($paymentID: Int!) {
 get_order_info(paymentId:$paymentID){
    data{
    seller_id
    order_id
    seller_id
    payment_id
    is_new_buyer
    is_free_shipping
    is_fulfillment
    order_status
    item_price
    promo_info {
      promo_id
      promo_code
      promo_amount
    }
    shop_info {
      shop_owner_id
      shop_owner_email
      shop_owner_phone
      shop_name
      shop_domain
      shop_id
      last_login_at
    }
    order_info {
      order_detail {
        order_detail_id
        product_id
        product_name
        product_desc_pdp
        product_desc_atc
        product_price
        subtotal_price
        weight
        total_weight
        quantity
        quantity_deliver
        quantity_reject
        is_free_returns
        insurance_price
        normal_price
        currency_id
        currency_rate
        min_order
        child_cat_id
        category_name
        campaign_id
        product_picture
        snapshot_url
      }
      order_age_day
      shipping_age_day
      delivered_age_day
      partial_process
      shipping_info {
        sp_id
        shipping_id
        logistic_name
        logistic_service
        shipping_price
        shipping_price_rate
        shipping_fee
        insurance_price
        fee
        is_change_courier
        second_sp_id
        second_shipping_id
        second_logistic_name
        second_logistic_service
        second_agency_fee
        second_insurance
        second_rate
        second_shipping_time
        awb
        autoresi_cashless_status
        autoresi_awb
        autoresi_shipping_price
        count_awb
        isCashless
        is_fake_delivery
      }
    }
    create_time
    shipping_date
    update_time
    payment_date
    delivered_date
    est_shipping_date
    est_delivered_date
    }
 }
}