query GetInsureTechCart($data: TransactionalUserInfo) {
  cart_list_transactional(userInfo: $data) {
    	shops{
        shop_id
        items{
            product_id
            digital_product{
            digital_product_id
            cart_item_id
            type_id
            price_per_product
            total_price
            opt_in
            is_product_level
            is_seller_money
            is_application_needed
            is_new
            product_info{
              title
              sub_title
              link_detail_info_title
              description
              icon_url
              web_link_html
              ticker_text
            }
            application_details{
              id
              label
              place_holder
              type
              required
              value
              values {
                id
                value
              }
              validations {
                id
                type
                value
                error_message
              }
            }
          }
        }
      }
  }
}