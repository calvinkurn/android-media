query get_preference_list {
    get_all_profiles_occ {
            error_message
            status
            data{
                messages
                success
                max_profile
                profiles{
                    profile_id
                    status
                    address {
                            address_id
                            receiver_name
                            address_name
                            address_street
                            district_id
                            district_name
                            city_id
                            city_name
                            province_id
                            province_name
                            phone
                            longitude
                            latitude
                            geolocation
                            postal_code
                    }
                    payment {
                            enable
                            active
                            gateway_code
                            gateway_name
                            image
                            description
                            url
                            metadata
                    }
                    shipment {
                            service_id
                            service_duration
                            service_name
                    }
                }
            }
        }
}