<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="20dp"
    android:paddingBottom="20dp">

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader00"
        android:layout_width="243dp"
        android:layout_height="24dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader99"
        android:layout_width="126dp"
        android:layout_height="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="21dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loader00"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader0"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loader99"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader1"
        android:layout_width="227dp"
        android:layout_height="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toEndOf="@id/loader0"
        app:layout_constraintTop_toBottomOf="@id/loader99"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader2"
        android:layout_width="55dp"
        android:layout_height="16dp"
        android:layout_marginTop="6dp"
        app:layout_constraintStart_toStartOf="@id/loader1"
        app:layout_constraintTop_toBottomOf="@id/loader1"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader4"
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:layout_below="@+id/loader2"
        android:layout_marginTop="15dp"
        android:gravity="center_vertical"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loader0"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/view2"
        android:layout_width="209dp"
        android:layout_height="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="@color/Unify_N50"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/loader4"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader6"
        android:layout_width="46dp"
        android:layout_height="46dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="26dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader7"
        android:layout_width="114dp"
        android:layout_height="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="26dp"
        app:layout_constraintStart_toEndOf="@id/loader6"
        app:layout_constraintTop_toBottomOf="@+id/view2"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader8"
        android:layout_width="100dp"
        android:layout_height="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="@id/loader7"
        app:layout_constraintTop_toBottomOf="@id/loader7"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader9"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginEnd="23dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/loader7"
        app:loader_corner_radius="4dp"
        app:loader_type="rect" />

</androidx.constraintlayout.widget.ConstraintLayout>