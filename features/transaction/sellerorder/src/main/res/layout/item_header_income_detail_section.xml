<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_header_income_detail_label"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textColor="@color/Unify_NN950"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tv_header_income_detail_value"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="display_3"
        app:typographyWeight="bold"
        tools:text="Total Harga (4 Barang)" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_header_income_detail_value"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textColor="@color/Unify_NN950"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="display_3"
        app:typographyWeight="bold"
        tools:text="Rp10.326.000" />

</androidx.constraintlayout.widget.ConstraintLayout>