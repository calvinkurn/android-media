<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvTitleFilterDate"
        tools:text="Tanggal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:textColor="@color/Unify_N700"
        app:typographyType="heading_4"
        app:typographyWeight="bold"/>

    <com.tokopedia.sellerorder.filter.presentation.widget.FilterSelectDate
        android:id="@+id/selectDateFilter"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl3"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="@dimen/spacing_lvl3"
        android:layout_below="@+id/tvTitleFilterDate"/>

</RelativeLayout>