    status
    message_error
    data{
      pickup_location{
        title
        address
        phone
      }
      detail{
        title
        shippers{
          name
          service
          note
          courier_image
          count_text
          count
        }
        orchestra_partner
      }
      notes{
        title
        list
      }
      schedule_time_day {
          today {
            key
            start
            end
          }
          tomorrow {
            key
            start
            end
          }
        }
    }

  }
}