query OrderFilterSom {
  orderFilterSom{
    status_list{
        order_status_id
      key
      order_status
      order_status_amount
      is_checked
      child_status {
        id
        key
        text
        amount
        is_checked
      }
    }
    waiting_payment_counter{
      text
      counter
    }
    shipping_list{
      shipping_id
      shipping_code
      shipping_name
      shipping_desc
      status
      img_logo
    }
  }
}