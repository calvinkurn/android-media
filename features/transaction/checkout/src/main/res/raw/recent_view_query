query recent_view($userID: Int){
  get_recent_view(userID: $userID){
    items{
      product_id,
      product_name,
      product_price,
      shop_url,
      product_rating,
      product_review_count,
      product_image,
      shop_id,
      shop_location,
      badges {
        title
        image_url
      }
      wishlist
    }
  }
}