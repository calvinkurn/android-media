<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:padding="@dimen/dp_16"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/text_view_branch_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp_4"
        android:textStyle="bold"
        android:textColor="@color/black_39"
        tools:text="Kampus Anggrek"/>

    <TextView
        android:id="@+id/text_view_branch_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_12"
        android:textColor="@color/black_54"
        android:layout_below="@+id/text_view_branch_name"
        tools:text="Alam Sutera, Serpong"/>


</RelativeLayout>