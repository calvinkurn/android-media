<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.unifycomponents.ImageUnify
        android:id="@+id/checkout_iv_identifier"
        android:layout_width="15dp"
        android:layout_height="15dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@id/checkout_label_identifier"
        app:layout_constraintBottom_toBottomOf="@id/checkout_label_identifier"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/checkout_label_identifier"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="3dp"
        app:layout_constraintStart_toEndOf="@id/checkout_iv_identifier"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:textColor="@color/Unify_N700_68"
        app:typographyType="body_3"
        tools:text="text"/>

</androidx.constraintlayout.widget.ConstraintLayout>