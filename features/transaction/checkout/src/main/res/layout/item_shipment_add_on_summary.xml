<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="4dp">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_shipment_summary_add_on_wording"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/Unify_N700_96"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="body_3"
        tools:text="Total Jasa Pasang" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_shipment_summary_add_on_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/Unify_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_shipment_summary_add_on_wording"
        app:typographyType="body_3"
        app:typographyWeight="reguler"
        tools:text="Rp10.000.000" />

</androidx.constraintlayout.widget.ConstraintLayout>
