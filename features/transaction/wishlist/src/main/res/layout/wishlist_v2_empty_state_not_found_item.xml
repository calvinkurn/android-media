<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="16dp"
    android:layout_marginEnd="16dp">

        <com.tokopedia.empty_state.EmptyStateUnify
            android:id="@+id/empty_state"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:unifyEmptyStateCTAFullWidth="true"
            app:unifyEmptyStateImageUrl="@string/empty_state_img_not_found"
            app:unifyEmptyStateOrientation="horizontal"
            app:unifyEmptyStatePrimaryCTAText="@string/empty_state_not_found_button"
            app:unifyEmptyStateTitle="@string/empty_state_not_found_title"
            app:unifyEmptyStateType="section"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>