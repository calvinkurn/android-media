<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/Unify_N0"
    android:paddingTop="@dimen/dp_16"
    android:paddingBottom="@dimen/dp_16">

    <RelativeLayout
        android:id="@+id/rl_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginRight="@dimen/dp_16"
        android:background="@drawable/bg_outline_item_grey"
        android:padding="@dimen/dp_16">

        <com.tokopedia.unifyprinciples.Typography
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_toStartOf="@+id/tv_seller_cashback_price"
            android:text="@string/label_seller_cashback"
            android:textColor="@color/Unify_N700_68"
            app:typographyType="body_3"
            app:typographyWeight="reguler" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_seller_cashback_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:gravity="end"
            android:textColor="@color/Unify_N700_68"
            app:typographyType="body_3"
            app:typographyWeight="reguler"
            tools:ignore="RelativeOverlap"
            tools:text="Rp1.400.000" />

    </RelativeLayout>

</FrameLayout>