mutation add_to_cart_occ($param: OneClickCheckoutATCParam) {
    add_to_cart_occ(param: $param) {
        error_message
        status
        data {
            message
            success
            detail {
                cart_id
                customer_id
                is_scp
                is_trade_in
                notes
                product_id
                quantity
                shop_id
                warehouse_id
            }
        }
    }
}