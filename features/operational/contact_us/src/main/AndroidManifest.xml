<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.contact_us">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity
            android:name=".createticket.activity.ContactUsActivity"
            android:hardwareAccelerated="true"
            android:label="@string/title_activity_contact_us"
            android:screenOrientation="portrait" />
        <activity
            android:name=".inboxticket.activity.InboxTicketActivity"
            android:hardwareAccelerated="true"
            android:label="@string/title_activity_inbox_ticket"
            android:screenOrientation="portrait" />
        <activity
            android:name=".inboxticket.activity.InboxTicketDetailActivity"
            android:hardwareAccelerated="true"
            android:label="@string/title_activity_inbox_ticket_detail"
            android:screenOrientation="portrait" />
        <activity
            android:name=".createticket.activity.ContactUsCreateTicketActivity"
            android:screenOrientation="portrait" />
    </application>
</manifest>