<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.contactus">

    <application>
        <activity
            android:name="com.tokopedia.contactus.home.view.ContactUsHomeActivity"
            android:exported="true"
            android:label="@string/contact_us_title_home"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" >

            <intent-filter
                android:label="deeplink second activity">
                <action android:name="com.tokopedia.internal.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:scheme="@string/tokopedia_scheme"
                    android:host="@string/host_contactus"/>
            </intent-filter>
        </activity>

        <activity
            android:name="com.tokopedia.contactus.createticket.activity.ContactUsActivity"
            android:hardwareAccelerated="true"
            android:label="@string/contact_us_title_home"
            android:theme="@style/Theme.White"
            android:screenOrientation="portrait" >

            <intent-filter>
                <action android:name="com.tokopedia.internal.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:scheme="@string/tokopedia_scheme"
                    android:host="@string/host_contact_us"/>
            </intent-filter>
        </activity>
        <activity
            android:name="com.tokopedia.contactus.createticket.activity.ContactUsCreateTicketActivity"
            android:theme="@style/Theme.White"
            android:screenOrientation="portrait" />
        <activity
            android:name=".inboxticket2.view.activity.InboxListActivity"
            android:exported="true"
            android:label="@string/contact_us_inbox"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" >

            <intent-filter>
                <action android:name="com.tokopedia.internal.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:scheme="@string/internal_scheme"
                    android:host="@string/host_customercare"
                    android:path="@string/path_inbox_list"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".inboxticket2.view.activity.InboxDetailActivity"
            android:exported="true"
            android:label="@string/detail_kendala"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" >
            <intent-filter>
                <action android:name="com.tokopedia.internal.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:scheme="@string/tokopedia_scheme"
                    android:host="@string/host_customercare"
                    android:pathPattern="@string/path_ticket_detail"/>
            </intent-filter>
        </activity>
        <activity android:name="com.tokopedia.contactus.inboxticket2.view.activity.ContactUsProvideRatingActivity"
            android:exported="true"
            android:label="@string/survey_layanan"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" />

       <activity
        android:name=".inboxticket2.view.activity.ClearCacheActivity"
        android:exported="true"
        android:label="@string/contact_us_title_clear_cache"
        android:screenOrientation="portrait"
        android:theme="@style/Theme.White" >
        <intent-filter>
            <action android:name="com.tokopedia.internal.VIEW" />

            <category android:name="android.intent.category.DEFAULT" />
            <category android:name="android.intent.category.BROWSABLE" />
            <data
                android:scheme="@string/tokopedia_scheme"
                android:host="@string/host_contactus"
                android:pathPattern="@string/path_clear_cache"/>
        </intent-filter>
    </activity>

    </application>
</manifest>