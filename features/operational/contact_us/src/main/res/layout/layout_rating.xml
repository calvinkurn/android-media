<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    style="@style/Theme.White.Background">

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="4dp"
        android:layout_marginBottom="16dp"
        android:background="@color/contact_us_dms_rating_divider"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/txt_help_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="8dp"
        android:padding="16dp"
        android:text="@string/text_satisfied"
        android:textColor="@color/Unify_N700_96"
        app:typographyType="display_1"
        app:typographyWeight="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/btn_inactive_1"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_1"
        android:textSize="14sp"
        android:visibility="visible"
        android:contentDescription="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

    <ImageView
        android:id="@+id/btn_inactive_2"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_2"
        android:textSize="14sp"
        android:visibility="visible"
        android:contentDescription="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_1"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_3"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

    <ImageView
        android:id="@+id/btn_inactive_3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_3"
        android:textSize="14sp"
        android:visibility="visible"
        android:contentDescription="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_2"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_4"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

    <ImageView
        android:id="@+id/btn_inactive_4"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_4"
        android:textSize="14sp"
        android:visibility="visible"
        android:contentDescription="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_3"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_5"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

    <ImageView
        android:id="@+id/btn_inactive_5"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_5"
        android:textSize="14sp"
        android:visibility="visible"
        android:contentDescription="@null"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

</androidx.constraintlayout.widget.ConstraintLayout>