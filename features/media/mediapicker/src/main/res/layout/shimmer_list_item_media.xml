<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container"
    android:visibility="gone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    tools:visibility="visible">

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer_media_1"
        app:layout_constraintTop_toBottomOf="@id/guideline"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/shimmer_media_2"
        android:layout_marginTop="0dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer_media_2"
        app:layout_constraintTop_toBottomOf="@id/guideline"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintRight_toRightOf="@+id/shimmer_media_3"
        app:layout_constraintLeft_toRightOf="@+id/shimmer_media_1"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer_media_3"
        app:layout_constraintTop_toBottomOf="@id/guideline"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/shimmer_media_2"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer_media_4"
        app:layout_constraintTop_toBottomOf="@id/shimmer_media_1"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/shimmer_media_5"
        android:layout_marginTop="0dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer_media_5"
        app:layout_constraintTop_toBottomOf="@id/shimmer_media_2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintRight_toRightOf="@+id/shimmer_media_6"
        app:layout_constraintLeft_toRightOf="@+id/shimmer_media_4"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer_media_6"
        app:layout_constraintTop_toBottomOf="@id/shimmer_media_3"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/shimmer_media_5"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer_media_7"
        app:layout_constraintTop_toBottomOf="@id/shimmer_media_4"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/shimmer_media_8"
        android:layout_marginTop="0dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer_media_8"
        app:layout_constraintTop_toBottomOf="@id/shimmer_media_5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintRight_toRightOf="@+id/shimmer_media_9"
        app:layout_constraintLeft_toRightOf="@+id/shimmer_media_7"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer_media_9"
        app:layout_constraintTop_toBottomOf="@id/shimmer_media_6"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_margin="4dp"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/shimmer_media_8"
        app:loader_type="rect"/>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintGuide_begin="0dp"
        android:orientation="horizontal"/>

</androidx.constraintlayout.widget.ConstraintLayout>
