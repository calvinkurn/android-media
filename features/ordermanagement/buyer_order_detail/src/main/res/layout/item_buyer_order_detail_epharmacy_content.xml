<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="8dp">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_buyer_order_detail_ephar_content_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="2dp"
        android:ellipsize="end"
        android:maxLines="1"
        app:layout_constraintEnd_toStartOf="@+id/gl_buyer_order_detail_ephar_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="body_3"
        tools:text="Konsultasi" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_buyer_order_detail_ephar_content_desc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="@color/Unify_NN950"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tv_buyer_order_detail_ephar_content_title"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="body_3"
        tools:text="Dkonsul" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_buyer_order_detail_ephar_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.45" />

</androidx.constraintlayout.widget.ConstraintLayout>