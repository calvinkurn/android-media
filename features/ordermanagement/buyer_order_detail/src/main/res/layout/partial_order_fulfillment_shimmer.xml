<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader1"
        android:layout_width="102dp"
        android:layout_height="12dp"
        android:layout_marginTop="8dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader2"
        android:layout_width="102dp"
        android:layout_height="12dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/loader1"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.DividerUnify
        android:id="@+id/divider1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/loader1"
        />

    <com.tokopedia.unifycomponents.ContainerUnify
        android:id="@+id/containerLoader"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/loader1"
        app:layout_constraintBottom_toTopOf="@id/divider2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader3"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        app:layout_constraintStart_toStartOf="@id/loader1"
        app:layout_constraintTop_toBottomOf="@id/loader1"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader4"
        android:layout_width="171dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        app:layout_constraintStart_toEndOf="@id/loader3"
        app:layout_constraintTop_toTopOf="@id/loader3"
        app:layout_constraintBottom_toTopOf="@id/loader5"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader5"
        android:layout_width="93dp"
        android:layout_height="12dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toStartOf="@id/loader4"
        app:layout_constraintTop_toBottomOf="@id/loader4"
        app:layout_constraintBottom_toBottomOf="@id/loader3"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader6"
        android:layout_width="51dp"
        android:layout_height="12dp"
        app:layout_constraintEnd_toEndOf="@id/loader2"
        app:layout_constraintTop_toTopOf="@id/loader4"
        app:layout_constraintBottom_toBottomOf="@id/loader5"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.DividerUnify
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/loader3" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader7"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="@id/loader1"
        app:layout_constraintTop_toBottomOf="@id/divider2"
        app:loader_type="circle" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader8"
        android:layout_width="182dp"
        android:layout_height="12dp"
        android:layout_marginStart="6dp"
        app:layout_constraintStart_toEndOf="@id/loader7"
        app:layout_constraintTop_toTopOf="@id/loader7"
        app:layout_constraintBottom_toBottomOf="@id/loader7"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader9"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:layout_constraintEnd_toEndOf="@id/loader2"
        app:layout_constraintTop_toTopOf="@id/loader7"
        app:layout_constraintBottom_toBottomOf="@id/loader7"
        app:loader_type="circle" />

    <com.tokopedia.unifycomponents.DividerUnify
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/loader7" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader10"
        android:layout_width="171dp"
        android:layout_height="12dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="@id/loader1"
        app:layout_constraintTop_toBottomOf="@id/divider3"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader11"
        android:layout_width="63dp"
        android:layout_height="12dp"
        app:layout_constraintEnd_toEndOf="@id/loader2"
        app:layout_constraintTop_toTopOf="@id/loader10"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader12"
        android:layout_width="171dp"
        android:layout_height="12dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="@id/loader1"
        app:layout_constraintTop_toBottomOf="@id/loader10"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader13"
        android:layout_width="63dp"
        android:layout_height="12dp"
        app:layout_constraintEnd_toEndOf="@id/loader2"
        app:layout_constraintTop_toTopOf="@id/loader12"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.DividerUnify
        android:id="@+id/divider4"
        android:layout_width="0dp"
        android:layout_height="1dp"
        android:layout_marginTop="14dp"
        app:layout_constraintStart_toStartOf="@id/loader1"
        app:layout_constraintEnd_toEndOf="@id/loader2"
        app:layout_constraintTop_toBottomOf="@id/loader12" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader14"
        android:layout_width="171dp"
        android:layout_height="12dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="@id/loader1"
        app:layout_constraintTop_toBottomOf="@id/divider4"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader15"
        android:layout_width="63dp"
        android:layout_height="12dp"
        app:layout_constraintEnd_toEndOf="@id/loader2"
        app:layout_constraintTop_toTopOf="@id/loader14"
        app:loader_type="rect"/>

    <View
        android:id="@+id/loaderShadowBtn"
        android:layout_width="0dp"
        android:layout_height="2dp"
        android:layout_marginTop="18dp"
        android:background="@drawable/buyer_order_detail_sticky_action_buttons_shadow"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/loader14"
        app:layout_constraintBottom_toTopOf="@id/loaderBtn15" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loaderBtn15"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintStart_toStartOf="@id/loader1"
        app:layout_constraintEnd_toEndOf="@id/guidelineBtn"
        app:layout_constraintTop_toBottomOf="@id/loaderShadowBtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:loader_type="rect"/>


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loaderBtn16"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintEnd_toEndOf="@id/loader2"
        app:layout_constraintStart_toEndOf="@id/guidelineBtn"
        app:layout_constraintTop_toTopOf="@id/loaderBtn15"
        app:layout_constraintBottom_toBottomOf="@id/loaderBtn15"
        app:loader_type="rect"/>

</androidx.constraintlayout.widget.ConstraintLayout>