<?xml version="1.0" encoding="utf-8"?>
<com.tokopedia.unifycomponents.CardUnify android:id="@+id/other_address_card"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="170dp"
    android:layout_height="170dp"
    android:orientation="vertical"
    android:layout_marginStart="10dp"
    android:layout_marginEnd="16dp"
    app:cardCornerRadius="4dp"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

            <com.tokopedia.iconunify.IconUnify
                android:id="@+id/btn_other_address"
                android:layout_width="32dp"
                android:layout_height="32dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toTopOf="@id/txt_other_address"
                app:layout_constraintVertical_chainStyle="packed"
                app:icon_color_light_enable="@color/Unify_GN500"
                app:icon_color_night_enable="@color/Unify_GN500"
                app:icon_name="CHEVRON_RIGHT"
                android:background="@drawable/bg_circle_more"
                tools:ignore="ContentDescription"
                android:elevation="@dimen/unify_space_4"/>

            <com.tokopedia.unifyprinciples.Typography
                android:id="@+id/txt_other_address"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:paddingStart="29dp"
                android:paddingEnd="29dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toBottomOf="@id/btn_other_address"
                app:layout_constraintBottom_toBottomOf="parent"
                app:typographyType="display_3"
                app:typographyWeight="bold"
                android:text="@string/cek_alamat_lainnya"
                android:textColor="@color/Unify_GN500"
                android:gravity="center"/>

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.tokopedia.unifycomponents.CardUnify>