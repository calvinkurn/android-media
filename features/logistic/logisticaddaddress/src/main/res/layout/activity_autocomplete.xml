<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".features.autocomplete.AutoCompleteActivity"
    android:orientation="vertical">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:theme="?toolbarTheme"
        app:subtitleTextAppearance="?subtitleTextAppearance"
        app:titleTextAppearance="?titleTextAppearance"
        tools:layout_height="?actionBarSize" />

    <fragment
        android:id="@+id/fragment_autocomplete"
        android:name="com.tokopedia.logisticaddaddress.features.autocomplete.AutoCompleteFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout="@layout/fragment_autocomplete" />

</LinearLayout>