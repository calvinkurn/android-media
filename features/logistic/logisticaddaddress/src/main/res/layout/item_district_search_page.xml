<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/rl_autocomplete_item"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/ic_location"
        android:layout_width="@dimen/unify_space_16"
        android:layout_height="@dimen/dp_20"
        android:layout_marginEnd="@dimen/dp_18"
        android:layout_marginRight="@dimen/dp_18"
        app:icon_name="LOCATION"
        android:layout_alignParentTop="true"
        android:layout_marginTop="@dimen/dp_8"/>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginTop="@dimen/dp_8"
        android:layout_marginBottom="@dimen/dp_10"
        android:layout_marginRight="@dimen/dp_18"
        android:layout_marginEnd="@dimen/dp_18"
        android:layout_toRightOf="@id/ic_location"
        android:layout_toEndOf="@id/ic_location"
        android:orientation="vertical">

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/search_place_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_14"
            android:textStyle="bold"
            android:layout_marginBottom="@dimen/dp_2"
            android:text="Tokopedia Towet"/>

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/search_place_address"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="@dimen/sp_11"
            android:text="adadadad ada"/>

    </LinearLayout>

    <View
        android:id="@+id/divider_nearest_loc"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/Unify_N100"/>

</RelativeLayout>