<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.editshipping">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <activity android:name="com.tokopedia.editshipping.ui.EditShippingActivity"
            android:label="@string/shop_shipping_setting"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:scheme="@string/internal_scheme"
                    android:host="@string/host_marketplace"
                    android:path="@string/path_shop_setting_shipping"/>
            </intent-filter>
        </activity>
    </application>

</manifest>