query shippingEditorMobilePopup($inputShippingEditorMobilrPopup : KeroShippingEditorMobilePopupInput!) {
  kero_shipping_editor_mobile_popup(input: $inputShippingEditorMobilePopup ) {
    status
    config
    server_process_time
    message_status
    data {
      show_popup
      ticker_title
      ticker_content
      popup_content
    }
  }
}