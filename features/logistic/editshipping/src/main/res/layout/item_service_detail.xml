<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_service_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:typographyType="heading_4"
        app:typographyWeight="bold"
        android:text="SiCepat"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_service_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/unify_space_4"
        android:layout_marginBottom="@dimen/unify_space_24"
        app:typographyType="body_3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_service_name"
        app:layout_constraintBottom_toBottomOf="parent"
        android:text="Bawa pesanan ke gerai terdekat dan minta resi dari petugas" />


</androidx.constraintlayout.widget.ConstraintLayout>