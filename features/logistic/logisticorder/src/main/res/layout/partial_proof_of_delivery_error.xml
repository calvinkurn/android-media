<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"

    android:id="@+id/view_image_not_valid"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/dms_proof_background_image_static_black"
    android:visibility="gone"
    tools:visibility="visible">

    <FrameLayout
        android:id="@+id/btn_dismiss"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:padding="13.2dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.tokopedia.iconunify.IconUnify
            android:layout_width="17.5dp"
            android:layout_height="17.5dp"
            android:layout_gravity="center"
            app:icon_color_light_enable="@color/Unify_Static_White"
            app:icon_color_night_enable="@color/Unify_Static_White"
            app:icon_name="CLOSE" />
    </FrameLayout>


    <com.tokopedia.unifyprinciples.Typography
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingHorizontal="16dp"
        android:text="@string/pod_not_valid_proof"
        android:textAlignment="center"
        android:textColor="@color/Unify_Static_White"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="paragraph_3" />


</androidx.constraintlayout.widget.ConstraintLayout>