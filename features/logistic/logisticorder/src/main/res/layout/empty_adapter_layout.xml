<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginTop="@dimen/dp_8"
    android:layout_marginBottom="@dimen/dp_8"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <ImageView
        android:id="@+id/circle_green_logistic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="@id/empty_tracking_notes"
        android:layout_width="@dimen/dp_8"
        android:layout_height="@dimen/dp_8"
        android:layout_marginTop="@dimen/dp_8"
        android:src="@drawable/circle_green_logistic" />


    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/empty_tracking_notes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginStart="@dimen/dp_16"
        app:layout_constraintLeft_toRightOf="@id/circle_green_logistic"
        app:layout_constraintStart_toEndOf="@id/circle_green_logistic"
        app:layout_constraintTop_toTopOf="@id/circle_green_logistic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:typographyType="body_2"
        tools:text="Penjual keliru menginput nomor resi atau tanggal pengiriman." />

</androidx.constraintlayout.widget.ConstraintLayout>