<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.logisticcart">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <activity
            android:name="com.tokopedia.logisticcart.logisticinsurance.view.InsuranceTnCActivity"
            android:label="@string/title_term_and_condition"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" />

        <activity
            android:name="com.tokopedia.logisticcart.cod.view.CodActivity"
            android:label="@string/cod_brand_tag"
            android:theme="@style/Theme.White">
            <intent-filter android:autoVerify="true">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="@string/host_marketplace"
                    android:path="@string/path_cod"
                    android:scheme="@string/internal_scheme" />
            </intent-filter>
        </activity>
    </application>

</manifest>