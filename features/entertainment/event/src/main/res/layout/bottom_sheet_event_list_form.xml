<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.tokopedia.unifycomponents.SearchBarUnify
        android:id="@+id/event_search_list_form"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:search_bar_show_icon="false"
        />

    <com.tokopedia.unifycomponents.list.ListUnify
        android:id="@+id/list_form"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/event_search_list_form"
        android:choiceMode="singleChoice"
        />


</androidx.constraintlayout.widget.ConstraintLayout>