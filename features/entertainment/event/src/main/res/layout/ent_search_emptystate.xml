<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginTop="@dimen/dimen_dp_20">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ent_search_empty_state_heading"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="heading_4"
        app:typographyWeight="bold" />

    <com.tokopedia.unifyprinciples.Typography
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ent_search_empty_state_body"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/typography2"
        android:layout_marginTop="@dimen/spacing_lvl2"
        app:typographyType="body_3"
        app:typographyWeight="reguler" />


</androidx.constraintlayout.widget.ConstraintLayout>