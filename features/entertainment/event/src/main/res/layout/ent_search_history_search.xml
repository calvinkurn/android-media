<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/spacing_lvl4">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/txtTerakhirDicari"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/ent_search_search_last_item"
        app:typographyWeight="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="small"/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recycler_view"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtTerakhirDicari"
        android:layout_marginTop="@dimen/spacing_lvl4"
        app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
        tools:listitem="@layout/ent_search_event_list_item"/>



</androidx.constraintlayout.widget.ConstraintLayout>