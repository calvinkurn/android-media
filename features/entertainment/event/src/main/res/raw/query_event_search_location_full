query searchEventLocation{
  event_location_search(searchParams:[
    {
      key: "category_id",
      value: "1"
    },
    {
      key:"name",
      value:""},
    {
      key: "sort_by",
      value: "priority"
    },
    {
      key: "order",
      value: "desc"
    },
    {
      key: "size",
      value: "20"
    }
])
  {
    locations {
      id
      name
      search_name
      city_id
      city_name
      icon_app
      image_app
    }
    count
  }
}