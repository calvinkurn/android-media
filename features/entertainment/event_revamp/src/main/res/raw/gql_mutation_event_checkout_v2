mutation checkout_general_v2($params:CheckoutGeneralV2Params!){
checkout_general_v2(params:$params) {
      header {
        process_time
        reason
        error_code
      }
      data {
        success
        error
        error_state
        message
      	data{
          callback_url
          parameter{
            amount
          }
          price_validation{
            is_updated
            message{
              action
              desc
              title
            }
          }
          product_list{
            id
            name
            price
            quantity
          }
          query_string
          redirect_url
        }
      }
      status
      error_reporter {
        eligible
      }
  }
}