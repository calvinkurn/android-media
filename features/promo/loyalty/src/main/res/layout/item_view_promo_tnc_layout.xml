<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    android:background="@color/Unify_NN0"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/tnc_header_height"
        android:background="@drawable/bg_white_outline_grey"
        android:gravity="center"
        android:text="@string/syarat_amp_ketentuan"
        android:textColor="@color/Unify_N700_96"
        android:textSize="18sp" />

    <com.tokopedia.abstraction.base.view.webview.TkpdWebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/Unify_NN0" />

</LinearLayout>