<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
        android:layout_marginTop="20dp">

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_status"
        android:layout_width="4dp"
        android:layout_height="12dp"
        android:layout_marginTop="8dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/tv_coupon_state"
        android:layout_width="60dp"
        android:layout_height="12dp"
        android:layout_marginStart="8dp"
        app:layout_constraintBottom_toBottomOf="@id/loader_status"
        app:layout_constraintStart_toEndOf="@id/loader_status"
        app:layout_constraintTop_toTopOf="@id/loader_status"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_date"
        android:layout_width="120dp"
        android:layout_height="12dp"
        android:layout_marginEnd="12dp"
        android:textColor="#31353B"
        app:layout_constraintBottom_toBottomOf="@id/loader_status"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/loader_status"
        app:typographyType="small"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_coupon"
        android:layout_width="44dp"
        android:layout_height="44dp"
        android:layout_marginTop="12dp"
        android:src="@drawable/tm_ic_member_grey"
        app:layout_constraintStart_toStartOf="@id/tv_coupon_state"
        app:layout_constraintTop_toBottomOf="@id/tv_coupon_state"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_title"
        android:layout_width="248dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="@dimen/dp_12"
        android:textColor="#212121"
        app:layout_constraintStart_toEndOf="@id/loader_coupon"
        app:layout_constraintTop_toTopOf="@id/loader_coupon"
        app:typographyType="heading_5"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_membership"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:padding="4dp"
        app:layout_constraintBottom_toBottomOf="@id/loader_coupon"
        app:layout_constraintStart_toStartOf="@id/loader_title"
        app:layout_constraintTop_toBottomOf="@id/loader_title"
        app:typographyType="heading_6"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/tv_quota_static"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="12dp"
        android:layout_marginBottom="16dp"
        android:textColor="#6D7588"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/loader_membership"
        app:layout_constraintTop_toBottomOf="@id/loader_coupon"
        app:typographyType="small"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

</androidx.constraintlayout.widget.ConstraintLayout>

