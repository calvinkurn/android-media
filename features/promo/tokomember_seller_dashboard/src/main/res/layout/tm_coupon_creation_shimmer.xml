<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Unify_Background"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="MissingDefaultResource">

    <com.tokopedia.unifycomponents.ticker.Ticker
        android:id="@+id/ticker"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:closeButtonVisibility="gone"
        app:descriptionText="@string/tm_vip_coupon_ticker_text"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:tickerShapeType="full"
        app:tickerType="announcement" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ticker"
        android:layout_width="@dimen/tm_dash_dp_93"
        android:layout_height="@dimen/unify_space_12"
        android:layout_marginTop="20dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/box1"
        android:layout_width="@dimen/tm_dash_dp_60"
        android:layout_height="@dimen/unify_space_12"
        android:layout_marginTop="@dimen/tm_dash_dp_18"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/box2"
        android:layout_width="@dimen/tm_dash_dp_93"
        android:layout_height="@dimen/unify_space_12"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/box3"
        android:layout_width="@dimen/tm_dash_dp_78"
        android:layout_height="@dimen/unify_space_32"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box5"
        app:layout_constraintTop_toBottomOf="@id/box3"
        app:layout_constraintStart_toEndOf="@id/box4"
        android:layout_width="@dimen/tm_dash_dp_78"
        android:layout_height="@dimen/unify_space_32"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box6"
        app:layout_constraintTop_toBottomOf="@id/box4"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unify_space_48"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/box6"
        android:layout_width="@dimen/tm_dash_dp_93"
        android:layout_height="@dimen/unify_space_12"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box12"
        app:layout_constraintTop_toBottomOf="@id/box7"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unify_space_48"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box13"
        app:layout_constraintTop_toBottomOf="@id/box12"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unify_space_48"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box9"
        app:layout_constraintTop_toBottomOf="@id/box8"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unify_space_48"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box10"
        app:layout_constraintTop_toBottomOf="@id/box9"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unify_space_48"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/divider1"
        app:layout_constraintTop_toBottomOf="@id/box13"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="@dimen/tm_dash_dp_10"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider1"
        android:layout_width="@dimen/tm_dash_dp_93"
        android:layout_height="@dimen/unify_space_12"
        android:layout_marginTop="@dimen/tm_dash_dp_28"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="@dimen/unify_space_8"
        app:loader_type="rect"/>

</androidx.constraintlayout.widget.ConstraintLayout>