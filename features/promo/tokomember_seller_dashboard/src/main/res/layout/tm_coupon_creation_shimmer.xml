<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Unify_Background"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:ignore="MissingDefaultResource">

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_width="93dp"
        android:layout_height="12dp"
        android:layout_marginTop="18dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/box1"
        android:layout_width="60dp"
        android:layout_height="12dp"
        android:layout_marginTop="18dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/box2"
        android:layout_width="93dp"
        android:layout_height="12dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/box3"
        android:layout_width="78dp"
        android:layout_height="32dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box5"
        app:layout_constraintTop_toBottomOf="@id/box3"
        app:layout_constraintStart_toEndOf="@id/box4"
        android:layout_width="78dp"
        android:layout_height="32dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box6"
        app:layout_constraintTop_toBottomOf="@id/box4"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box7"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/box6"
        android:layout_width="93dp"
        android:layout_height="12dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box12"
        app:layout_constraintTop_toBottomOf="@id/box7"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box13"
        app:layout_constraintTop_toBottomOf="@id/box12"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box9"
        app:layout_constraintTop_toBottomOf="@id/box8"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box10"
        app:layout_constraintTop_toBottomOf="@id/box9"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/divider1"
        app:layout_constraintTop_toBottomOf="@id/box13"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="10dp"
        android:layout_marginTop="28dp"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/divider1"
        android:layout_width="93dp"
        android:layout_height="12dp"
        android:layout_marginTop="28dp"
        android:layout_marginStart="@dimen/unify_space_32"
        android:layout_marginEnd="@dimen/unify_space_32"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>


    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/box11"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="48dp"
        android:layout_margin="18dp"
        app:loader_corner_radius="8dp"
        app:loader_type="rect"/>

</androidx.constraintlayout.widget.ConstraintLayout>