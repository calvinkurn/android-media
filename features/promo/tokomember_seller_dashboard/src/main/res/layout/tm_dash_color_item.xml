<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    app:cardCornerRadius="@dimen/unify_space_8"
    tools:ignore="MissingDefaultResource">

    <FrameLayout
        android:id="@+id/colorFrame"
        android:layout_gravity="center"
        android:layout_width="@dimen/unify_space_40"
        android:background="@drawable/tm_bg_round_corner"
        android:layout_height="@dimen/unify_space_40" />

    <View
        android:id="@+id/viewColorSelector"
        android:layout_width="@dimen/unify_space_48"
        android:layout_height="@dimen/unify_space_48"
        android:visibility="gone"
        android:background="@drawable/tm_dash_item_selector"/>

</androidx.cardview.widget.CardView>