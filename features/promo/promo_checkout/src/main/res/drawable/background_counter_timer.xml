<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="15dp"/>
            <gradient android:angle="90" android:endColor="@color/Unify_NN0" android:startColor="@color/Unify_NN50"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="15dp"/>
                <gradient android:angle="90" android:endColor="@color/Unify_RN100" android:startColor="@color/Unify_RN100"/>
            </shape>
        </clip>
    </item>
</layer-list>
