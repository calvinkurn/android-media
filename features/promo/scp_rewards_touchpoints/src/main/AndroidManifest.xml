<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.scp_rewards_touchpoints">

    <application>
        <activity
            android:name=".RewardsTouchPointActionsActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.DayNight.Translucent"
            >
            <intent-filter>
                <action android:name="com.tokopedia.internal.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="scp-rewards"
                    android:pathPattern="/medali/celebration-bottomsheet/.*"
                    android:scheme="@string/internal_scheme" />
            </intent-filter>
        </activity>
    </application>

</manifest>