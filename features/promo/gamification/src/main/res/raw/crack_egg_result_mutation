mutation CrackResult($tokenUserID: Int!, $campaignID: Int!) {    crackResult(tokenUserID: $tokenUserID, campaignID: $campaignID) {        resultStatus {            code            message            status        }        imageUrl        benefitType        benefits {            text            color            size        }        ctaButton {            title            url            applink            type        }        returnButton {            title            url            applink            type        }    }}