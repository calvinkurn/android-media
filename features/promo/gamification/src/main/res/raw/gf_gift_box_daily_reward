mutation GamiCrack($campaignSlug:String!,uniqueCode:String!){
  gamiCrack(campaignSlug:$campaignSlug,uniqueCode:uniqueCode) {
    resultStatus {
      code
      message
      status
    }
    imageUrl
    benefitText
    benefits {
      text
      color
      size
      benefitType
      imageUrl
      dummyCode
      referenceID
      isAutoApply
      autoApplyMsg
      isBigPrize
    }
    actionButton {
      text
      url
      applink
      type
      backgroundColor
    }
  }
}