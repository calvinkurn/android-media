query flightVoucher($cartID: String!, $voucherCode: String!) {
    status
    flightVoucher(cartID: $cartID, voucherCode: $voucherCode) {
        voucherCode
        UserID
        DiscountAmount
        DiscountAmountPlain
        CashbackAmount
        CashbackAmountPlain
        DiscountedPrice
        DiscountedPricePlain
        Message
        VoucherAmount
    }
}