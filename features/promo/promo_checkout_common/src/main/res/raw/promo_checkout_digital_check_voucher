query rechargeCheckVoucher($data: RechargeInputVoucher!) {
    rechargeCheckVoucher(voucher: $data) {
        data {
          success
          code
          discount_amount
          cashback_amount
          promo_code_id
          is_coupon
          message {
            state
            color
            text
          }
          title_description
        }
        errors{
          status
          title
        }
    }
}