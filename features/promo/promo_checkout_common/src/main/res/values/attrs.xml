<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TickerCheckoutView">
        <attr name="state" format="enum">
            <enum name="inactive" value="0"/>
            <enum name="active" value="1"/>
            <enum name="failed" value="2"/>
            <enum name="empty" value="3"/>
        </attr>
        <attr name="title" format="string"/>
        <attr name="counter" format="string"/>
        <attr name="desc" format="string"/>
    </declare-styleable>
</resources>