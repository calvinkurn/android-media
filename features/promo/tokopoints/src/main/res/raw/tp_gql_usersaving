query tokopointsUserSaving($year: Int, $month: Int, $type: Int, $apiVersion: String){
    tokopointsUserSaving:tokopointsUserSaving(year: $year, month: $month, type: $type, apiVersion: $apiVersion){
    resultStatus {
      code
      status
    }
    userSaving{
      title
      userTotalSaving
      userTotalSavingStr
      backgroundImageURL
      backgroundImageURLOptimized
      backgroundImageURLDesktop
      descriptions{
        text
        fontStyle
      }
    }
  }
}