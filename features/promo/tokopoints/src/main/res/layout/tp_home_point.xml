<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/layout_homepoint"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_80"
    android:padding="@dimen/dp_8">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvNonLoginCta"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/white"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.603"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="heading_4" />


    <LinearLayout
        android:id="@+id/ll_tokomember"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_4"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ll_challenge"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/img_tokomember"
            android:layout_width="30dp"
            android:layout_height="@dimen/dp_30"
            android:layout_gravity="center"/>

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_tokomember"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_8"
            tools:text="Tokomember"
            android:textColor="#AD31353B" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_challenge"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_4"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ll_mycoupon"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ll_tokomember"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.375">

        <ImageView
            android:id="@+id/img_challenge"
            android:layout_width="@dimen/dp_30"
            android:layout_height="@dimen/dp_30"
            android:layout_gravity="center" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_challenge"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_8"
            tools:text="Challenge"
            android:textColor="#AD31353B" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/ll_mycoupon"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/dp_4"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toEndOf="@+id/ll_challenge"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0">

        <ImageView
            android:id="@+id/img_mycoupon"
            android:layout_width="30dp"
            android:layout_height="@dimen/dp_30"
            android:layout_gravity="center" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_mycoupon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_8"
            tools:text="My Coupon"
            android:textColor="#AD31353B" />
    </LinearLayout>

    <View
        android:id="@+id/line_separator_points_vertical"
        android:layout_width="1dp"
        android:layout_height="@dimen/dp_16"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@color/white_two"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/ll_challenge"
        app:layout_constraintStart_toEndOf="@id/ll_tokomember"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/line_separator_points_vertical1"
        android:layout_width="1dp"
        android:layout_height="@dimen/dp_16"
        android:layout_marginTop="8dp"
        android:layout_marginBottom="8dp"
        android:background="@color/white_two"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/ll_mycoupon"
        app:layout_constraintStart_toEndOf="@id/ll_challenge"
        app:layout_constraintTop_toTopOf="parent" />


