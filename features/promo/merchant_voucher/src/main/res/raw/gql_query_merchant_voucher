query GetPublicMerchantVoucherList($shop_id: Int!, $num_voucher: Int!) {
getPublicMerchantVoucherList(shop_id:$shop_id, num_voucher:$num_voucher) {
vouchers { voucher_id voucher_name voucher_code voucher_type { voucher_type identifier }
voucher_code amount { amount_type amount } minimum_spend
owner { owner_id identifier }
valid_thru tnc
banner { desktop_url mobile_url }
status { status identifier }
in_use_expiry }
error_code
error_message_title
error_message} }