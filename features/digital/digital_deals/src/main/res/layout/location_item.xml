<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="@dimen/new_margin_med"
    android:orientation="vertical">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/new_margin_sm"
        android:fontFamily="sans-serif"
        android:lineSpacingExtra="2sp"
        android:text="Bali"
        android:textColor="@color/black_39"
        android:textSize="@dimen/text_size_medium"
        android:textStyle="normal" />

    <View
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/divider_height_thin"
        android:layout_marginTop="@dimen/new_margin_lm_22"
        android:background="@color/grey_300" />

</LinearLayout>