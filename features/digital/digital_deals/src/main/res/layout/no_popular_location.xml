<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/title_no_loc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        android:textColor="@color/clr_f531353b"
        android:textSize="@dimen/sp_16"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="center"
        android:text="Yah, nggak ada hasil yang cocok"/>


    <TextView
        android:id="@+id/subtitle_no_loc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/clr_ae31353b"
        app:layout_constraintTop_toBottomOf="@+id/title_no_loc"
        android:textSize="@dimen/sp_14"
        android:layout_marginTop="@dimen/dp_16"
        android:gravity="center"
        android:text="Coba cari ulang pakai kata kunci laink"/>

</androidx.constraintlayout.widget.ConstraintLayout>