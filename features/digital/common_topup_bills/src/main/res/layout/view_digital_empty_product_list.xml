<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/telco_empty_state_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical"
    android:padding="@dimen/sp_16">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/title_empty_product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_85"
        android:lines="1"
        android:textColor="@color/Neutral_N700_96"
        app:typographyType="heading_4"
        tools:text="Paket Roaming Tidak Tersedia" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/desc_empty_product"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/title"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="center_horizontal"
        app:typographyType="body_3"
        tools:text="Produk paket roaming untuk provider ini belum tersedia, silakan coba provider lainnya." />

</LinearLayout>