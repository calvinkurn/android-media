query SearchSchedule($date: String!, $origin: String!, $originCity: String!, $destination: String!, $destinationCity: String!) {
    kaiSearchSchedule(date: $date, origin: $origin, originCity: $originCity, destination: $destination, destinationCity: $destinationCity) {
        schedules {
            origin
            destination
            trains {
                id
                arrivalTimestamp
                departureTimestamp
                displayDuration
                duration
                trainKey
                trainName
                trainNo
                fares {
                    id
                    class
                    displayClass
                    subclass
                    adultFare
                    displayAdultFare
                    infantFare
                    displayInfantFare
                }
            }
        }
    }
}