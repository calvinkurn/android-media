mutation kaiCheckVoucher($reservationId: String!, $reservationCode: String!, $galaCode: String!) {
    kaiCheckGala(input : {reservationId: $reservationId, reservationCode: $reservationCode, galaCode: $galaCode} ) {
        galaCode
        discountAmount
        cashbackAmount
        successMessage
        isCoupon
        invoiceDescription
    }
}