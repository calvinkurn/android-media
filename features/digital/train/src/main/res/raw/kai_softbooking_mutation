mutation kaiSoftbook($input: KaiBookingData!) {
    kaiPesanLunak(input: $input) {
        reservationId
        reservationCode
        expiryTimestamp
        departureTrip {
            bookCode
            org
            des
            trainNo
            trainClass
            subclass
            departureTimestamp
            arrivalTimestamp
            errCode
            numcode
            paxPassenger {
                name
                idNumber
                paxType
                seat {
                    wagonCode
                    row
                    column
                }
            }
            normalSales
            displayNormalSales
            extraFee
            displayExtraFee
            bookBalance
            displayBookBalance
            discount
            displayDiscount
            adultPrice
            displayAdultPrice
            infantPrice
            displayInfantPrice
            totalPriceAdult
            displayTotalPriceAdult
            totalPriceInfant
            displayTotalPriceInfant
            totalPrice
            displayTotalPrice
        }
        returnTrip {
            bookCode
            org
            des
            trainNo
            trainClass
            subclass
            departureTimestamp
            arrivalTimestamp
            errCode
            paxPassenger {
                name
                idNumber
                paxType
                seat {
                    wagonCode
                    row
                    column
                }
            }
            normalSales
            displayNormalSales
            extraFee
            displayExtraFee
            bookBalance
            displayBookBalance
            discount
            displayDiscount
            adultPrice
            displayAdultPrice
            infantPrice
            displayInfantPrice
            totalPriceAdult
            displayTotalPriceAdult
            totalPriceInfant
            displayTotalPriceInfant
            totalPrice
            displayTotalPrice
        }
    }
}