mutation kaiCheckout($reservationId: String!, $reservationCode: String!, $galaCode: String!, $client: String!, $version: String!) {
  kaiCheckout(input: {reservationId: $reservationId, reservationCode: $reservationCode, galaCode: $galaCode, client: $client, version: $version}) {
    redirectURL
    thanksURL
    callbackURLSuccess
    callbackURLFailed
    queryString
    parameter {
      merchantCode
      profileCode
      transactionId
      transactionDate
      customerName
      customerEmail
      customerMsisdn
      currency
      itemsName
      itemsQuantity
      itemsPrice
      amount
      signature
      userDefinedValue
    }
  }
}