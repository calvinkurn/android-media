query GetTravelBanner ($languageId: String!, $countryId : String!, deviceId : String!, instance : String!){
  travelBanner(languageID: $languageId,countryID: $countryId, deviceID: $deviceId, instance:$instance){
    id,
    attributes{
      description
      linkURL
      imageURL
      promoCode
    }
  }
}