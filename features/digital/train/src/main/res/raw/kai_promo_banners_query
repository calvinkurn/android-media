query travelBanner($languageId: Int!, $countryId : String!, $deviceId : Int!, $instance : Int!) {
  travelBanner(languageID: $languageId,countryID: $countryId, deviceID: $deviceId, instanceID:$instance) {
    id
    attributes {
      description
      linkURL
      imageURL
      promoCode
    }
  }
}