query Seatmap($date: String!, $origin: String!, $destination: String!, $trainNo: String!, $subclass: String!) {
  kaiSeatmap(input : {date: $date, origin: $origin, destination: $destination, trainNo: $trainNo, subclass: $subclass} ) {
    seatmaps{
      id
      origin
      destination
      trainNo
      departureTimestamp
      wagons{
        wagonCode
        seatDetail{
          column
          row
          status
          subClass
        }
      }
    }
  }
}