mutation debitRegisterBca($merchantCode : String!, $action : String!, $bankCode : String!, $callbackUrl : String!, $signature : String!, $debitData : String!){
    notifyDebitRegister(merchantCode : $merchantCode, action : $action, bankCode : $bankCode, callbackUrl : $callbackUrl, signature : $signature, debitData : $debitData) {
    success
    processTime
    data {
      callbackURL
      debitData
    }
  }
}