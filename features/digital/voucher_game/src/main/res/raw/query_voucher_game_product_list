query voucherGameProductList($menuID: Int!){
  rechargeCatalogOperatorSelect(menuID: $menuID, platformID: 5) {
    componentID
    name
    paramName
    text
    operators {
      id
      attributes {
        name
        image
        image_url
        help_cta
        help_text
        help_image
        operator_labels
      }
    }
  }
}