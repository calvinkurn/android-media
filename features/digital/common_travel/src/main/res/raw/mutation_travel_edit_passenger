mutation travelUpdatePassenger($input: TravelPassengerGeneric!, $instanceType: TravelType!) {
  travelUpdatePassenger(
    input: $input
    instanceType: $instanceType) {
        id
        userId
        title
        name
        firstName
        lastName
        birthDate
        nationality
        passportNo
        passportCountry
        passportExpiry
        idNumber
        isBuyer
        paxType
        travelId
  }
}