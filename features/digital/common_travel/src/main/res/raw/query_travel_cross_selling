query OrderDetailsQuery($orderCategoryStr: String, $orderId: String) {
  status
  orderDetails(orderCategoryStr: $orderCategoryStr, orderId: $orderId) {
    crossSell{
           items {
              product
              title
              content
              prefix
              uri
              uriWeb
              imageURL
            }
           meta{
              title
              uri
              uriWeb
            }
      }
  }
}