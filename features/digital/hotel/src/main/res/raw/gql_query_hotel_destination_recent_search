query propertyRecentSearch($id: Int!){
 travelRecentSearch(dataType:HOTEL,userID:$id){
   UUID
   property {
     type
     value
     ID
     location{
       district
       region
       city
       country
     }
   }
   startTime
   endTime
   lastSearch
   customer {
     adult
     child
     class
     infant
     room
   }
 }
}