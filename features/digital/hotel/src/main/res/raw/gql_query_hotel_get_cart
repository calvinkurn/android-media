query PropertyCart($data: PropertyGetCartRequest!) {
    propertyGetCart(input: $data) {
        cartID
        cart {
            contact {
                name
                email
                phone
                phoneCode
            }
            adult
            rooms {
                roomID
                numOfRooms
            }
            checkIn
            checkOut
            guestName
            specialRequest
            totalPrice
            totalPriceAmount
            localTotalPrice
            localTotalPriceAmount
            currency
            fares {
                type
                description
                price
                priceAmount
                localPrice
                localPriceAmount
            }
        }
        property {
          propertyID
          name
          type
          address
          image
          star
          paymentNote
          room {
            roomID
            isBreakFastIncluded
            maxOccupancy
            roomName
            isNeedDeposit
            isRefundAble
            isCCRequired
            paymentTerms {
              name
              cancellationDescription
              prepaymentDescription
            }
            roomPolicies {
              content
              policyType
            }
            cancellationPolicies {
              title
              content
              isClickable
              details {
                shortTitle
                longTitle
                shortDesc
                longDesc
                isActive
              }
            }
          }
          checkInFrom
          checkInTo
          checkOutFrom
          checkOutTo
          isDirectPayment
        }
    }
}