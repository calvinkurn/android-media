<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SwitcherCustomView">
        <attr name="leftSubtitleText" format="string" />
        <attr name="leftTitleText" format="string" />
        <attr name="rightSubtitleText" format="string" />
        <attr name="rightTitleText" format="string" />
    </declare-styleable>

    <declare-styleable name="TicketView">
        <attr format="reference" name="tv_anchor1"/>
        <attr format="reference" name="tv_anchor2"/>

        <attr format="color" name="tv_dashColor"/>
        <attr format="dimension" name="tv_dashSize"/>
        <attr format="dimension" name="tv_circleSpace"/>
        <attr format="dimension" name="tv_circleRadius"/>
    </declare-styleable>
    <declare-styleable name="InfoTextView">
        <attr name="titleText" format="string" />
        <attr name="descriptionText" format="string" />
        <attr name="truncateDescription" format="boolean" />
    </declare-styleable>
</resources>