query TravelCollectiveOrderList($page: Int!, $perPage: Int!, $filterStatus: String!) {
  TravelCollectiveOrderList(product: ALL, page: $page, perPage: $perPage, filterStatus: $filterStatus) {
      orders{
        product
        title
        subtitle
        prefix
        value
        webURL
        appURL
        imageURL
      }

      meta{
        title
        appURL
        webURL
      }
  }
}