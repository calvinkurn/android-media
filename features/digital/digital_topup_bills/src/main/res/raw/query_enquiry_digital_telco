query enquiryTelco ($clientNumber: String!,
  $productId: String!) {
  status
  enquiry(
    clientNumber: $clientNumber,
    productId: $productId,
    requestType: NEW) {
    attributes: Attributes {
      UserID
      ProductID
      ClientNumber
      Title
      Price
      PricePlain
      mainInfo: MainInfo {
        label: Label
        value: Value
      }
    }
  }
}