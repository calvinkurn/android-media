query GetBankListWDV2($isAdmin: Boolean!){
    GetBankListWDV2(isAdmin: $isAdmin){
        status
        message
        data {
            bankID
            accountNo
            bankName
            bankBranch
            bankAccountID
            bankImageUrl
            isDefaultBank
            adminFee
            minAmount
            maxAmount
            status
            isVerifiedAccount
            accountName
            statusFraud
            copyWriting
            is_fraud
            have_rp_program
            have_special_offer
            default_bank_account
            warning_message
            warning_color
            notes
            wallet_app_data {
                message
                cta_copy_writing
                cta_link
            }
        }
        gopay_data {
            limit
            limit_copy_writing
            image_url
            widget_note
            bottomsheet_data {
                title
                description
                balance
            }
        }
    }
}