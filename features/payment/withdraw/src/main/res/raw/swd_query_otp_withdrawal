mutation richieSubmitWithdrawal(
     $isSeller:Boolean,
     $amount:String!,
     $userId:String!,
     $email:String!,
     $action:String!,
     $type:Int!,
     $deviceType:String!,
     $token:String!,
     $masterEmail:String!,
     $masterID: String!,
     $lang:String!,
     $accountID: String!,
     $accountName: String!,
     $accountNumber: String!,
     $bankId:String!,
     $bankName: String!,
     $validateToken:String!,
     $program:String,
     $isJoinRP:Boolean
){
 richieSubmitWithdrawal(
        input : {
             isSeller:$isSeller,
             amount:$amount,
             userId:$userId,
             email:$email,
             action:$action,
             type:$type,
             deviceType:$deviceType,
             token:$token,
             masterEmail:$masterEmail,
             masterID:$masterID,
             lang:$lang,
             accountID:$accountID,
             accountName:$accountName,
             accountNumber:$accountNumber,
             bankId:$bankId,
             bankName:$bankName,
             validateToken:$validateToken,
             program:$program
             isJoinRP:$isJoinRP
        }
 ){
   process_time
   message
   status
   message_error
   accountName
   accountNumber
   bankName
   amount
   adminFee
   withdrawalNote
   title
   description
   joinPromptMessageResponse{
       title
       desc
       actionText
       actionLink
       isSuccess
       statusCode
   }
   errorCode
 }
}