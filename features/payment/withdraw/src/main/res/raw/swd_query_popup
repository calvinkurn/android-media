query ValidatePopUpWithdrawal($bankID : String!, $language:String){
    ValidatePopUpWithdrawal(bankID: $bankID, language:$language){
        status
        message
        data{
            title
            note
            needShow
        }
        joinData{
            title
            desc
            isJoinPrompt
            programID
            programName
           }
    }
}