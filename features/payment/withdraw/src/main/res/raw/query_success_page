mutation richieSubmitWithdrawal(
     $isSeller:Boolean,
     $amount:String!,
     $userId:String!,
     $email:String!,
     $action:String!,
     $type:Int!,
     $deviceType:String!,
     $token:String!,
     $masterEmail:String!,
     $masterID:String!,
     $lang:String!,
     $accountID:String!,
     $accountName:String!,
     $accountNumber:String!,
     $bankId:String!,
     $bankName:String!,
     $validateToken:String!,
     $password:String,
     $program:String){
 richieSubmitWithdrawal(
     input : {
     isSeller:$isSeller,
     amount:$amount,
     userId:$userId,
     email:$email,
     action:$action,
     type:$type,
     deviceType:$deviceType,
     token:$token,
     masterEmail:$masterEmail,
     masterID:$masterID,
     lang:$lang,
     accountID:$accountID,
     accountName:$accountName,
     accountNumber:$accountNumber,
     bankId:$bankId,
     bankName:$bankName,
     validateToken:$validateToken,
     password:$password,
     program:$program
   }
 ){
   process_time
   message
   message_error
   status
   errorCode
 }
}