mutation UpsertAutoWDData {
  UpsertAutoWDData(
    input: {
      autoWDUserId: $autoWDUserId
			oldAutoWDScheduleId: $oldAutoWDScheduleId
			scheduleType: $scheduleType
			accId: $accId
			accNo: $accNo
			bankId: $bankId
			isUpdate: $isUpdate
			validateToken: $validateToken
			isQuit: $isQuit
    }
  ){
    code
    message
  }
}