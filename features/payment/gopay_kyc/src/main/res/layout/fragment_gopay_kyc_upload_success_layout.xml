<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Unify_N0">

    <include
        android:id="@+id/uploadFailLayout"
        layout="@layout/gopay_kyc_success_failed_empty_layout"
        android:layout_width="@dimen/unify_space_0"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/unify_space_16"
        android:layout_marginEnd="@dimen/unify_space_16"
        app:layout_constraintBottom_toTopOf="@+id/finishButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.unifycomponents.UnifyButton
        android:id="@+id/finishButton"
        android:layout_width="@dimen/unify_space_0"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/unify_space_16"
        android:layout_marginEnd="@dimen/unify_space_16"
        android:layout_marginBottom="@dimen/unify_space_24"
        android:text="@string/gopay_kyc_finish_kyc_button_text"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:unifyButtonSize="large"
        app:unifyButtonType="main"
        app:unifyButtonVariant="filled" />

</androidx.constraintlayout.widget.ConstraintLayout>