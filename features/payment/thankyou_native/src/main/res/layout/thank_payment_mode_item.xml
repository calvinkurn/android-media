<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoicePaymentModeName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:textColor="@color/Neutral_N700_68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="body_3"
        tools:text="Payment Mode Name" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoicePaidWithModeValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="end"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/tvInvoicePaymentModeName"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="body_3"
        tools:text="RP2323234423" />

</androidx.constraintlayout.widget.ConstraintLayout>