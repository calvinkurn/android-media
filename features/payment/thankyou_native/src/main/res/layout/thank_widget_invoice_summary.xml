<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="@dimen/dp_16">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalPrice"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:text="@string/thank_invoice_total_price"
        android:textColor="@color/Neutral_N700_68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="body_3" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalPriceValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="end"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/tvInvoiceSummaryTotalPrice"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="body_3"
        tools:text="@string/thank_payment_method" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalDiscount"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/thank_invoice_total_discount"
        android:textColor="@color/Neutral_N700_68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalPrice"
        app:typographyType="body_3" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalDiscountValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="end"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalPrice"
        app:typographyType="body_3"
        tools:text="@string/thank_payment_method" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalProductProtection"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/thank_invoice_total_product_protect"
        android:textColor="@color/Neutral_N700_68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalDiscount"
        app:typographyType="body_3" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalProductProtectionValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="end"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalDiscount"
        app:typographyType="body_3"
        tools:text="@string/thank_payment_method" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalShippingCost"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/thank_invoice_total_shipping_price"
        android:textColor="@color/Neutral_N700_68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalProductProtection"
        app:typographyType="body_3" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalShippingCostValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="end"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalProductProtection"
        app:typographyType="body_3"
        tools:text="@string/thank_payment_method" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalShippingDiscount"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/thank_invoice_total_shipping_discount"
        android:textColor="@color/Neutral_N700_68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalShippingCost"
        app:typographyType="body_3" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalShippingCostDiscountValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="end"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalShippingCost"
        app:typographyType="body_3"
        tools:text="@string/thank_payment_method" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalShippingInsurance"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/thank_invoice_total_shipping_insurance"
        android:textColor="@color/Neutral_N700_68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalShippingDiscount"
        app:typographyType="body_3" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalShippingInsuranceValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="end"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalShippingDiscount"
        app:typographyType="body_3"
        tools:text="@string/thank_payment_method" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalDonation"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/thank_invoice_total_donation"
        android:textColor="@color/Neutral_N700_68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalShippingInsurance"
        app:typographyType="body_3" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalDonationValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="end"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalShippingInsurance"
        app:typographyType="body_3"
        tools:text="@string/thank_payment_method" />


    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalEGold"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:text="@string/thank_invoice_eGold"
        android:textColor="@color/Neutral_N700_68"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalDonation"
        app:typographyType="body_3" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvInvoiceSummaryTotalEGoldValue"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_8"
        android:gravity="end"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvInvoiceSummaryTotalDonation"
        app:typographyType="body_3"
        tools:text="@string/thank_payment_method" />



</androidx.constraintlayout.widget.ConstraintLayout>