query topAdsListKeyword($source: String!, $filter: TopAdsKeywordFilterReq, $page: TopAdsKeywordPageReq) {
  topAdsListKeyword(source: $source, filter: $filter, page: $page) {
    error {
      code
      title
      detail
    }
    data {
      keywords {
        keyword_id
        tag
        type
        status
        price_bid
      }
      pagination {
        next_cursor
      }
    }
  }
}
