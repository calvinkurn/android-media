query BidInfo(
  $dataSuggestions: [BidInfoDataSuggestions]!,
  $shopId: Int!,
  $requestType: String!,
  $source: String!
) {
  topadsBidInfo(dataSuggestions: $dataSuggestions, shopId: $shopId, requestType: $requestType, source: $source) {
    data {
      id
      max_bid
      min_bid
      suggestion_bid
    }
    request_type
  }
}