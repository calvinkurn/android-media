query topadsGetListProductsOfGroupV2($shopID: String!, $page: Int!, $perPage: Int!, $type: String!, $groupID: String!) {
  topadsGetListProductsOfGroupV2(shopID: $shopID, page: $page, perPage: $perPage, type: $type, groupID: $groupID) {
       page {
         perPage
         total
       }
    data {
      adID
      itemID
      adPriceBidFmt
      adDetailProduct {
        productName
        productURI
        productImageURI
      }
      groupName
      groupID
    }
  }
}
