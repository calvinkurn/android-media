<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.tokopedia.topads.create">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        tools:replace="android:label">

        <activity android:name="com.tokopedia.topads.view.activity.CreateAdsOnboardingActivity"
            android:label="@string/mulai_beriklan">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="@string/host_topads"
                    android:path="/create-ads"
                    android:scheme="@string/internal_scheme" />
            </intent-filter>
        </activity>

        <activity android:name="com.tokopedia.topads.view.activity.CreateAdsStepperActivity" />
    </application>
</manifest>


