query topAdsGetKeywordSuggestionV2($product_ids: String!, $group_id: Int, $shop_id: Int!) {
  topAdsGetKeywordSuggestionV2(product_ids: $product_ids, group_id: $group_id, shop_id: $shop_id) {
    data {
      keyword
      bid_suggest
      total_search
      min_bid
    }
    errors {
      code
      detail
      object {
        type
        text
      }
      title
    }
  }
}
