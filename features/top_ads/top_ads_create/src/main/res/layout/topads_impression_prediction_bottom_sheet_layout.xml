<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:ignore="MissingDefaultResource">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/searchBidTitle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="small"
        tools:text="Di Pencarian" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/searchBidPrediction"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/searchBidTitle"
        app:typographyType="paragraph_3"
        app:typographyWeight="bold"
        tools:text="400x/minggu" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/RecomBidPrediction"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/searchBidPrediction"
        app:typographyType="small"
        tools:text="+12% meningkat" />

</androidx.constraintlayout.widget.ConstraintLayout>