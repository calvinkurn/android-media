<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout2"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/topads_create_bg_top_round"
    android:padding="@dimen/dp_16">


    <ImageView
        android:id="@+id/btn_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="@+id/typography22"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/typography22"
        app:srcCompat="@drawable/topads_create_ic_group_close" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography22"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_8"
        android:gravity="center_vertical"
        android:text="@string/tip_memilih_kata_kunci"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_close"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="heading_3" />


    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="@+id/typography23"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/typography23"
        app:srcCompat="@drawable/topads_create_ic_checklist" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography23"
        style="@style/TextView.Small"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_16"
        android:layout_marginLeft="@dimen/dp_8"
        android:gravity="top"
        android:text="@string/sesuaikan_kata_kunci_dengan_nama_produk"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/typography22"
        app:typographyType="body_2" />


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="@+id/typography25"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/typography25"
        app:srcCompat="@drawable/topads_create_ic_checklist" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography25"
        style="@style/TextView.Small"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_16"
        android:layout_marginLeft="@dimen/dp_8"
        android:text="@string/lebih_efektif_dengan_kata_kunci_spesifik"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/typography23"
        app:typographyType="body_2" />


    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="@+id/typography26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/typography26"
        app:srcCompat="@drawable/topads_create_ic_checklist" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography26"
        style="@style/TextView.Small"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_8"
        android:layout_marginTop="@dimen/dp_16"
        android:text="@string/gunakan_kata_kunci_umum_dengan_maksimum_2_kata"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@+id/typography25"
        app:typographyType="body_2" />

</androidx.constraintlayout.widget.ConstraintLayout>