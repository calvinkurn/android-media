<?xml version="1.0" encoding="utf-8"?>
<com.tokopedia.unifycomponents.CardUnify xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/card2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/dp_16"
    android:layout_marginRight="@dimen/dp_16">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_16">

        <com.tokopedia.unifycomponents.ImageUnify
            android:id="@+id/top_corner_icon2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.tokopedia.unifycomponents.ImageUnify
            android:id="@+id/top_corner_bg2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/typography9"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_16"
            android:text="@string/iklan_manual"
            android:textColor="@color/black"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/top_corner_icon2"
            app:typographyType="heading_3" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/typography10"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/iklan_manual_subtitle"
            app:layout_constraintTop_toBottomOf="@id/typography9"
            app:typographyType="body_3" />

        <com.tokopedia.unifycomponents.ImageUnify
            android:id="@+id/icon6"
            android:layout_width="@dimen/dp_24"
            android:layout_height="@dimen/dp_24"
            android:layout_alignParentStart="true"
            app:layout_constraintBottom_toBottomOf="@+id/desc1"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/typography10"
            app:layout_constraintTop_toTopOf="@+id/desc_1"
            tools:ignore="UnknownIdInLayout" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/desc_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_16"
            android:gravity="start"
            android:text="@string/iklan_manual_desc1"
            app:layout_constraintStart_toEndOf="@+id/icon6"
            app:layout_constraintTop_toBottomOf="@+id/typography10"
            app:typographyType="body_3" />

        <com.tokopedia.unifycomponents.ImageUnify
            android:id="@+id/icon7"
            android:layout_width="@dimen/dp_24"
            android:layout_height="@dimen/dp_24"
            android:layout_alignParentStart="true"
            app:layout_constraintBottom_toBottomOf="@id/desc_2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/icon6"
            app:layout_constraintTop_toTopOf="@id/desc_2" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/desc_2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_12"
            android:text="@string/iklan_manual_desc2"
            app:layout_constraintStart_toEndOf="@+id/icon7"
            app:layout_constraintTop_toBottomOf="@+id/desc_1"
            app:typographyType="body_3" />

        <com.tokopedia.unifycomponents.ImageUnify
            android:id="@+id/icon8"
            android:layout_width="@dimen/dp_24"
            android:layout_height="@dimen/dp_24"
            android:layout_alignParentStart="true"
            app:layout_constraintBottom_toBottomOf="@+id/desc_3"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/icon7"
            app:layout_constraintTop_toTopOf="@+id/desc_3" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/desc_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_12"
            android:layout_marginTop="@dimen/dp_12"
            android:text="@string/iklan_manual_desc3"
            app:layout_constraintStart_toEndOf="@+id/icon8"
            app:layout_constraintTop_toBottomOf="@+id/desc_2"
            app:typographyType="body_3" />

        <com.tokopedia.unifycomponents.UnifyButton
            android:id="@+id/btn_start_manual_ads"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_24"
            android:gravity="center"
            android:text="@string/mulai_iklan_manual"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/desc_3"
            app:unifyButtonSize="medium"
            app:unifyButtonType="main"
            app:unifyButtonVariant="filled" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.tokopedia.unifycomponents.CardUnify>
