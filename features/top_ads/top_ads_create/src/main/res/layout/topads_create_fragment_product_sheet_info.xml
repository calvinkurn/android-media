<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linearLayout8"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/dp_16"
    android:background="@drawable/topads_bg_top_round">


    <com.tokopedia.unifycomponents.ImageUnify
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography32"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="top"
        android:textColor="@color/topads_heading_color"
        android:layout_marginLeft="@dimen/dp_12"
        android:text="@string/pilih_produk_dengan_ulasan_terbanyak"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView5"
        app:typographyType="body_2" />

    <com.tokopedia.unifycomponents.ImageUnify
        android:id="@+id/imageView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="@+id/typography33"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/typography33" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography33"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_16"
        android:layout_marginLeft="@dimen/dp_12"
        android:textColor="@color/topads_heading_color"
        android:text="@string/pilih_produk_terpopuler"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView6"
        app:layout_constraintTop_toBottomOf="@+id/typography32"
        app:typographyType="body_2" />


</androidx.constraintlayout.widget.ConstraintLayout>