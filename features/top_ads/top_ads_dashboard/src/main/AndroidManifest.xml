<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.topads.dashboard" >

    <application android:label="@string/app_name"
        android:allowBackup="true">

        <activity
            android:name=".view.activity.TopAdsDashboardActivity"
            android:label="@string/top_ads_title_dashboard"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.Green" >
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:scheme="https"
                    android:host="ta.tokopedia.com"
                    android:pathPrefix="/v2/manage/headline"/>
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW"/>
                <category android:name="android.intent.category.DEFAULT"/>
                <category android:name="android.intent.category.BROWSABLE"/>
                <data android:scheme="https"
                    android:host="ta.tokopedia.com"
                    android:pathPrefix="/v2/manage/keyword/insight"/>
            </intent-filter>
        </activity>
        <activity
            android:name=".view.activity.TopAdsAddCreditActivity"
            android:label="@string/title_top_ads_add_credit"
            android:theme="@style/Theme.White"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.TopAdsPaymentCreditActivity"
            android:label="@string/title_top_ads_add_credit"
            android:theme="@style/Theme.White"
            android:screenOrientation="portrait" />
        <activity
            android:name=".view.activity.SellerCenterActivity"
            android:label="@string/topads_menu_seller_centre"
            android:theme="@style/Theme.White"
            android:screenOrientation="portrait" />
    </application>
</manifest>
