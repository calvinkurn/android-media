<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:viewBindingIgnore="true"
    android:background="@color/Unify_Static_White"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.tokopedia.header.HeaderUnify
        android:id="@+id/headerEducationActivity"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:unify_header_is_show_back_button="true"
        app:unify_header_is_show_shadow="false"
        app:unify_header_title="@string/topads_education_title" />

    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unify_space_0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/headerEducationActivity" />
</androidx.constraintlayout.widget.ConstraintLayout>