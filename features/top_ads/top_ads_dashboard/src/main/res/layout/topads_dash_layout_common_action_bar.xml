<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:viewBindingIgnore="true"
    android:id="@+id/select_actionbar"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_44"
    android:background="@color/Unify_NN950">

    <com.tokopedia.unifycomponents.UnifyImageButton
        android:id="@+id/close_butt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:background="@color/transparent"
        app:unifyImageButtonVariant="image"
        android:tint="@color/Unify_NN500"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/topads_create_ic_group_close" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/activate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="@string/topads_dash_aktifan"
        android:textColor="@color/Unify_NN0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/deactivate"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="heading_6" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/deactivate"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="@string/topads_dash_nonaktifkan"
        android:textColor="@color/Unify_NN0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/movetogroup"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="heading_6" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/movetogroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:text="@string/topads_dash_movetogroup"
        android:textColor="@color/Unify_NN0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/delete"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="heading_6" />

    <com.tokopedia.unifycomponents.UnifyImageButton
        android:id="@+id/delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:background="@android:color/transparent"
        android:tint="@color/Unify_NN500"
        app:unifyImageButtonVariant="image"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/topads_ic_delete" />

</androidx.constraintlayout.widget.ConstraintLayout>