query GetTopadsDashboardGroupStatistics($queryInput: GetTopadsDashboardGroupStatisticsInputType!) {
  GetTopadsDashboardGroupStatistics(queryInput: $queryInput) {
    separate_statistic
    meta {
      page {
        per_page
        current
      }
    }
    data {
      group_id
      stat_avg_click
      stat_total_spent
      stat_total_impression
      stat_total_click
      stat_total_ctr
      stat_total_conversion
      stat_total_sold
    }
  }
}