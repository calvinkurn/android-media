query GetTopadsDashboardGroups($queryInput: GetTopadsDashboardGroupsInputType!) {
  GetTopadsDashboardGroups(queryInput: $queryInput) {
    separate_statistic
    data {
      group_id
      total_item
      total_keyword
      group_status
      group_status_desc
      group_status_toogle
      group_price_bid
      group_price_daily
      group_price_daily_spent_fmt
      group_price_daily_bar
      group_name
      stat_avg_click
      stat_total_spent
      stat_total_impression
      stat_total_click
      stat_total_ctr
      stat_total_conversion
      stat_total_sold
    }
  }
}
