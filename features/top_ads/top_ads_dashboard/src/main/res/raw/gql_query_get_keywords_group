query GetTopadsDashboardKeywords($queryInput :  GetTopadsDashboardKeywordsInputType!){
  GetTopadsDashboardKeywords(queryInput: $queryInput) {
    data {
      keyword_id
      keyword_tag
      keyword_status
      keyword_type_desc
      keyword_price_bid
      keyword_price_bid_fmt
      stat_avg_click
      stat_total_spent
      stat_total_impression
      stat_total_click
      stat_total_ctr
      stat_total_conversion
      stat_total_sold
    }
  }
}
