query getTopadsDashboardStatistics($startDate: String!, $endDate:String!,$shopId:Int!){
    topadsDashboardStatistics(startDate:$startDate,endDate:$endDate,shopID:$shopId){
    data{
      summary {
        ads_impression_sum_fmt
        ads_click_sum_fmt
        ads_cost_sum_fmt
        ads_all_gross_profit_fmt
      }
    }
  }
}