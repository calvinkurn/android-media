<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/edit_ad_item_widget_ads_potential_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="small"
        tools:text="Di Pencarian" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/edit_ad_item_widget_ads_potential_retention"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/edit_ad_item_widget_ads_potential_title"
        app:typographyType="paragraph_3"
        app:typographyWeight="bold"
        tools:text="400x/minggu" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/edit_ad_item_widget_ads_potential_percentage"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/edit_ad_item_widget_ads_potential_retention"
        app:typographyType="small"
        tools:text="+12% meningkat" />

</androidx.constraintlayout.widget.ConstraintLayout>
