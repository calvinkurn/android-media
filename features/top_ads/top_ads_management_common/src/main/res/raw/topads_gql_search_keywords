query topAdsKeywordSearchTerm($product_ids: String!, $search_term: String!, $shop_id: Int!, $group_id: Int) {
  topAdsKeywordSearchTerm(product_ids: $product_ids, search_term: $search_term, shop_id: $shop_id, group_id: $group_id) {
    data {
      product_id
      keyword
      bid_suggest
      min_win_bid
      max_win_bid
      total_search
      min_bid
      competition
      source
    }
    errors {
      code
      detail
      object {
        type
        text
      }
      title
    }
  }
}
