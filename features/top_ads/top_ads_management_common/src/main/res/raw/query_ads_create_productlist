query topadsGetListProduct($shopID: Int!, $keyword: String, $etalase: String, $sortBy: String,  $rows: Int!, $start: Int,$status:String) {
  topadsGetListProduct(shopID: $shopID, keyword: $keyword, etalase: $etalase, sortBy: $sortBy,  rows: $rows, start: $start,status:$status) {
    data {
      productID
      productName
      productPrice
      productImage
      adID
      priceBid
      suggestedBid
    }
    errors {
      code
      detail
      title
    }
  }
}
