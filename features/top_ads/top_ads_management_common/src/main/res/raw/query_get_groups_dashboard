query GetTopadsDashboardGroups($queryInput: GetTopadsDashboardGroupsInputType!) {
  GetTopadsDashboardGroups(queryInput: $queryInput) {
    separate_statistic
       meta {
          page {
            per_page
            current
            total
          }
        }
    data {
      group_id
      total_item
      total_keyword
      group_status
      group_status_desc
      group_status_toogle
      group_price_bid
      group_price_daily
      group_price_daily_spent_fmt
      group_price_daily_bar
      group_name
      group_type
      group_end_date

    }
  }
}
