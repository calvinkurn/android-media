query GetDashboardProductStatistics($queryInput: GetDashboardProductStatisticsInputType!) {
  GetDashboardProductStatistics(queryInput: $queryInput) {
    data {
      ad_id
      stat_avg_click
      stat_total_spent
      stat_total_impression
      stat_total_click
      stat_total_ctr
      stat_total_conversion
      stat_total_sold
    }
  }
}
