query topAdsGetKeywordSuggestionV3($product_ids: String!, $group_id: Int, $shop_id: Int!, $type: Int) {
  topAdsGetKeywordSuggestionV3(product_ids: $product_ids, group_id: $group_id, shop_id: $shop_id, type: $type) {
    data {
      min_bid
      product_id
      keyword_data {
        keyword
        bid_suggest
        total_search
        source
      }
    }
    errors {
      code
      detail
      title
    }
  }
}
