query topAdsGetPromoGroupProduct($group_id : String!, $source: String!){
   topAdsGetPromoGroupProduct(group_id: $group_id, source: $source){
    data {
      id
      name
      shop_id
      status
      ad_total
      daily_budget
      bid_settings{
        bid_type
        price_bid
      }
      strategies
    }
  }
}