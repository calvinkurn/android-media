<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="3dip"/>
            <gradient android:startColor="@color/black_twenty_percent" android:centerColor="@color/black_twenty_percent" android:centerY="0.50" android:endColor="@color/black_twenty_percent" android:angle="270"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="3dip"/>
                <gradient android:startColor="@color/tkpd_main_green" android:endColor="@color/tkpd_main_green" android:angle="90"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="3dip"/>
                <gradient android:startColor="@color/tkpd_main_green" android:endColor="@color/tkpd_main_green" android:angle="90"/>
            </shape>
        </clip>
    </item>
</layer-list>
