<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout8"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/topads_bg_top_round"
    android:padding="@dimen/dp_16">


    <ImageView
        android:id="@+id/btn_close"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/topads_create_ic_group_close" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography31"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:gravity="center_vertical"
        android:text="@string/tip_title"
        android:textColor="@color/black"
        app:layout_constraintBottom_toBottomOf="@+id/btn_close"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/btn_close"
        app:layout_constraintTop_toTopOf="@+id/btn_close"
        app:typographyType="heading_3" />


    <ImageView
        android:id="@+id/imageView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="@+id/typography2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/typography2"
        app:srcCompat="@drawable/topads_create_ic_checklist" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_16"
        android:gravity="top"
        android:text="@string/edit_auto_ads_sheet_desc1"
        android:textColor="@color/topads_heading_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView1"
        app:layout_constraintTop_toBottomOf="@+id/typography31"
        app:typographyType="body_2" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="@+id/typography3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/typography3"
        app:srcCompat="@drawable/topads_create_ic_checklist" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_16"
        android:text="@string/edit_auto_ads_sheet_desc2"
        android:textColor="@color/topads_heading_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/typography2"
        app:typographyType="body_2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="@+id/typography4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/typography4"
        app:srcCompat="@drawable/topads_create_ic_checklist" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_16"
        android:text="@string/edit_auto_ads_sheet_desc3"
        android:textColor="@color/topads_heading_color"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView3"
        app:layout_constraintTop_toBottomOf="@+id/typography3"
        app:typographyType="body_2" />

    <ImageView
        android:id="@+id/imageView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:padding="@dimen/dp_2"
        app:layout_constraintBottom_toBottomOf="@+id/typography5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/typography5"
        app:srcCompat="@drawable/topads_create_ic_checklist" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_16"
        android:text="@string/edit_auto_ads_sheet_desc4"
        android:textColor="@color/topads_heading_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView4"
        app:layout_constraintTop_toBottomOf="@id/typography4"
        app:typographyType="body_2" />


</androidx.constraintlayout.widget.ConstraintLayout>