<manifest xmlns:android="http://schemas.android.com/apk/res/android"

    package="com.tokopedia.mitratoppers">

    <application android:label="@string/app_name">
        <activity
            android:name=".dashboard.MitraToppersDashboardActivity"
            android:label="@string/title_mitra_toppers"
            android:screenOrientation="portrait"
            android:exported="true"
            android:theme="@style/Theme.White">
            <intent-filter android:autoVerify="true">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:scheme="@string/internal_scheme"
                    android:host="@string/host_marketplace"
                    android:pathPattern="@string/path_mitra_toppers_dashboard"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".preapprove.view.activity.MitraToppersPreApproveWebViewActivity"
            android:label="@string/title_mitra_toppers"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White"/>
    </application>

</manifest>
