<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.insurance">

    <application android:label="@string/app_name">
        <activity
            android:name="com.tokopedia.insurance.InsuranceInfoActivity"
            android:exported="false"
            android:theme="@style/Theme.AppCompat.DayNight.Translucent">
            <intent-filter>
                <action android:name="com.tokopedia.internal.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="@string/host_fintech"
                    android:path="@string/path_insurance_info"
                    android:scheme="@string/internal_scheme" />
            </intent-filter>
        </activity>
    </application>

</manifest>