<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/il_main_content"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.tokopedia.header.HeaderUnify
        android:id="@+id/saldo_deposit_toolbar"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:elevation="@dimen/unify_space_4"
        app:contentInsetLeft="0dp"
        app:contentInsetStart="0dp"
        app:contentInsetStartWithNavigation="0dp"
        app:elevation="@dimen/unify_space_4"
        app:unify_header_is_show_back_button="true"
        app:unify_header_title="@string/title_saldo_detail"
        />

    <FrameLayout
        android:id="@+id/saldo_deposit_parent_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/saldo_deposit_toolbar" />


</RelativeLayout>
