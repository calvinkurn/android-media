query DepositActivityQuery($page: Int!, $maxRows: Int!, $dateFrom: String!, $dateTo: String!) {
    depositActivity(page: $page, maxRows: $maxRows, dateFrom: $dateFrom, dateTo: $dateTo) {
    message
    have_error
    have_next_page
    deposit_history_list {
          note
          amount
          class
          image
          create_time
          __typename
    }
    __typename
    }
}