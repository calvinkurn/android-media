<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="9dp"
        android:height="2dp"
        android:top="5dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="#77db80"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="16dp"
        android:height="2dp"
        android:left="4.5dp"
        android:top="3dp">
        <rotate
            android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="#42b549"/>
            </shape>
        </rotate>
    </item>
</layer-list>
