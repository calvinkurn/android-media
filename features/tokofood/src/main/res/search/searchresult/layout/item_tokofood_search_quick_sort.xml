<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="16dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_item_tokofood_search_quick_sort"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:maxLines="1"
        android:ellipsize="end"
        android:layout_marginEnd="16dp"
        android:textColor="@color/Unify_NN950"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/radio_item_tokofood_search_quick_sort"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toTopOf="@id/divider_item_tokofood_search_quick_sort"
        app:typographyType="display_2"
        app:typographyWeight="bold"
        tools:text="Paling Sesuai"/>

    <com.tokopedia.unifycomponents.selectioncontrol.RadioButtonUnify
        android:id="@+id/radio_item_tokofood_search_quick_sort"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/tv_item_tokofood_search_quick_sort"
        app:layout_constraintBottom_toBottomOf="@id/tv_item_tokofood_search_quick_sort"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.tokopedia.unifycomponents.DividerUnify
        android:id="@+id/divider_item_tokofood_search_quick_sort"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/tv_item_tokofood_search_quick_sort"
        app:layout_constraintBottom_toBottomOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>