    id
    slugName
    name
    location
    address
    phone
    email
    logoUrl
    permissionOfHajj
    permissionOfUmrah
    pilgrimsPerYear
    establishedSince
    description
    products{
        id
        title
        banner
        slugName
        originalPrice
        slashPrice
        downPaymentPrice
        durationDays
        departureDate
        returningDate
    }
    ui {
        establishedSince
        pilgrimsPerYear
    }
  }
}