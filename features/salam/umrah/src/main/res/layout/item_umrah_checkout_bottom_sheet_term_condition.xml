<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tg_umrah_checkout_title_term"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl3"
        app:typographyType="body_2"
        app:typographyWeight="bold"
        tools:text="Keamanan"
        android:visibility="gone"
        android:textColor="@color/Neutral_N700"
        />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tg_umrah_checkout_desc_term"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/dimen_dp_18"
        app:typographyType="body_3"
        app:typographyWeight="reguler"
        tools:text="Keamanan"
        android:visibility="gone"
        android:textColor="@color/Neutral_N700"
        />

</LinearLayout>