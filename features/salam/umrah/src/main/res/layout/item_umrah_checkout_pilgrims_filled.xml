<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:foreground="?attr/selectableItemBackgroundBorderless"
    android:clickable="true"
    android:focusable="true"
    android:id="@+id/container_widget_umrah_checkout_green_rect"
    android:background="@drawable/umrah_bg_order_checkout"
    android:layout_marginTop="@dimen/unify_space_12"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:targetApi="m"
    tools:ignore="UnusedAttribute">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tg_widget_umrah_checkout_green_rect_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dimen_dp_1"
        android:textColor="@color/Green_G500"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_chainStyle="packed"
        app:typographyType="heading_5"
        app:typographyWeight="bold"
        tools:text="Patuna Tour Travel"
        android:layout_marginTop="@dimen/spacing_lvl3"
        android:layout_marginLeft="@dimen/unify_space_12"
        android:layout_marginStart="@dimen/unify_space_12" />

    <androidx.appcompat.widget.AppCompatImageView
        android:visibility="gone"
        android:id="@+id/iv_widget_umrah_checkout_green_rect_first_line"
        android:layout_width="@dimen/spacing_lvl4"
        android:layout_height="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl2"
        app:layout_constraintTop_toBottomOf="@+id/tg_widget_umrah_checkout_green_rect_title"
        app:layout_constraintLeft_toLeftOf="@+id/tg_widget_umrah_checkout_green_rect_title"
        android:src="@color/Blue_B300"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tg_widget_umrah_checkout_green_rect_first_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toRightOf="@+id/iv_widget_umrah_checkout_green_rect_first_line"
        app:layout_constraintTop_toTopOf="@+id/iv_widget_umrah_checkout_green_rect_first_line"
        tools:text="1 Jamaah"
        app:typographyWeight="reguler"
        app:typographyType="body_3"
        android:textColor="@color/Neutral_N700_68"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:visibility="gone"
        android:id="@+id/iv_widget_umrah_checkout_green_rect_second_line"
        android:layout_width="@dimen/spacing_lvl4"
        android:layout_height="@dimen/spacing_lvl4"
        app:layout_constraintTop_toBottomOf="@+id/tg_widget_umrah_checkout_green_rect_first_line"
        app:layout_constraintLeft_toLeftOf="@+id/iv_widget_umrah_checkout_green_rect_first_line"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@color/Blue_B300"
        android:layout_marginBottom="@dimen/unify_space_12"/>

    <com.tokopedia.unifyprinciples.Typography
        android:visibility="gone"
        android:id="@+id/tg_widget_umrah_checkout_green_rect_second_line"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="@+id/tg_widget_umrah_checkout_green_rect_first_line"
        app:layout_constraintTop_toBottomOf="@+id/tg_widget_umrah_checkout_green_rect_first_line"
        tools:text="1 Jamaah"
        android:layout_marginBottom="@dimen/unify_space_12"
        app:typographyWeight="reguler"
        app:typographyType="body_3" />

    <View
        android:layout_width="wrap_content"
        android:layout_height="@dimen/unify_space_12"
        app:layout_constraintTop_toBottomOf="@+id/tg_widget_umrah_checkout_green_rect_first_line"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/iv_widget_umrah_checkout_green_rect_action"
        android:layout_width="@dimen/spacing_lvl5"
        android:layout_height="@dimen/spacing_lvl5"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginEnd="@dimen/unify_space_12"
        android:layout_marginRight="@dimen/unify_space_12"
        android:src="@color/Blue_B300" />

</androidx.constraintlayout.widget.ConstraintLayout>