<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.tokopedia.salam.umrah.common.presentation.widget.UmrahItemWidget
        android:id="@+id/iw_umrah_travel_agent"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_16"
        android:layout_marginLeft="@dimen/dp_16"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
         />
    <com.tokopedia.unifycomponents.UnifyButton
        android:id="@+id/btn_umrah_travel_contact"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/dp_16"
        android:layout_marginRight="@dimen/dp_16"
        app:unifyButtonSize="small"
        app:layout_constraintTop_toBottomOf="@+id/iw_umrah_travel_agent"
        android:layout_marginTop="@dimen/dp_16"
        app:unifyButtonType="alternate"
        android:text="@string/umrah_travel_contact"
        app:unifyButtonVariant="ghost"
        />


</androidx.constraintlayout.widget.ConstraintLayout>