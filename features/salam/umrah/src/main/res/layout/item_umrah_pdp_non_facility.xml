<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <androidx.appcompat.widget.AppCompatImageView
        android:id="@+id/item_umrah_pdp_non_facilities_logo"
        app:layout_constraintTop_toTopOf="@+id/item_umrah_pdp_non_facilities_title"
        app:layout_constraintLeft_toLeftOf="parent"
        app:srcCompat="@drawable/umrah_ic_pdp_substract"
        android:layout_width="@dimen/dp_12"
        android:layout_height="@dimen/dp_12"
        android:layout_marginTop="@dimen/dp_3"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/item_umrah_pdp_non_facilities_title"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/item_umrah_pdp_non_facilities_logo"
        app:layout_constraintRight_toRightOf="parent"
        app:typographyWeight="reguler"
        app:typographyType="body_3"
        android:textColor="@color/Neutral_N700_96"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        tools:text="Paspor"
        android:layout_marginLeft="@dimen/dp_6"
        android:layout_marginStart="@dimen/dp_6" />
</androidx.constraintlayout.widget.ConstraintLayout>