<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/telco_input_number_result_layout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/layout_lvl1"
    android:visibility="gone">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/telco_phone_number_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:lines="1"
            android:textColor="@color/Neutral_N700_96"
            android:layout_marginRight="@dimen/layout_lvl1"
            app:typographyType="heading_4"
            tools:text="08123223232" />

        <ImageView
            android:id="@+id/telco_img_operator_result"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_25"
            tools:ignore="contentDescription" />

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>