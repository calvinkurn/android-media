<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/input_number_layout_result"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="@dimen/fontSize_lvl2"
    android:visibility="gone">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/phone_number_result"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:lines="1"
        android:textColor="@color/Neutral_N700_96"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="heading_4"
        tools:text="08123223232" />

    <ImageView
        android:id="@+id/img_operator_result"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_25"
        android:layout_gravity="center_vertical"
        android:layout_toRightOf="@+id/title_enquiry_result"
        app:layout_constraintLeft_toLeftOf="@+id/title_enquiry_result"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="contentDescription" />

</androidx.constraintlayout.widget.ConstraintLayout>