query catalogMenuDetail($menuId: Int!) {
  rechargeCatalogMenuDetail(menuID: $menuId, platformID: 7) {
    menu_name
    menu_label
    category_ids
    catalog {
      id
      name
      label
      icon
      sub_menu {
        id
        name
        label
      }
    }
    tickers {
      ID
      Name
      Content
      Type
    }
    onboarding_descriptions
  }
}