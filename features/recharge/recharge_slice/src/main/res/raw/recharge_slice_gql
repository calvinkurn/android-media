query rechargeFavoriteRecommendationList($device_id: Int!) {
	rechargeFavoriteRecommendationList(device_id: $device_id) {
		title
		recommendations{
		    productId
			iconUrl
			title
			clientNumber
			appLink
			webLink
			position
			categoryName
			categoryId
			productName
			type
			productPrice
		}
	}
}