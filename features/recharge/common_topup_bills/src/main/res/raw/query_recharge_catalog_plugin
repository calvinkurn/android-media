query rechargeCatalogPlugin($filters: [RechargeCatalogPluginFilter]) {
  rechargeCatalogPlugin(filters: $filters){
    add_to_mybills {
      is_enabled
      attributes {
        client_numbers {
          number
          status
        }
      }
    }
    instant_checkout {
      is_enabled
      attributes {
        text
      }
    }
    buy_button {
      is_enabled
      attributes {
        text
      }
    }
  }
}