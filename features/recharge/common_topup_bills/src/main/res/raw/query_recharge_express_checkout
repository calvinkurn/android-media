mutation rechargeExpressCheckout($cart: RechargeInputExCheckout!) {
  rechargeExpressCheckout(cart: $cart) {
    data {
      redirect_url
      callback_url_success
      callback_url_failed
      thanks_url
      transaction_id
      query_string
      need_otp
    }
    errors {
      status
      title
    }
  }
}