<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container_digital_search_number"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToPadding="false"
    android:orientation="vertical">

    <com.tokopedia.header.HeaderUnify android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?actionBarSize"
        android:theme="?toolbarTheme"
        app:contentInsetLeft="@dimen/layout_lvl0"
        app:contentInsetStart="@dimen/layout_lvl0"
        app:contentInsetStartWithNavigation="@dimen/layout_lvl0"
        app:subtitleTextAppearance="?subtitleTextAppearance"
        app:titleTextAppearance="?titleTextAppearance"
        xmlns:app="http://schemas.android.com/apk/res-auto" />

    <FrameLayout
        android:id="@+id/parent_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/digital_search_number_toolbar_seperator_top_margin" />

</LinearLayout>