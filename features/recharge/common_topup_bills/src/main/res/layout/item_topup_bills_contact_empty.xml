<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.unifycomponents.ImageUnify
        android:id="@+id/common_topup_bills_contact_empty_img"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/common_topup_ic_contact_empty"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.4" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/common_topup_bills_contact_empty_title"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/common_topup_bills_contact_empty_img"
        app:layout_constraintStart_toStartOf="@id/common_topup_bills_contact_empty_img"
        app:layout_constraintEnd_toEndOf="@id/common_topup_bills_contact_empty_img"
        app:typographyType="heading_2"
        android:gravity="center"
        android:text="Hmm, belum ada kontak di HP kamu" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/common_topup_bills_contact_empty_desc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/common_topup_bills_contact_empty_title"
        app:layout_constraintStart_toStartOf="@id/common_topup_bills_contact_empty_img"
        app:layout_constraintEnd_toEndOf="@id/common_topup_bills_contact_empty_img"
        app:typographyType="body_2"
        android:gravity="center"
        android:text="Cek lagi di sini kalau sudah ada kontak tersimpan di HP kamu, ya."/>

</androidx.constraintlayout.widget.ConstraintLayout>