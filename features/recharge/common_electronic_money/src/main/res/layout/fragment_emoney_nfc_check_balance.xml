<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Unify_N0">

    <com.tokopedia.common_electronic_money.compoundview.ETollUpdateBalanceResultView
        android:id="@+id/view_update_balance_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="top"
        android:visibility="gone" />

    <com.tokopedia.common_electronic_money.compoundview.NFCDisabledView
        android:id="@+id/view_nfc_disabled"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="gone" />

    <com.tokopedia.common_electronic_money.compoundview.TapETollCardView
        android:id="@+id/view_tap_emoney_card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:visibility="gone" />

</FrameLayout>