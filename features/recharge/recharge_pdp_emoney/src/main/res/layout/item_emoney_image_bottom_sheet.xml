<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.unifycomponents.ImageUnify
        android:layout_marginTop="12dp"
        android:layout_marginLeft="12dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        android:id="@+id/img_bottom_sheet_emoney"
        android:layout_width="wrap_content"
        android:layout_height="24dp"
        />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tg_bottom_sheet_emoney"
        android:layout_width="0dp"
        app:layout_constraintLeft_toRightOf="@id/img_bottom_sheet_emoney"
        app:layout_constraintTop_toTopOf="@+id/img_bottom_sheet_emoney"
        app:layout_constraintBottom_toBottomOf="@+id/img_bottom_sheet_emoney"
        android:layout_marginLeft="12dp"
        android:layout_height="wrap_content"
        app:typographyType="display_3"
        app:typographyWeight="reguler"
        tools:text="Emoney" />

    <View
        android:id="@+id/view_separator"
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        app:layout_constraintTop_toBottomOf="@id/img_bottom_sheet_emoney"
        android:layout_marginTop="12dp"
        android:background="@color/Unify_NN50"
        />

</androidx.constraintlayout.widget.ConstraintLayout>