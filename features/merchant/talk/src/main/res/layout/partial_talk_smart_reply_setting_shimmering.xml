<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/Unify_Background"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/talk_smart_reply_setting_title"
        android:layout_width="match_parent"
        android:layout_height="12dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/talk_smart_reply_setting_subtitle"
        android:layout_width="@dimen/dp_168"
        android:layout_height="12dp"
        android:layout_marginTop="8dp"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"
        app:layout_constraintStart_toStartOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_title" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/talk_smart_reply_setting_subtitle_2"
        android:layout_width="96dp"
        android:layout_height="12dp"
        android:layout_marginTop="8dp"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"
        app:layout_constraintStart_toStartOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_subtitle" />

    <com.tokopedia.unifycomponents.DividerUnify
        android:id="@+id/talk_smart_reply_setting_divider"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_1"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintStart_toStartOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_subtitle_2" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/talk_smart_reply_setting_subtitle_3"
        android:layout_width="@dimen/dp_204"
        android:layout_height="12dp"
        android:layout_marginTop="16dp"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"
        app:layout_constraintStart_toStartOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_divider" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/talk_smart_reply_setting_subtitle_4"
        android:layout_width="96dp"
        android:layout_height="12dp"
        android:layout_marginTop="8dp"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"
        app:layout_constraintStart_toStartOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_subtitle_3" />

    <com.tokopedia.unifycomponents.CardUnify
        android:id="@+id/talk_smart_reply_setting_card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:card_type="border"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_subtitle_4">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="16dp">

            <com.tokopedia.unifycomponents.LoaderUnify
                android:id="@+id/talk_smart_reply_setting_image"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="12dp"
                app:loader_corner_radius="4dp"
                app:loader_type="rect"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <com.tokopedia.unifycomponents.LoaderUnify
                android:id="@+id/talk_smart_reply_setting_info"
                android:layout_width="0dp"
                android:layout_height="12dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="12dp"
                app:loader_corner_radius="4dp"
                app:loader_type="rect"
                app:layout_constraintBottom_toBottomOf="@id/talk_smart_reply_setting_image"
                app:layout_constraintEnd_toStartOf="@id/talk_smart_reply_setting_amount"
                app:layout_constraintStart_toEndOf="@id/talk_smart_reply_setting_image"
                app:layout_constraintTop_toTopOf="@id/talk_smart_reply_setting_image" />

            <com.tokopedia.unifycomponents.LoaderUnify
                android:id="@+id/talk_smart_reply_setting_amount"
                android:layout_width="40dp"
                android:layout_height="12dp"
                android:layout_marginEnd="12dp"
                app:loader_corner_radius="4dp"
                app:loader_type="rect"
                app:layout_constraintBottom_toBottomOf="@id/talk_smart_reply_setting_image"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/talk_smart_reply_setting_image" />

            <com.tokopedia.unifycomponents.DividerUnify
                android:id="@+id/talk_smart_reply_card_item_divider"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_image" />

            <com.tokopedia.unifycomponents.LoaderUnify
                android:id="@+id/talk_smart_reply_setting_image_2"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="12dp"
                app:loader_corner_radius="4dp"
                app:loader_type="rect"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_card_item_divider" />

            <com.tokopedia.unifycomponents.LoaderUnify
                android:id="@+id/talk_smart_reply_setting_info_2"
                android:layout_width="0dp"
                android:layout_height="12dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="12dp"
                app:loader_corner_radius="4dp"
                app:loader_type="rect"
                app:layout_constraintBottom_toBottomOf="@id/talk_smart_reply_setting_image_2"
                app:layout_constraintEnd_toStartOf="@id/talk_smart_reply_setting_amount_2"
                app:layout_constraintStart_toEndOf="@id/talk_smart_reply_setting_image_2"
                app:layout_constraintTop_toTopOf="@id/talk_smart_reply_setting_image_2" />

            <com.tokopedia.unifycomponents.LoaderUnify
                android:id="@+id/talk_smart_reply_setting_amount_2"
                android:layout_width="40dp"
                android:layout_height="12dp"
                android:layout_marginEnd="12dp"
                app:loader_corner_radius="4dp"
                app:loader_type="rect"
                app:layout_constraintBottom_toBottomOf="@id/talk_smart_reply_setting_image_2"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/talk_smart_reply_setting_image_2" />

            <com.tokopedia.unifycomponents.DividerUnify
                android:id="@+id/talk_smart_reply_card_item_divider_2"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginTop="12dp"
                app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_image_2"  />

            <com.tokopedia.unifycomponents.LoaderUnify
                android:id="@+id/talk_smart_reply_setting_image_3"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:layout_marginStart="12dp"
                android:layout_marginTop="8dp"
                app:loader_corner_radius="4dp"
                app:loader_type="rect"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_card_item_divider_2" />

            <com.tokopedia.unifycomponents.LoaderUnify
                android:id="@+id/talk_smart_reply_setting_info_3"
                android:layout_width="0dp"
                android:layout_height="12dp"
                android:layout_marginStart="8dp"
                android:layout_marginEnd="12dp"
                app:loader_corner_radius="4dp"
                app:loader_type="rect"
                app:layout_constraintBottom_toBottomOf="@id/talk_smart_reply_setting_image_3"
                app:layout_constraintEnd_toStartOf="@id/talk_smart_reply_setting_amount_3"
                app:layout_constraintStart_toEndOf="@id/talk_smart_reply_setting_image_3"
                app:layout_constraintTop_toTopOf="@id/talk_smart_reply_setting_image_3" />

            <com.tokopedia.unifycomponents.LoaderUnify
                android:id="@+id/talk_smart_reply_setting_amount_3"
                android:layout_width="40dp"
                android:layout_height="12dp"
                android:layout_marginEnd="12dp"
                app:loader_corner_radius="4dp"
                app:loader_type="rect"
                app:layout_constraintBottom_toBottomOf="@id/talk_smart_reply_setting_image_3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="@id/talk_smart_reply_setting_image_3" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.tokopedia.unifycomponents.CardUnify>

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/talk_smart_reply_setting_subtitle_5"
        android:layout_width="@dimen/dp_204"
        android:layout_height="12dp"
        android:layout_marginTop="16dp"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"
        app:layout_constraintStart_toStartOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_card" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/talk_smart_reply_setting_subtitle_6"
        android:layout_width="64dp"
        android:layout_height="12dp"
        android:layout_marginTop="8dp"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"
        app:layout_constraintStart_toStartOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_subtitle_5" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/talk_smart_reply_setting_subtitle_7"
        android:layout_width="@dimen/dp_234"
        android:layout_height="12dp"
        android:layout_marginTop="16dp"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"
        app:layout_constraintStart_toStartOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_subtitle_6" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/talk_smart_reply_setting_subtitle_8"
        android:layout_width="@dimen/dp_168"
        android:layout_height="12dp"
        android:layout_marginTop="8dp"
        app:loader_corner_radius="4dp"
        app:loader_type="rect"
        app:layout_constraintStart_toStartOf="@id/talk_smart_reply_setting_title"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_subtitle_7" />

    <com.tokopedia.unifycomponents.DividerUnify
        android:id="@+id/talk_smart_reply_setting_divider_2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_1"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/talk_smart_reply_setting_subtitle_8" />

</androidx.constraintlayout.widget.ConstraintLayout>