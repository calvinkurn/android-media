<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/containerNewSellerParameterRelief"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/icProtectionCheck"
        android:layout_width="@dimen/layout_lvl3"
        android:layout_height="@dimen/layout_lvl3"
        android:layout_marginTop="16dp"
        android:layout_marginStart="12dp"
        app:icon_name="PROTECTION_CHECK"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvTitleParameterRelief"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        android:layout_marginStart="12dp"
        tools:text="@string/title_parameter_relief_new_seller"
        app:layout_constraintBottom_toBottomOf="@id/icProtectionCheck"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/icProtectionCheck"
        app:layout_constraintTop_toTopOf="@id/icProtectionCheck"
        app:typographyType="heading_5" />

    <com.tokopedia.unifycomponents.CardUnify
        android:id="@+id/cardParameterRelief"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:card_type="clear"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:layout_marginBottom="@dimen/spacing_lvl4"
        android:layout_marginStart="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl3"
        app:cardCornerRadius="@dimen/spacing_lvl3"
        app:cardElevation="@dimen/layout_lvl0"
        app:card_background_color="@color/Unify_N50"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/icProtectionCheck"
        app:layout_constraintTop_toBottomOf="@id/tvTitleParameterRelief">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.recyclerview.widget.RecyclerView
                android:id="@+id/rvParameterReliefDetail"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_lvl2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:itemCount="2"
                tools:listitem="@layout/item_parameter_protected_detail" />

            <com.tokopedia.unifycomponents.CardUnify
                android:id="@+id/cardDescParameterRelief"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                app:card_type="clear"
                android:layout_marginTop="12dp"
                android:layout_marginEnd="12dp"
                android:layout_marginStart="@dimen/spacing_lvl2"
                android:layout_marginBottom="12dp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toBottomOf="@id/rvParameterReliefDetail">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <com.tokopedia.unifyprinciples.Typography
                        android:id="@+id/tvDescParameterRelief"
                        android:layout_width="@dimen/layout_lvl0"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/spacing_lvl3"
                        android:layout_marginTop="@dimen/spacing_lvl3"
                        android:layout_marginBottom="@dimen/spacing_lvl3"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toStartOf="@id/icDescParameterRelief"
                        app:typographyType="body_3"
                        tools:text="@string/desc_new_seller_parameter_relief" />

                    <com.tokopedia.iconunify.IconUnify
                        android:id="@+id/icDescParameterRelief"
                        android:layout_width="@dimen/layout_lvl2"
                        android:layout_height="@dimen/layout_lvl2"
                        app:icon_name="CHEVRON_RIGHT"
                        android:layout_marginStart="@dimen/spacing_lvl3"
                        android:layout_marginEnd="@dimen/spacing_lvl3"
                        app:layout_constraintStart_toEndOf="@id/tvDescParameterRelief"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@id/tvDescParameterRelief"
                        app:layout_constraintBottom_toBottomOf="@id/tvDescParameterRelief"/>

                </androidx.constraintlayout.widget.ConstraintLayout>

            </com.tokopedia.unifycomponents.CardUnify>

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.tokopedia.unifycomponents.CardUnify>


</androidx.constraintlayout.widget.ConstraintLayout>