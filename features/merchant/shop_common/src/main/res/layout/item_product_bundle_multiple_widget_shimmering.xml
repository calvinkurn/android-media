<?xml version="1.0" encoding="utf-8"?>
<com.tokopedia.unifycomponents.CardUnify android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="-2dp"
    android:layout_marginEnd="-2dp"
    app:layout_constraintTop_toTopOf="parent"
    app:layout_constraintStart_toStartOf="parent"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/layout_shimmer_container"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_1"
            android:layout_width="24dp"
            android:layout_height="24dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_2"
            android:layout_width="0dp"
            android:layout_height="20dp"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="7dp"
            app:layout_constraintBottom_toBottomOf="@+id/shimmer_1"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/shimmer_1"
            app:layout_constraintTop_toTopOf="@+id/shimmer_1"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_3"
            android:layout_width="141dp"
            android:layout_height="16dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="12dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/shimmer_1"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_"
            android:layout_width="169dp"
            android:layout_height="20dp"
            android:layout_marginStart="12dp"
            android:layout_marginTop="16dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/shimmer_6"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_4"
            android:layout_width="72dp"
            android:layout_height="72dp"
            android:layout_marginStart="24dp"
            android:layout_marginTop="28dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintEnd_toStartOf="@+id/divider2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/shimmer_3"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_5"
            android:layout_width="0dp"
            android:layout_height="12dp"
            android:layout_marginTop="4dp"
            app:layout_constraintEnd_toEndOf="@+id/shimmer_4"
            app:layout_constraintStart_toStartOf="@+id/shimmer_4"
            app:layout_constraintTop_toBottomOf="@+id/shimmer_4"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_6"
            android:layout_width="84dp"
            android:layout_height="16dp"
            android:layout_marginTop="4dp"
            app:layout_constraintEnd_toEndOf="@+id/shimmer_5"
            app:layout_constraintStart_toStartOf="@+id/shimmer_5"
            app:layout_constraintTop_toBottomOf="@+id/shimmer_5"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_7"
            android:layout_width="72dp"
            android:layout_height="0dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintBottom_toBottomOf="@+id/shimmer_4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/divider2"
            app:layout_constraintTop_toTopOf="@+id/shimmer_4"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_8"
            android:layout_width="0dp"
            android:layout_height="12dp"
            android:layout_marginTop="4dp"
            app:layout_constraintEnd_toEndOf="@+id/shimmer_7"
            app:layout_constraintStart_toStartOf="@+id/shimmer_7"
            app:layout_constraintTop_toBottomOf="@+id/shimmer_7"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_9"
            android:layout_width="84dp"
            android:layout_height="16dp"
            android:layout_marginTop="4dp"
            app:layout_constraintEnd_toEndOf="@+id/shimmer_8"
            app:layout_constraintStart_toStartOf="@+id/shimmer_8"
            app:layout_constraintTop_toBottomOf="@+id/shimmer_8"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_10"
            android:layout_width="100dp"
            android:layout_height="32dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:layout_marginBottom="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/shimmer_"
            app:loader_type="rect" />

        <com.tokopedia.unifycomponents.LoaderUnify
            android:id="@+id/shimmer_11"
            android:layout_width="0dp"
            android:layout_height="22dp"
            android:layout_marginStart="12dp"
            android:layout_marginEnd="20dp"
            app:layout_constraintBottom_toBottomOf="@+id/shimmer_10"
            app:layout_constraintEnd_toStartOf="@+id/shimmer_10"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@+id/shimmer_10"
            app:loader_type="rect" />

        <View
            android:id="@+id/divider2"
            android:layout_width="1dp"
            android:layout_height="287dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.tokopedia.unifycomponents.CardUnify>