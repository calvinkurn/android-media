mutation updateShopInfo($name:String, $domain:String, $tagline:String, $description:String, $imgId:String){
  updateShopInfo(input: {
      name: $name,
      domain: $domain,
      tagline: $tagline,
      description: $description,
      logo: {
        imgID: $imgId
      }
    }) {
      success
      message
    }
}
