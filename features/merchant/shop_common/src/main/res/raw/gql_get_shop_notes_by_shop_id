query getShopNotesByShopId($shopId: String!, $id: String, $isTerm: Boolean, $slug: String){
    shopNotesByShopID(shopID: $shopId, id: $id, isTerms: $isTerm: slug: $slug){
        result{
          id
          title
          content
          isTerms
          updateTime
          position
          url
        }
        error {
          message
        }
    }
}