query goldGetPMOSStatus {
  goldGetPMOSStatus(shopID: 480256, includeOS: true) {
    header {
      process_time
      messages
      reason
      error_code
    }
    data {
      shopID
      power_merchant{
        status
        auto_extend{
          status
          tkpd_product_id
        }
        expired_time
        shop_popup
      }
      official_store{
        status
        error
      }
    }
  }
}