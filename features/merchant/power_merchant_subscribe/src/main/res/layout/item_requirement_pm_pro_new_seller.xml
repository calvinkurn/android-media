<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="20dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.unifycomponents.ImageUnify
        android:id="@+id/ivRequirementPmProNewSeller"
        android:layout_width="@dimen/layout_lvl6"
        android:layout_height="@dimen/layout_lvl6"
        android:adjustViewBounds="true"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvRequirementPmProNewSeller"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        app:typographyType="body_2"
        android:textColor="@color/Unify_N700_96"
        tools:text="@string/title_requirement_pm_pro_new_seller_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/ivRequirementPmProNewSeller"
        app:layout_constraintTop_toTopOf="@id/ivRequirementPmProNewSeller"
        app:layout_constraintBottom_toBottomOf="@id/ivRequirementPmProNewSeller"/>

</androidx.constraintlayout.widget.ConstraintLayout>