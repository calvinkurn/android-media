<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/text_fee_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/unify_space_16"
        android:layout_marginTop="@dimen/unify_space_16"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/text_label_new"
        android:textColor="@color/Unify_NN950"
        app:typographyWeight="bold"
        android:text="@string/pm_fee_service"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/text_label_new"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_rounded_corners_rn_500"
        android:layout_marginStart="@dimen/unify_space_8"
        app:layout_constraintTop_toTopOf="@id/text_fee_service"
        app:layout_constraintStart_toEndOf="@id/text_fee_service"
        app:layout_constraintBottom_toBottomOf="@id/text_fee_service"
        android:textColor="@color/Unify_NN0"
        app:typographyWeight="bold"
        android:padding="@dimen/unify_space_4"
        android:textSize="@dimen/unify_font_10"
        android:text="Baru"/>
    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unify_space_2"
        android:background="@color/Unify_NN50"
        android:layout_marginTop="@dimen/unify_space_16"
        app:layout_constraintTop_toBottomOf="@id/text_fee_service"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/text_learn_fee"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:padding="@dimen/unify_space_16"
        android:textColor="@color/Unify_NN600"
        app:typographyWeight="reguler"
        android:text="@string/pm_learn_fee_service"
        app:layout_constraintTop_toBottomOf="@id/divider"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/chevron"/>

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/chevron"
        android:layout_width="@dimen/unify_space_24"
        android:layout_height="@dimen/unify_space_24"
        app:icon_color_light_disabled="@color/Unify_NN900"
        app:icon_color_light_enable="@color/Unify_NN900"
        app:icon_enable_state="true"
        app:icon_name="CHEVRON_RIGHT"
        android:layout_marginEnd="@dimen/unify_space_16"
        app:layout_constraintBottom_toBottomOf="@id/text_learn_fee"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/text_learn_fee" />

</androidx.constraintlayout.widget.ConstraintLayout>