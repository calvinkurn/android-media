<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Unify_N0">

    <com.tokopedia.abstraction.base.view.widget.SwipeToRefresh
        android:id="@+id/image_review_gallery_swipe_refresh_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.tokopedia.gallery.customview.ImageReviewRecyclerView
            android:id="@+id/image_review_gallery_recycler_view"
            android:paddingLeft="@dimen/review_gallery_default_padding"
            android:paddingRight="@dimen/review_gallery_default_padding"
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>

    </com.tokopedia.abstraction.base.view.widget.SwipeToRefresh>


</RelativeLayout>