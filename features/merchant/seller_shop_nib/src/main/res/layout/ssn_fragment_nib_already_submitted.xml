<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:layout_height="match_parent">

    <com.tokopedia.empty_state.EmptyStateUnify
        android:id="@+id/emptyState"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        app:unifyEmptyStateOrientation="vertical"
        app:unifyEmptyStatePrimaryCTAText="@string/ssn_back_to_home"
        app:unifyEmptyStateSecondaryCTAText="@string/ssn_call_tokopedia_care"
        app:unifyEmptyStateTitle="@string/ssn_already_submitted_title"
        app:unifyEmptyStateDescription="@string/ssn_already_submitted_description" />

</LinearLayout>