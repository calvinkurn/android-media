        android:id="@+id/barrier_rating_one_end"
        android:layout_width="1dp"
        android:layout_height="1dp"
        app:barrierDirection="end"
        app:constraint_referenced_ids="ic_review_detail_rating_one_inactive,ic_review_detail_rating_one_active"
