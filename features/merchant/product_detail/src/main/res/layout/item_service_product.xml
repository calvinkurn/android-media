<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/dp_12"
    android:paddingBottom="@dimen/dp_12">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/rate_label_title"
        android:layout_width="@dimen/dp_0"
        android:layout_height="wrap_content"
        android:textColor="@color/Unify_N700_96"
        app:layout_constraintEnd_toStartOf="@+id/rate_label_value"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="body_2"
        tools:text="JNE" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/rate_label_value"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="@color/Unify_N700_68"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="body_2"
        tools:text="Rp.20.000" />

</androidx.constraintlayout.widget.ConstraintLayout>