<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="16dp"
    android:paddingEnd="16dp"
    android:layout_marginTop="28dp">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tgpCategory"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:textColor="@color/Unify_NN950"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="13dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tgpQuantity"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="paragraph_3"
        tools:text="Suplemen Diet, Susu, Essential Oil, Kesehatan Wanita,Vitamin Multivitamin, Tes Kehamilan dan Masa Subur, Obat - Obatan, Perlengkapan Medis, Produk Dewasa, Tulang Otot Sendi, Masker" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tgpQuantity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:textColor="@color/Unify_NN600"
        app:layout_constraintBottom_toBottomOf="@+id/tgpCategory"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tgpCategory"
        app:typographyType="paragraph_2"
        app:typographyWeight="bold"
        tools:text="0/10 Produk" />



</androidx.constraintlayout.widget.ConstraintLayout>