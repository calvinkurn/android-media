<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.inbox_reputation">

    <application
        android:allowBackup="true"
        android:supportsRtl="true">
        <activity
            android:name="com.tokopedia.inboxreputation.presentation.activity.InboxReputationActivity"
            android:label="@string/title_activity_reputation_review"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White"
            android:windowSoftInputMode="stateHidden">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="@string/host_marketplace"
                    android:pathPattern="@string/path_review_inbox"
                    android:scheme="@string/internal_scheme" />
            </intent-filter>
        </activity>
    </application>
</manifest>