<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tool="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingStart="16dp"
    android:paddingEnd="0dp">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tfText"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:textColor="@color/Unify_NN950"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="display_2"
        app:typographyWeight="bold"
        tool:text="Text content title" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tfSubTextQuota"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:textColor="@color/Unify_NN600"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tfText"
        app:typographyType="paragraph_3"
        tool:text="subtext" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tfSubTextProgram"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="16dp"
        android:layout_marginStart="8dp"
        android:singleLine="true"
        android:ellipsize="end"
        android:textColor="@color/Unify_NN600"
        app:layout_constraintEnd_toStartOf="@+id/iconMore"
        app:layout_constraintStart_toEndOf="@+id/tfSubTextQuota"
        app:layout_constraintTop_toBottomOf="@+id/tfText"
        app:typographyType="paragraph_3"
        tool:text="|  Terdaftar di WIB" />

    <com.tokopedia.unifycomponents.DividerUnify
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/tfSubTextQuota" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/iconMore"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginEnd="16dp"
        app:icon_color_light_enable="@color/Unify_NN950"
        app:icon_name="MENU_KEBAB_VERTICAL"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tfText"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>