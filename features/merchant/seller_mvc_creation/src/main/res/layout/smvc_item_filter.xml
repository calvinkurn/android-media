<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tpgFilterName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Jakarta" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/iconCheckmarkState"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_margin="22dp"
        app:icon_color_light_enable="@color/Unify_GN500"
        app:icon_enable_state="true"
        app:icon_name="CHECK"
        app:layout_constraintBottom_toBottomOf="@+id/tpgFilterName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tpgFilterName" />



</androidx.constraintlayout.widget.ConstraintLayout>