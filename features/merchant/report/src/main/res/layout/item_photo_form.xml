<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/dp_32"
    android:paddingLeft="16dp"
    android:paddingStart="16dp"
    android:paddingRight="16dp"
    android:paddingEnd="16dp">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/title_upload"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:lineSpacingExtra="@dimen/sp_4"
        android:textColor="@color/Unify_N700_96"
        app:typographyType="heading_5"
        app:typographyWeight="bold"
        tools:text="Foto bukti Harga Eceran Tertinggi (0/3)"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/description_upload"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/Unify_N700_96"
        android:lineSpacingExtra="@dimen/sp_6"
        android:layout_marginTop="4dp"
        app:typographyWeight="reguler"
        app:typographyType="body_2"
        tools:text="Upload foto yang membuktikan Harga Eceran Tertinggi produk."/>

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_uploaded_foto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        tools:visibility="gone"/>

</LinearLayout>