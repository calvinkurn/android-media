<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/setting_indented_divider_thin_padding_top"
    android:paddingBottom="@dimen/setting_indented_divider_thin_padding_bottom"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.tokopedia.unifycomponents.DividerUnify
        android:id="@+id/thinIndentedDivider"
        android:layout_width="0dp"
        android:layout_height="@dimen/setting_divider_thin"
        app:layout_constraintWidth_percent="0.85"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>