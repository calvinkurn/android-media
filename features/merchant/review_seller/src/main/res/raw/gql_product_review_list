query get_product_review_list($sortBy: String!, $filterBy: String, $limit: Int!, $page:Int!) {
	productrevShopRatingAggregate(sortBy: $sortBy, filterBy: $filterBy, limit: $limit, page: $page) {
    data {
        product {
            productID
            productName
            productImageURL
        }
        rating
        reviewCount
    }
    hasNext
	}
}