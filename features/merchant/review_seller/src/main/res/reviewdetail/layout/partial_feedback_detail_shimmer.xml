<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/dp_16">

    <com.elyeproj.loaderviewlibrary.LoaderTextView
        android:id="@+id/rectangle_rating_feedback"
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_10"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginRight="@dimen/spacing_lvl4"
        app:corners="5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintWidth_percent="0.2" />

    <com.elyeproj.loaderviewlibrary.LoaderTextView
        android:id="@+id/rectangle_title_feedback"
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_10"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginRight="@dimen/spacing_lvl4"
        app:corners="5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/rectangle_rating_feedback"
        app:layout_constraintWidth_percent="0.4" />

    <com.elyeproj.loaderviewlibrary.LoaderTextView
        android:id="@+id/rectangle_user_feedback"
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_10"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginRight="@dimen/spacing_lvl4"
        app:corners="5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/rectangle_rating_feedback"
        app:layout_constraintWidth_percent="0.4" />

    <com.elyeproj.loaderviewlibrary.LoaderTextView
        android:id="@+id/rectangle_variant_feedback"
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_10"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginRight="@dimen/spacing_lvl4"
        app:corners="5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/rectangle_user_feedback"
        app:layout_constraintWidth_percent="0.9" />

    <com.elyeproj.loaderviewlibrary.LoaderTextView
        android:id="@+id/rectangle_desc_feedback"
        android:layout_width="@dimen/dp_0"
        android:layout_height="@dimen/dp_10"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginRight="@dimen/spacing_lvl4"
        app:corners="5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/rectangle_variant_feedback"
        app:layout_constraintWidth_percent="0.9" />

    <com.elyeproj.loaderviewlibrary.LoaderTextView
        android:id="@+id/rectangle_pic1_feedback"
        android:layout_width="@dimen/layout_lvl7"
        android:layout_height="@dimen/layout_lvl7"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginRight="@dimen/spacing_lvl4"
        app:corners="8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/rectangle_desc_feedback" />

    <com.elyeproj.loaderviewlibrary.LoaderTextView
        android:id="@+id/rectangle_pic2_feedback"
        android:layout_width="@dimen/layout_lvl7"
        android:layout_height="@dimen/layout_lvl7"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginRight="@dimen/spacing_lvl4"
        app:corners="8"
        app:layout_constraintStart_toEndOf="@id/rectangle_pic1_feedback"
        app:layout_constraintTop_toBottomOf="@id/rectangle_desc_feedback" />

    <com.elyeproj.loaderviewlibrary.LoaderTextView
        android:id="@+id/rectangle_pic3_feedback"
        android:layout_width="@dimen/layout_lvl7"
        android:layout_height="@dimen/layout_lvl7"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginRight="@dimen/spacing_lvl4"
        app:corners="8"
        app:layout_constraintStart_toEndOf="@id/rectangle_pic2_feedback"
        app:layout_constraintTop_toBottomOf="@id/rectangle_desc_feedback" />

    <com.elyeproj.loaderviewlibrary.LoaderTextView
        android:id="@+id/rectangle_pic4_feedback"
        android:layout_width="@dimen/layout_lvl7"
        android:layout_height="@dimen/layout_lvl7"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginLeft="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        android:layout_marginRight="@dimen/spacing_lvl4"
        app:corners="8"
        app:layout_constraintStart_toEndOf="@id/rectangle_pic3_feedback"
        app:layout_constraintTop_toBottomOf="@id/rectangle_desc_feedback" />

</androidx.constraintlayout.widget.ConstraintLayout>