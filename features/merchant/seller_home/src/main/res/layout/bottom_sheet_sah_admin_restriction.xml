<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.tokopedia.empty_state.EmptyStateUnify
        android:id="@+id/empty_state_sah_admin_restriction"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:unifyEmptyStateTitle="@string/sah_admin_restriction_title"
        app:unifyEmptyStateDescription="@string/sah_admin_restriction_desc"
        app:unifyEmptyStatePrimaryCTAText="@string/sah_admin_restriction_button"
        app:unifyEmptyStateCTAFullWidth="true"/>

</androidx.constraintlayout.widget.ConstraintLayout>