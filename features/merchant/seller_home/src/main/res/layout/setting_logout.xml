<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/logoutLayout"
    android:layout_width="match_parent"
    android:layout_height="@dimen/setting_card_height"
    android:layout_marginTop="@dimen/setting_logout_margin_top"
    android:background="?attr/selectableItemBackground">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/logoutTitle"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginStart="@dimen/setting_logout_margin_start"
        android:gravity="center_vertical"
        android:text="@string/setting_logout"
        android:textColor="@color/Unify_G500"
        app:layout_constraintStart_toStartOf="parent"
        app:typographyType="body_1"
        app:typographyWeight="reguler" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/appVersionText"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:layout_marginEnd="16dp"
        android:gravity="center_vertical"
        android:textColor="@color/Unify_N700_32"
        app:layout_constraintEnd_toEndOf="parent"
        app:typographyType="body_1"
        app:typographyWeight="reguler"
        tools:text="Versi 2.69" />

</androidx.constraintlayout.widget.ConstraintLayout>