<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:background="@drawable/bg_sah_new_other_header_pm"
    tools:layout_height="@dimen/layout_lvl7">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_sah_new_other_header_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginBottom="@dimen/sah_new_other_header_shop_name_margin_bottom"
        android:textColor="@color/Unify_N0"
        android:ellipsize="end"
        android:lines="1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:typographyType="heading_4"
        tools:text="Rockerpowershop"/>

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/ic_sah_new_other_header_name"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        app:layout_constraintStart_toEndOf="@id/tv_sah_new_other_header_name"
        app:layout_constraintEnd_toEndOf="@id/ic_sah_new_other_header_share"
        app:layout_constraintTop_toTopOf="@id/tv_sah_new_other_header_name"
        app:layout_constraintBottom_toBottomOf="@id/tv_sah_new_other_header_name"
        app:layout_constraintHorizontal_bias="0"
        app:icon_name="CHEVRON_RIGHT"
        app:icon_color_light_enable="@color/Unify_N0"/>
    
    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/ic_sah_new_other_header_share"
        android:layout_width="wrap_content"
        android:layout_height="0dp"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/tv_sah_new_other_header_name"
        app:layout_constraintBottom_toBottomOf="@id/tv_sah_new_other_header_name"
        app:icon_name="SHARE_MOBILE"
        app:icon_color_light_enable="@color/Unify_N0"/>

</androidx.constraintlayout.widget.ConstraintLayout>