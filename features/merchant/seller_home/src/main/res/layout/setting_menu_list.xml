<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/setting_menu_list_height"
    android:background="?attr/selectableItemBackground"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <ImageView
        android:id="@+id/settingMenuIcon"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:scaleType="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@id/settingMenuTitle"
        app:layout_constraintWidth_percent="0.15"
        tools:src="@drawable/unify_wishlist_avd"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/settingMenuTitle"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:gravity="center_vertical"
        android:textColor="@color/setting_black_text"
        app:typographyType="body_1"
        app:typographyWeight="reguler"
        app:layout_constraintStart_toEndOf="@id/settingMenuIcon"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>