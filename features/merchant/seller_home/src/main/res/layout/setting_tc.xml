<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/setting_card_height"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tcDivider"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:text="@string/setting_tc_divider"
        app:typographyType="body_3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/settingTC"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginRight="10dp"
        android:layout_marginStart="0dp"
        android:layout_marginEnd="10dp"
        android:gravity="center_vertical"
        android:text="@string/setting_terms_and_condition"
        android:clickable="true"
        android:focusable="true"
        app:typographyType="body_3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/tcDivider"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/settingPrivacy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginEnd="0dp"
        android:gravity="center_vertical"
        android:text="@string/setting_privacy_terms"
        android:clickable="true"
        android:focusable="true"
        app:typographyType="body_3"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/tcDivider"/>

</androidx.constraintlayout.widget.ConstraintLayout>