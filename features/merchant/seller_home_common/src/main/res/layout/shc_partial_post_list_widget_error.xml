<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/sahPostListOnErrorLayout"
    android:layout_width="match_parent"
    android:layout_height="@dimen/shc_large_widget_height"
    android:padding="16dp"
    android:visibility="gone"
    tools:visibility="visible">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvPostListTitleOnError"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textColor="@color/Unify_NN950_96"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="heading_5"
        app:typographyWeight="bold"
        tools:text="Power Merchant (Aktif)" />

    <com.tokopedia.sellerhomecommon.presentation.view.customview.WidgetErrorStateView
        android:id="@+id/shcPostListCommonErrorView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>