query getCartRedirectionV2($cartRedirectionParams: [[CartRedirectionParamsV2]]) {
  cart_redirection_v2(params: $cartRedirectionParams) {
    error_message
    status
    data {
      config_name
      available_buttons {
        text
        color
        cart_type
        show_recommendation
      }
      unavailable_buttons
    }
  }
}
