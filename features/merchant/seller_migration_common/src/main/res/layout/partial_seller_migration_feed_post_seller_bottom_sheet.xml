<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    
    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvShopPost"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        tools:text="@string/seller_migration_bottom_sheet_feed_post_shop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.unifycomponents.Label
        android:id="@+id/tvInSeller"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        tools:text="@string/seller_migration_bottom_sheet_feed_in_tokopedia_seller"
        app:labelType="general_light_green"
        android:layout_marginStart="@dimen/spacing_lvl3"
        app:layout_constraintStart_toEndOf="@id/tvShopPost"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>