<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_marginEnd="8dp"
    android:id="@+id/chip_container_domain_suggestion"
    android:background="@drawable/unify_chip_normal_small"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    tools:ignore="RtlSymmetry">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/chip_textview"
        android:background="@drawable/unify_chips_bg_img_rounded_corner"
        android:textSize="14sp"
        android:gravity="center"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="6dp"
        android:layout_marginRight="12dp"
        android:layout_marginLeft="12dp"
        android:textColor="@color/Unify_NN500"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>