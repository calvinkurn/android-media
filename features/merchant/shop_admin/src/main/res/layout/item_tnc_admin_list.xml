<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvTncNo"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        tools:text="1."
        app:typographyType="body_3"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvTncName"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:typographyType="body_3"
        tools:text="Dengan mengaktifkan fitur-fitur Power Merchant"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintStart_toEndOf="@id/tvTncNo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@id/tvTncNo"
        app:layout_constraintBottom_toBottomOf="@id/tvTncNo"/>

</androidx.constraintlayout.widget.ConstraintLayout>