<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.tokopedia.tkpd.tkpdreputation"
    xmlns:tools="http://schemas.android.com/tools"
    >

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true"
        >
        <meta-data android:name="android.support.VERSION" android:value="26.1.0"
            tools:replace="android:value" />
        <activity
            android:name=".inbox.view.activity.InboxReputationActivity"
            android:label="@string/title_activity_reputation"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden"
            android:theme="@style/Theme.White"/>
        <activity
            android:name=".inbox.view.activity.InboxReputationDetailActivity"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden"
            android:theme="@style/Theme.White"/>
        <activity
            android:name=".inbox.view.activity.InboxReputationFormActivity"
            android:label="@string/title_activity_submit_review"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="stateHidden"
            android:theme="@style/Theme.White"/>
        <activity
            android:name=".inbox.view.activity.InboxReputationFilterActivity"
            android:label="@string/title_filter"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".inbox.view.activity.ImageUploadPreviewActivity"
            android:label="@string/title_activity_image_upload"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".inbox.view.activity.InboxReputationReportActivity"
            android:label="@string/title_report"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".shopreputation.ReputationViewShop"
            android:label="@string/title_activity_reputation"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".reputationproduct.view.activity.ReputationProduct"
            android:label="@string/title_activity_reputation"
            android:screenOrientation="portrait"/>
        <activity
            android:name=".reputationproduct.view.activity.ReputationProductActivity"
            android:label="@string/title_activity_reputation"
            android:screenOrientation="portrait"/>
        <activity
            android:label="@string/title_activity_reputation"
            android:name=".review.product.view.ReviewProductActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" >
            <intent-filter android:autoVerify="true">
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:scheme="@string/internal_scheme"
                    android:host="@string/host_marketplace"
                    android:pathPattern="@string/path_product_review"/>
            </intent-filter>
        </activity>
        <activity
            android:label="@string/title_activity_reputation"
            android:name=".review.shop.view.ReviewShopInfoActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White"
            />
    </application>

</manifest>
