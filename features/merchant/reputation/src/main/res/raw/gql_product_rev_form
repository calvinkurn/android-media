query productrevGetForm($reputationId:Int!,$productId:Int!){
  productrevGetForm(reputationID:$reputationId, productID:$productId){
    reputationID
    orderID
    productData{
      productID
      productName
      productStatus
      productPageURL
      productImageURL
    }
    shopData{
      shopID
      shopOpen
    }
    userData{
      userID
      userName
      userStatus
    }
  }
}