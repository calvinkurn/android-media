<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    
    <ImageView
        android:id="@+id/reviewPendingEmptyImage"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/layout_lvl1"
        android:layout_marginStart="@dimen/layout_lvl5"
        android:layout_marginEnd="@dimen/layout_lvl5"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/reviewPendingEmptyTitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/layout_lvl1"
        app:typographyType="heading_2"
        app:typographyWeight="bold"
        app:layout_constraintTop_toBottomOf="@id/reviewPendingEmptyImage"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/reviewPendingEmptySubtitle"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_12"
        app:typographyType="body_2"
        app:typographyWeight="reguler"
        app:layout_constraintTop_toBottomOf="@id/reviewPendingEmptyTitle"/>



</androidx.constraintlayout.widget.ConstraintLayout>