<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/reviewHistoryAttachedImageBlankSpace"
        android:layout_width="@dimen/layout_lvl2"
        android:layout_height="@dimen/layout_lvl0"
        android:clickable="true"
        android:focusable="true"
        android:visibility="gone"
        app:layout_constraintTop_toTopOf="@id/reviewHistoryAttachedImage"
        app:layout_constraintBottom_toBottomOf="@id/reviewHistoryAttachedImage"
        app:layout_constraintStart_toStartOf="parent"/>

    <ImageView
        android:id="@+id/reviewHistoryAttachedImage"
        android:layout_width="@dimen/dp_56"
        android:layout_height="@dimen/dp_56"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>