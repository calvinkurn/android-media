<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/ic_review_detail_rating_one"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:icon_color_light_disabled="@color/Unify_YN300"
        app:icon_color_light_enable="@color/Unify_YN300"
        app:icon_color_night_disabled="@color/Unify_YN300"
        app:icon_color_night_enable="@color/Unify_YN300"
        app:icon_name="STAR_FILLED"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/ic_review_detail_rating_two"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:icon_color_light_disabled="@color/Unify_YN300"
        app:icon_color_light_enable="@color/Unify_YN300"
        app:icon_color_night_disabled="@color/Unify_YN300"
        app:icon_color_night_enable="@color/Unify_YN300"
        app:icon_name="STAR_FILLED"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/ic_review_detail_rating_one"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/ic_review_detail_rating_three"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:icon_color_light_disabled="@color/Unify_YN300"
        app:icon_color_light_enable="@color/Unify_YN300"
        app:icon_color_night_disabled="@color/Unify_YN300"
        app:icon_color_night_enable="@color/Unify_YN300"
        app:icon_name="STAR_FILLED"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/ic_review_detail_rating_two"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/ic_review_detail_rating_four"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:icon_color_light_disabled="@color/Unify_YN300"
        app:icon_color_light_enable="@color/Unify_YN300"
        app:icon_color_night_disabled="@color/Unify_YN300"
        app:icon_color_night_enable="@color/Unify_YN300"
        app:icon_name="STAR_FILLED"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/ic_review_detail_rating_three"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/ic_review_detail_rating_five"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:icon_color_light_disabled="@color/Unify_YN300"
        app:icon_color_light_enable="@color/Unify_YN300"
        app:icon_color_night_disabled="@color/Unify_YN300"
        app:icon_color_night_enable="@color/Unify_YN300"
        app:icon_name="STAR_FILLED"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/ic_review_detail_rating_four"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>