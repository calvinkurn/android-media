<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/ic_bulk_review_mini_action"
        android:layout_width="16dp"
        android:layout_height="16dp"
        app:icon_name="CAMERA"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_bulk_review_mini_action"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="4dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="@color/Unify_NN600"
        app:layout_constrainedWidth="true"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0"
        app:layout_constraintStart_toEndOf="@id/ic_bulk_review_mini_action"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="display_3"
        tools:text="@string/tv_bulk_review_add_attachments" />

</androidx.constraintlayout.widget.ConstraintLayout>