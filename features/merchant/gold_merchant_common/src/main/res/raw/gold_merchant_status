query goldGetPMOSStatus($shop_id: Int!, $includeOS: Boolean!) {
  goldGetPMOSStatus(shopID : $shop_id, includeOS: $includeOS) {
    header {
      process_time
      messages
      reason
      error_code
    }
    data {
      shopID
      power_merchant{
        status
        auto_extend{
          status
          tkpd_product_id
        }
        expired_time
        shop_popup
      }
      official_store{
        status
        error
      }
    }
  }
}