<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="VoucherTargetPromoCodeInfoView">
        <attr name="isChangeEnabled" format="boolean"/>
        <attr name="code" format="string"/>
    </declare-styleable>
    <declare-styleable name="VoucherTargetCardItemView">
        <attr name="isEnabled" format="boolean"/>
        <attr name="targetType" format="enum">
            <enum name="targetPublic" value="0"/>
            <enum name="targetSpecial" value="1"/>
        </attr>
        <attr name="isHavePromoCode" format="boolean"/>
        <attr name="promoCode" format="string"/>
    </declare-styleable>
    <declare-styleable name="VoucherCreateWidgetTitle">
        <attr name="title" format="string"/>
    </declare-styleable>
    <declare-styleable name="DoubleLineInfo">
        <attr name="infoTitle" format="string"/>
        <attr name="value" format="string"/>
    </declare-styleable>
</resources>