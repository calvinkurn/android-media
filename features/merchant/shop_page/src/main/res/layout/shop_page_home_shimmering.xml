        app:corners="4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/loaderImageMultipleImageColumn"
        app:layout_constraintWidth_percent="0.3" />

    <com.elyeproj.loaderviewlibrary.LoaderImageView
