<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/spacing_lvl3">

    <com.tokopedia.unifycomponents.ImageUnify
        android:id="@+id/ivTabFeedHasPost"
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_marginStart="@dimen/spacing_lvl3"
        tools:src="@drawable/ic_tab_feed_has_post_seller_migration"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvTitleTabFeedHasPost"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/dp_12"
        tools:text="@string/seller_migration_tab_feed_bottom_sheet_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/ivTabFeedHasPost"
        app:layout_constraintEnd_toEndOf="parent"/>

</androidx.constraintlayout.widget.ConstraintLayout>