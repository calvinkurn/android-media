<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/background_apps"
    android:divider="@drawable/bg_line_separator"
    android:orientation="vertical"
    android:showDividers="beginning|middle|end">

    <com.tokopedia.design.label.LabelView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/label_view_etalase"
        style="@style/LabelLayout"
        android:layout_marginTop="@dimen/dp_8"
        app:lv_title="@string/shop_info_default_produk_title"
        app:lv_title_text_style="bold" />

</LinearLayout>