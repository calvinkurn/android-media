query getShopFeaturedProduct($shopId: Int!, $userID: Int!){
    shop_featured_product(shopID: $shopId, userID: $userID){
        data{
            parent_id
            product_id
            name
            uri
            image_uri
            price
            preorder
            returnable
            wholesale
            cashback
            isWishlist
            is_rated
            original_price
            percentage_amount
            cashback_detail{
                cashback_status
                cashback_percent
                is_cashback_expired
                cashback_value
            }
            free_ongkir {
                is_active
                img_url
            }
            label_groups {
                position
                type
                title
            }
            total_review
            rating
        }
    }
}