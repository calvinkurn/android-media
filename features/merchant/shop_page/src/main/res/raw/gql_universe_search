query universe_search($shopID: Int, $q: String){
    universe_search(shopID : $shopID,q: $q){
            data{
                id
                name
                items {
                    location
                    imageURI
                    applink
                    url
                    keyword
                    recom
                    sc
                    isOfficial
                    post_count
                    affiliate_username
                }
            }
       }
}