<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/icSpCheck"
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:icon_color_light_enable="@color/Unify_GN500"
        app:icon_name="CHECK"
        app:layout_constraintBottom_toBottomOf="@+id/tvSpResultInfoItem"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tvSpResultInfoItem" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvSpResultInfoItem"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="6dp"
        android:layout_marginBottom="6dp"
        android:textColor="@color/Unify_Static_Black_96"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/icSpCheck"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="display_2"
        tools:text="Sering mencari peluang untuk strategi baru bisnismu" />
</androidx.constraintlayout.widget.ConstraintLayout>