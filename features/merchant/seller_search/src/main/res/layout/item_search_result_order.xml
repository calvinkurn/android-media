<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="?selectableItemBackground"
    android:paddingStart="16dp"
    android:paddingTop="8dp"
    android:paddingEnd="16dp"
    android:paddingBottom="8dp">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvSearchResultOrderTitle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="display_2"
        app:typographyWeight="reguler"
        tools:text="Bedak Marks" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tvSearchResultOrderDesc"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="4dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/tvSearchResultOrderTitle"
        app:layout_constraintTop_toBottomOf="@id/tvSearchResultOrderTitle"
        app:typographyType="display_3"
        app:typographyWeight="reguler"
        tools:text="Cari produk di pesanan" />

</androidx.constraintlayout.widget.ConstraintLayout>