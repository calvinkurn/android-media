<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.product.addedit">

    <application
        android:allowBackup="true"
        android:supportsRtl="true">

        <activity
            android:name=".preview.presentation.activity.AddEditProductPreviewActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" />

        <activity
            android:name=".detail.presentation.activity.AddEditProductDetailActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" />

        <activity
            android:name=".description.presentation.AddEditProductDescriptionActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" />

        <activity
            android:name=".shipment.presentation.activity.AddEditProductShipmentActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White" />

        <service
            android:name="com.tokopedia.product.addedit.preview.presentation.service.AddEditProductUploadService"
            android:permission="android.permission.BIND_JOB_SERVICE"
            android:enabled="true" />

    </application>
</manifest>