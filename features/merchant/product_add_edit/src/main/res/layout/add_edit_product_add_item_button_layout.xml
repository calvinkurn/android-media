<?xml version="1.0" encoding="utf-8"?>
<androidx.appcompat.widget.LinearLayoutCompat xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/icon_add"
        app:icon_name="ADD_CIRCLE"
        app:icon_color_light_enable="@color/Unify_GN500"
        app:icon_color_light_disabled="@color/Unify_NN300"
        app:icon_color_night_enable="@color/Unify_GN500"
        app:icon_color_night_disabled="@color/Unify_NN300"
        android:layout_width="26dp"
        android:layout_height="26dp" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/typography_add"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginStart="6dp"
        android:gravity="center_vertical"
        android:textColor="@color/Unify_GN500"
        tools:text="dummy text"
        app:typographyType="body_3"
        app:typographyWeight="bold"/>

</androidx.appcompat.widget.LinearLayoutCompat>