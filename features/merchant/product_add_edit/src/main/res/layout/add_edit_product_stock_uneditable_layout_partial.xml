<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:parentTag="androidx.constraintlayout.widget.ConstraintLayout">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_available_stock_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/spacing_lvl5"
        android:text="@string/label_available_stock"
        android:textColor="@color/Neutral_N700_44"
        android:visibility="gone"
        app:typographyType="body_3"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_available_stock_desc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/Neutral_N700_96"
        android:visibility="gone"
        app:typographyType="body_1"
        app:layout_constraintTop_toBottomOf="@id/tv_available_stock_header"
        tools:visibility="visible"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_minimum_order_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/spacing_lvl5"
        android:text="@string/label_minimum_order"
        android:textColor="@color/Neutral_N700_44"
        android:visibility="gone"
        app:typographyType="body_3"
        app:layout_constraintTop_toBottomOf="@id/tv_available_stock_desc"
        tools:visibility="visible"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_minimum_order_desc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textColor="@color/Neutral_N700_96"
        android:visibility="gone"
        app:typographyType="body_1"
        app:layout_constraintTop_toBottomOf="@id/tv_minimum_order_header"
        tools:visibility="visible"/>

</merge>