query ProductListMeta($shopID : String!){
  ProductListMeta(shopID:$shopID){
    header{
      processTime
      messages
      reason
      errorCode
    }
    data{
      tab{
        id
        name
        value
      }
      filter{
      	id
      	name
      	value
      }
      sort{
      	id
      	name
      	value
      }
    }
  }
}