<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <include
        android:id="@+id/errorShopInfoLayout"
        layout="@layout/layout_seller_menu_shop_info_success"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@id/errorShopInfoLayout"
        app:layout_constraintBottom_toBottomOf="@id/divider_other_error" />

    <com.tokopedia.unifycomponents.LocalLoad
        android:id="@+id/settingLocalLoad"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toBottomOf="@id/errorShopInfoLayout"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"/>

    <com.tokopedia.unifycomponents.DividerUnify
        android:id="@+id/divider_other_error"
        android:layout_width="match_parent"
        android:layout_height="@dimen/setting_divider_thick"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@id/settingLocalLoad"
        android:background="@color/Unify_NN0"/>

</androidx.constraintlayout.widget.ConstraintLayout>