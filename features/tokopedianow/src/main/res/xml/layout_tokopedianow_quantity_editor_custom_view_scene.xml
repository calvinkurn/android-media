<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="300">
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/end"
        motion:constraintSetEnd="@+id/start"
        motion:duration="300"/>
    <Transition
        motion:constraintSetStart="@+id/startWithValue"
        motion:constraintSetEnd="@+id/end"
        motion:duration="300">
        <KeyFrameSet>
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="0"
                motion:motionTarget="@id/add_button" />
            <KeyAttribute
                android:alpha="1"
                motion:framePosition="100"
                motion:motionTarget="@id/add_button" />
        </KeyFrameSet>
        <KeyFrameSet>
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="0"
                motion:motionTarget="@id/sub_button" />
            <KeyAttribute
                android:alpha="1"
                motion:framePosition="100"
                motion:motionTarget="@id/sub_button" />
        </KeyFrameSet>
    </Transition>
    <Transition
        motion:constraintSetStart="@+id/end"
        motion:constraintSetEnd="@+id/startWithValue"
        motion:duration="300">
        <KeyFrameSet>
            <KeyAttribute
                android:alpha="1"
                motion:framePosition="0"
                motion:motionTarget="@id/add_button" />
            <KeyAttribute
                android:alpha="0.5"
                motion:framePosition="50"
                motion:motionTarget="@id/add_button" />
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="100"
                motion:motionTarget="@id/add_button" />
        </KeyFrameSet>
        <KeyFrameSet>
            <KeyAttribute
                android:alpha="1"
                motion:framePosition="0"
                motion:motionTarget="@id/sub_button" />
            <KeyAttribute
                android:alpha="0.5"
                motion:framePosition="50"
                motion:motionTarget="@id/sub_button" />
            <KeyAttribute
                android:alpha="0"
                motion:framePosition="100"
                motion:motionTarget="@id/sub_button" />
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/background"
            motion:layout_constraintEnd_toEndOf="@id/edit_text"
            android:layout_width="0dp"
            android:layout_height="30dp"
            motion:layout_constraintBottom_toBottomOf="@id/edit_text"
            motion:layout_constraintTop_toTopOf="@id/edit_text"
            android:translationZ="1dp"
            motion:layout_constraintStart_toStartOf="@id/edit_text"
            android:layout_marginStart="0dp"
            android:layout_marginEnd="0dp">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="0" />
        </Constraint>
        <Constraint
            android:layout_height="46dp"
            android:layout_width="46dp"
            android:id="@+id/sub_button"
            android:translationZ="2dp"
            android:visibility="gone"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="@+id/background"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="8dp">
        </Constraint>
        <Constraint
            android:id="@+id/edit_text"
            android:layout_width="30dp"
            android:layout_height="30dp"
            android:translationZ="3dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" />
        <Constraint
            android:id="@+id/add_button"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="46dp"
            android:layout_height="46dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:translationZ="4dp"/>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="46dp"
            android:layout_width="46dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:translationZ="3dp"
            android:id="@+id/sub_button"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/edit_text"
            android:layout_width="0dp"
            android:layout_height="30dp"
            android:translationZ="2dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintEnd_toEndOf="@+id/background"
            motion:layout_constraintStart_toStartOf="@+id/background" />
        <Constraint
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="46dp"
            android:layout_height="46dp"
            android:translationZ="3dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:id="@+id/add_button" />
        <Constraint
            android:id="@+id/background"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="match_parent"
            android:layout_height="30dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:translationZ="1dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginEnd="8dp"
            android:layout_marginStart="8dp">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
    </ConstraintSet>

    <ConstraintSet android:id="@+id/startWithValue" >
        <Constraint
            android:id="@+id/background"
            motion:layout_constraintEnd_toEndOf="@id/edit_text"
            android:layout_width="0dp"
            android:layout_height="30dp"
            motion:layout_constraintBottom_toBottomOf="@id/edit_text"
            motion:layout_constraintTop_toTopOf="@id/edit_text"
            android:translationZ="1dp"
            motion:layout_constraintStart_toStartOf="@id/edit_text"
            android:layout_marginStart="0dp">
            <CustomAttribute
                motion:attributeName="crossfade"
                motion:customFloatValue="1" />
        </Constraint>
        <Constraint
            android:layout_height="46dp"
            android:layout_width="46dp"
            android:id="@+id/sub_button"
            android:translationZ="2dp"
            android:visibility="gone"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="@+id/background"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginEnd="8dp">
        </Constraint>
        <Constraint
            android:id="@+id/edit_text"
            android:layout_width="wrap_content"
            android:layout_height="30dp"
            android:translationZ="3dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="8dp" />
        <Constraint
            android:id="@+id/add_button"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="46dp"
            android:layout_height="46dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:translationZ="3dp"/>
    </ConstraintSet>
</MotionScene>