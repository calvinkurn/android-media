<?xml version="1.0" encoding="UTF-8"?>
<resources>
    <declare-styleable name="MainToolbar">
        <attr name="screenName" format="string"/>
    </declare-styleable>

    <declare-styleable name="NavToolbar">
        <attr name="toolbarContentType" format="enum">
            <enum name="typeSearchbar" value="0"/>
            <enum name="typeTitle" value="1"/>
            <enum name="typeCustom" value="2"/>
        </attr>
        <attr name="toolbarBackButton" format="enum">
            <enum name="none" value="0"/>
            <enum name="close" value="1"/>
            <enum name="back" value="2"/>
        </attr>
        <attr name="toolbarInitialTheme" format="enum">
            <enum name="dark" value="0"/>
            <enum name="light" value="1"/>
        </attr>
        <attr name="toolbarInitialFillColor" format="enum">
            <enum name="transparent" value="0"/>
            <enum name="filled" value="1"/>
        </attr>
        <attr name="toolbarInvertSearchBarColor" format="boolean"/>
        <attr name="toolbarAlwaysShowShadow" format="boolean"/>
        <attr name="toolbarTitle" format="string"/>
        <attr name="toolbarDefaultHint" format="string"/>
        <attr name="toolbarCustomContent" format="reference"/>
        <attr name="toolbarPageName" format="string"/>
    </declare-styleable>
</resources>