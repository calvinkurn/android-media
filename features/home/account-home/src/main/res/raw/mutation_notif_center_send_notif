mutation notifcenter_sendNotif($section_type: String!, $template_key: String!, $expired_time: Int!,$unique_id: Int) {
    notifcenter_sendNotif(section_type: $section_type, template_key:$template_key, expired_time :$expired_time, unique_id : $unique_id) {
        status
        message_error
        process_time
        server
        data {
            is_success
            warn_message
            notification {
                user_id
                notif_id
            }
        }
    }
}