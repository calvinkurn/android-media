<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <com.tokopedia.home.account.presentation.view.ItemListView
        android:id="@+id/labelview"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/Unify_N0"
        android:paddingLeft="@dimen/unify_space_16"
        android:paddingTop="@dimen/unify_space_16"
        android:paddingRight="@dimen/unify_space_16"
        android:paddingBottom="@dimen/unify_space_16" />

    <View
        android:id="@+id/separator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_list_separator_account_home" />

</LinearLayout>