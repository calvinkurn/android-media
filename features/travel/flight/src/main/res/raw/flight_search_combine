query searchCombineV3($data: SearchCombineArgs!) {
  flightSearchCombineV3(input: $data) {
    data {
      journeys {
        journeyID
      }
      combos {
        fares {
          adultPrice
          childPrice
          infantPrice
          adultPriceNumeric
          childPriceNumeric
          infantPriceNumeric
        }
        combination
        comboKey
        isBestPairing
        total
        totalNumeric
      }
    }
    meta {
      needRefresh
      refreshTime
      maxRetry
      adult
      child
      infant
      requestID
    }
    error {
      id
      status
      title
    }
  }
}