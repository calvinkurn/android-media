query propertyReview($data:PropertyReviewRequest!){
  propertyReview(input:$data){
    item{
      id
      propertyId
      reviewerId
      name
      score
      headline
      pros
      cons
      createTime
      country
    }
    totalReview
    totalReviewByCountry
    averageScoreReview
    hasNext
    headline
  }
}