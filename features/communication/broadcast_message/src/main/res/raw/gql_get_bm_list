query ChatBlastSellerList($page: Int!, $per_page: Int){
    chatBlastSellerList(page:$page, perPage:$per_page) {
        list {
            blastId
            executionTime
            message
            marketingThumbnail {
                attributes {
                    url
                    imageUrl
                }
            }
            products {
                attributes {
                    productId
                    productProfile {
                        name
                        price
                        url
                        imageUrl
                    }
                }
            }
            state {
                status
                totalSent
                totalRead
                totalTarget
            }
        }
        hasNext
        currentPage
    }
}