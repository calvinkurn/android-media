<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.tokopedia.searchbar.navigation_component.NavToolbar
        android:id="@+id/affiliate_nc_nav_toolbar"
        android:layout_width="0dp"
        android:layout_height="?actionBarSize"
        app:contentInsetLeft=".0dp"
        app:contentInsetStart=".0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:toolbarBackButton="back"
        app:toolbarInitialTheme="light"
        tools:layout_height="?actionBarSize" />

    <FrameLayout
        android:id="@+id/fragment_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/affiliate_nc_nav_toolbar" />

</androidx.constraintlayout.widget.ConstraintLayout>