<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/topchat_chatroom_cl_order_cancellation"
    android:orientation="horizontal"
    tools:ignore="RtlSymmetry">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/topchat_chatroom_tv_order_cancellation"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toLeftOf="@id/topchat_chatroom_icon_order_cancellation"
        app:layout_constraintBottom_toBottomOf="parent"
        android:paddingTop="10dp"
        android:paddingStart="16dp"
        android:paddingBottom="10dp"
        android:layout_gravity="center_vertical"
        app:typographyType="display_2"
        android:maxLines="1"
        android:ellipsize="end"
        tools:text="@tools:sample/lorem/random" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/topchat_chatroom_icon_order_cancellation"
        android:layout_width="24dp"
        android:layout_height="24dp"
        app:layout_constraintTop_toTopOf="@id/topchat_chatroom_tv_order_cancellation"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toBottomOf="@id/topchat_chatroom_tv_order_cancellation"
        android:layout_gravity="center_vertical"
        android:layout_marginEnd="16dp"
        app:icon_color_light_enable="@color/Unify_NN950_96"
        app:icon_name="CHEVRON_RIGHT" />

</androidx.constraintlayout.widget.ConstraintLayout>