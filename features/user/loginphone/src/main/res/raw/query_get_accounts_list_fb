query get_accounts_list($validate_token: String!, $login_type: String!) {
  accountsGetAccountsList(validate_token: $validate_token, login_type: $login_type) {
    key
    msisdn_view
    msisdn
    users_details {
      user_id
      fullname
      email
      msisdn_verified
      image
    }
    users_count
    errors {
      name
      message
    }
  }
}
