<?xml version="1.0" encoding="utf-8"?>
<resources>
  <declare-styleable name="LayoutCameraView">
    <attr name="layoutType" format="integer">
      <enum name="selfie" value="1"/>
      <enum name="idCard" value="2"/>
      <enum name="savingBook" value="3"/>
    </attr>
  </declare-styleable>
  
  <declare-styleable name="PhoneNumberView">
    <attr name="label" format="string"/>
    <attr name="message" format="string"/>
  </declare-styleable>
  
  <declare-styleable name="ThumbnailFileView">
    <attr name="title" format="string"/>
    <attr name="image" format="reference|integer"/>
  </declare-styleable>
</resources>