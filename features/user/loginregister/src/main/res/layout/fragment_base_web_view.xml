<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    android:background="@color/Unify_N0"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.tokopedia.unifycomponents.LoaderUnify
        android:layout_gravity="center"
        android:layout_width="@dimen/layout_lvl6"
        android:layout_height="@dimen/layout_lvl6"
        android:id="@+id/base_web_view_loader"
        android:visibility="gone" />

    <WebView
        android:id="@+id/base_web_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/base_external_register_toolbar" />

</FrameLayout>