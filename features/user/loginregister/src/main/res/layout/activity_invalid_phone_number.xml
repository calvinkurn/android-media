<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    tools:context=".invalid_phone_number.view.activity.InvalidPhoneNumberActivity">

    <com.tokopedia.header.HeaderUnify
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:unify_header_title="@string/ipn_toolbar_title"
        app:unify_header_is_show_back_button="true"
        app:unify_header_is_show_shadow="false" />

    <FrameLayout
        android:id="@+id/parent_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>