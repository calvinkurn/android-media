<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_redefine_register"
    app:startDestination="@id/redefineRegisterEmailFragment">

    <fragment
        android:id="@+id/redefineRegisterEmailFragment"
        android:name="com.tokopedia.loginregister.redefineregisteremail.view.registeremail.view.fragment.RedefineRegisterEmailFragment"
        android:label="RedefineRegisterEmailFragment"
        tools:layout="@layout/fragment_redefine_register_email">
        <action
            android:id="@+id/action_redefineRegisterEmailFragment_to_redefineRegisterInputPhoneFragment"
            app:destination="@id/redefineRegisterInputPhoneFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"/>
    </fragment>

    <fragment
        android:id="@+id/redefineRegisterInputPhoneFragment"
        android:name="com.tokopedia.loginregister.redefineregisteremail.view.inputphone.view.fragment.RedefineRegisterInputPhoneFragment"
        android:label="RedefineRegisterInputPhoneFragment"
        tools:layout="@layout/fragment_redefine_register_input_phone" >
        <argument
            android:name="source"
            app:argType="string"
            android:defaultValue=""/>
        <argument
            android:name="email"
            app:argType="string"
            android:defaultValue=""/>
        <argument
            android:name="password"
            app:argType="string"
            android:defaultValue=""/>
        <argument
            android:name="name"
            app:argType="string"
            android:defaultValue=""/>
        <argument
            android:name="isRequiredInputPhone"
            app:argType="boolean"
            android:defaultValue="false"/>
        <argument
            android:name="token"
            app:argType="string"
            android:defaultValue=""/>
        <argument
            android:name="hash"
            app:argType="string"
            android:defaultValue=""/>
    </fragment>

</navigation>