mutation register($regType: String!, $fullname: String, $email: String, $password: String, $osType: String, $validateToken: String, $h: String) {
    register_v2(input: {
        reg_type: $regType
        fullname: $fullname
        email: $email
        password: $password
        os_type: $osType
        validate_token: $validateToken
        h: $h
    }) {
        user_id
        sid
        access_token
        refresh_token
        token_type
        is_active
        action
        errors {
            name
            message
        }
    }
}
