<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.privacyaccount">

    <application
        android:allowBackup="true"
        android:supportsRtl="true">

        <activity
            android:name="com.tokopedia.privacyaccount.PrivacyAccountActivity"
            android:theme="@style/Theme.WhiteUnify"
            android:exported="false"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.tokopedia.internal.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="@string/host_user"
                    android:pathPattern="@string/path_main_privacy_account"
                    android:scheme="@string/internal_scheme" />
            </intent-filter>
        </activity>

    </application>

</manifest>