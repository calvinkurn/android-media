<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/Unify_N0">

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:layout_marginBottom="16dp"
        android:background="@color/Unify_N100"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txt_help_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginBottom="8dp"
        android:text="@string/csat_txt_help_title"
        android:textColor="@color/Unify_N700_68"
        android:textSize="14sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/divider" />

    <ImageView
        android:id="@+id/btn_inactive_1"
        android:clickable="true"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_1"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="14sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

    <ImageView
        android:id="@+id/btn_inactive_2"
        android:clickable="true"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_2"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="14sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_1"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_3"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

    <ImageView
        android:id="@+id/btn_inactive_3"
        android:clickable="true"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_3"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="14sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_2"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_4"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

    <ImageView
        android:id="@+id/btn_inactive_4"
        android:clickable="true"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_4"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="14sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_3"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_5"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

    <ImageView
        android:id="@+id/btn_inactive_5"
        android:clickable="true"
        android:layout_width="40dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        android:layout_marginBottom="16dp"
        android:gravity="center"
        app:srcCompat="@drawable/rating_inactive_5"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="14sp"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title" />

</androidx.constraintlayout.widget.ConstraintLayout>