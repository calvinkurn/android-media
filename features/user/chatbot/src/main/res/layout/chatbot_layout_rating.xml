<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/Unify_N0">

    <View
        android:id="@+id/divider"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unify_space_2"
        android:layout_marginBottom="@dimen/unify_space_16"
        android:background="@color/Unify_N100"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/txt_help_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/unify_space_16"
        android:layout_marginLeft="@dimen/unify_space_16"
        android:layout_marginTop="@dimen/unify_space_24"
        android:layout_marginBottom="@dimen/unify_space_8"
        android:text="@string/csat_txt_help_title"
        android:textColor="@color/Unify_N700_68"
        app:typographyWeight="bold"
        app:typographyType="display_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/divider" />

    <ImageView
        android:id="@+id/btn_inactive_1"
        android:clickable="true"
        android:layout_width="@dimen/unify_space_40"
        android:layout_height="@dimen/unify_space_40"
        android:layout_marginStart="@dimen/unify_space_16"
        android:layout_marginLeft="@dimen/unify_space_16"
        android:layout_marginTop="@dimen/unify_space_16"
        android:layout_marginBottom="@dimen/unify_space_16"
        android:gravity="center"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="@dimen/unify_font_14"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title"
        app:srcCompat="@drawable/rating_inactive_1"
        android:contentDescription="btn_inactive_1" />

    <ImageView
        android:id="@+id/btn_inactive_2"
        android:clickable="true"
        android:layout_width="@dimen/unify_space_40"
        android:layout_height="@dimen/unify_space_40"
        android:layout_marginTop="@dimen/unify_space_16"
        android:layout_marginBottom="@dimen/unify_space_16"
        android:gravity="center"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="@dimen/unify_font_14"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_1"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_3"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title"
        app:srcCompat="@drawable/rating_inactive_2"
        android:contentDescription="btn_inactive_2" />

    <ImageView
        android:id="@+id/btn_inactive_3"
        android:clickable="true"
        android:layout_width="@dimen/unify_space_40"
        android:layout_height="@dimen/unify_space_40"
        android:layout_marginTop="@dimen/unify_space_16"
        android:layout_marginBottom="@dimen/unify_space_16"
        android:gravity="center"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="@dimen/unify_font_14"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_2"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_4"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title"
        app:srcCompat="@drawable/rating_inactive_3"
        android:contentDescription="btn_inactive_3" />

    <ImageView
        android:id="@+id/btn_inactive_4"
        android:clickable="true"
        android:layout_width="@dimen/unify_space_40"
        android:layout_height="@dimen/unify_space_40"
        android:layout_marginTop="@dimen/unify_space_16"
        android:layout_marginBottom="@dimen/unify_space_16"
        android:gravity="center"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="@dimen/unify_font_14"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_3"
        app:layout_constraintRight_toLeftOf="@+id/btn_inactive_5"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title"
        app:srcCompat="@drawable/rating_inactive_4"
        android:contentDescription="btn_inactive_4" />

    <ImageView
        android:id="@+id/btn_inactive_5"
        android:clickable="true"
        android:layout_width="@dimen/unify_space_40"
        android:layout_height="@dimen/unify_space_40"
        android:layout_marginTop="@dimen/unify_space_16"
        android:layout_marginEnd="@dimen/unify_space_16"
        android:layout_marginRight="@dimen/unify_space_16"
        android:layout_marginBottom="@dimen/unify_space_16"
        android:gravity="center"
        android:text="@string/chatbot_no"
        android:textColor="@color/Unify_N700_32"
        android:textSize="@dimen/unify_font_14"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintLeft_toRightOf="@+id/btn_inactive_4"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_help_title"
        app:srcCompat="@drawable/rating_inactive_5"
        android:contentDescription="btn_inactive_5" />

</androidx.constraintlayout.widget.ConstraintLayout>