<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/chatcommon_56dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/back_icon"
        android:layout_width="24dp"
        android:layout_height="24dp"
        android:layout_marginLeft="10dp"
        app:icon_name="ARROW_BACK"
        app:icon_color_light_enable="@color/Unify_NN0"
        app:icon_color_night_enable="@color/Unify_NN0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/chatcommon_9dp"
        android:layout_marginEnd="@dimen/chatcommon_60dp"
        android:ellipsize="end"
        android:maxLines="1"
        android:textColor="@color/Unify_NN950_96"
        app:layout_constrainedWidth="true"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toRightOf="@id/back_icon"
        app:layout_constraintTop_toTopOf="@id/back_icon"
        app:layout_constraintBottom_toBottomOf="@id/back_icon"
        app:layout_constraintRight_toRightOf="parent"
        app:typographyType="heading_4"
        tools:text="@tools:sample/lorem/random" />



</androidx.constraintlayout.widget.ConstraintLayout>