<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.privacyacenter">

    <application
        android:allowBackup="true"
        android:supportsRtl="true">

        <activity
            android:name="com.tokopedia.privacycenter.PrivacyCenterActivity"
            android:theme="@style/TokopediaTransparentStatusBar"
            android:exported="false"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.tokopedia.internal.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="@string/host_user"
                    android:pathPattern="@string/path_privacy_center"
                    android:scheme="@string/internal_scheme" />
            </intent-filter>
        </activity>

    </application>

</manifest>