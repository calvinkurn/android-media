query get_existing_message_id($page: Int!, $filter: String!, $tab: String!) {
  status
  chatListMessage(page: $page, filter: $filter, tab: $tab) {
    list{
          msgID
          messageKey
          attributes {
            contact {
              id
              role
              domain
              name
              shopStatus
              tag
              thumbnail
            }
            lastReplyMessage
            lastReplyTimeStr
            readStatus
            unreads
            unreadsreply
            fraudStatus
            pinStatus
            isReplyByTopbot
          }
        }
    hasNext
    pagingNext
    showTimeMachine
  }
}
