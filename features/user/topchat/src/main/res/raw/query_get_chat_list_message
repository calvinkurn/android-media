query get_existing_message_id($page: Int!, $perPage: Int!, $filter: String!, $tab: String!) {
  status
  chatListMessage(filter: $filter, page: $page, tab: $tab, perPage: $perPage) {
    list{
          msgID
          messageKey
          attributes {
            contact {
              id
              role
              domain
              name
              shopStatus
              tag
              thumbnail
            }
            lastReplyMessage
            lastReplyTimeStr
            readStatus
            unreads
            unreadsreply
            fraudStatus
            pinStatus
            isReplyByTopbot
          }
        }
    hasNext
    pagingNext
    showTimeMachine
  }
}
