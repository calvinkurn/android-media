mutation richieAddBankAccountNewFlow($bankID: Int!,
  $bankName: String!,
  $accountNo: String!,
  $accountName:String!,
  $isManual : Boolean!){
  richieAddBankAccountNewFlow
        (input:
          {   bankID:$bankID,
              bankID:$bankID,
              accountNo:$accountNo,
              accountName:$accountName,
              isManual:$isManual
          }){
     	status
    	message
    	data{
            accID
            accNo
            accName
            bankID
            userID
      }
  }
}