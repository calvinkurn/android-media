<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/fp_account_chooser_parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/dp_24"
    tools:context=".view.AccountChooserDialog">

    <ImageView
        android:id="@+id/fp_account_chooser_avatar"
        android:layout_width="42dp"
        android:layout_height="42dp"
        app:layout_constraintBottom_toBottomOf="@+id/fp_account_chooser_email"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/fp_account_chooser_name" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/fp_account_chooser_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginLeft="15dp"
        app:layout_constraintStart_toEndOf="@+id/fp_account_chooser_avatar"
        app:layout_constraintTop_toTopOf="parent"
        tools:text="Yang Dipertuan Agung" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/fp_account_chooser_email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/fp_account_chooser_name"
        app:layout_constraintTop_toBottomOf="@+id/fp_account_chooser_name"
        app:typographyType="body_3"
        tools:text="yangdipertuanagung@gmail.com"/>

<!--    <ListU-->
<!--        android:layout_width=""-->
<!--        android:layout_height=""-->

</androidx.constraintlayout.widget.ConstraintLayout>