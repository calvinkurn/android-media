query get_accounts_list($validate_token: String!, $phone: String!, $login_type: String!) {
  accountsGetAccountsList(validate_token: $validate_token, phone: $phone, login_type: $login_type) {
    key
    msisdn_view
    msisdn
    users_details {
      user_id
      fullname
      email
      msisdn_verified
      image
      challenge_2fa
      user_id_enc
      shop_detail {
        id
        name
        domain
      }
    }
    users_count
    errors {
      name
      message
    }
  }
}
