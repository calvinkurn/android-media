<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/affiliate_parent_linear"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/spacing_lvl3"
    android:orientation="vertical">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/traffic_attribution_text"
        android:text="Jumlah komisi yang kamu dapatkan dari tingkat pembelian produk dan kunjungan."
        android:layout_height="wrap_content"
        android:layout_width="wrap_content"
        app:typographyType="body_2"
        android:layout_marginBottom="@dimen/spacing_lvl4"
        />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/traffic_rv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>

