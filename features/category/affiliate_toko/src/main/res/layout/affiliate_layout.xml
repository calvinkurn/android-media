<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <com.tokopedia.exploreCategory.ui.bottomnav.LottieBottomNavbar
        android:id="@+id/bottom_navbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unify_space_48"
        tools:layout_height="100dp"
        android:elevation="15dp"
        app:buttonColor="@color/Unify_N500"
        app:buttonContainerBackgroundColor="@color/Unify_N0"
        app:buttonsHeight="@dimen/unify_space_48"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:setViewPagerSwipeable="false"
        app:navbarElevation="15dp"
        app:viewPagerBackground="@color/Unify_N0"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools" />


</androidx.constraintlayout.widget.ConstraintLayout>