mutation hachikoRedeem($catalogId: Int!, $isGift: Int!, $giftUserId: Int!, $giftEmail: String, $notes: String ) {
  hachikoRedeem(catalog_id: $catalogId, is_gift: $isGift, gift_user_id: $giftUserId, gift_email: $giftEmail, notes: $notes) {
    coupons{
      id
      owner
      promo_id
      code
      title
      description
      cta
      cta_desktop
    }
    reward_points
  }
}