query Discovery_myCoupons($categoryID: Int, $page: Int, $limit: Int, $serviceID: String, $categoryIDCoupon: Int, $includeExtraInfo: Int, $apiVersion : String, $source : String, $catalogSlugs : [String!],$clientID : String,$isGetPromoInfo: Boolean) {
  tokopointsCouponListStack(input: {serviceID: $serviceID, categoryID: $categoryID, categoryIDCoupon: $categoryIDCoupon, page: $page, limit: $limit, includeExtraInfo: $includeExtraInfo , apiVersion: $apiVersion, source : $source, catalogSlugs : $catalogSlugs,clientID : $clientID, isGetPromoInfo: $isGetPromoInfo}) {
    tokopointsCouponDataStack {
         catalogID: id
         title
         code
         imageURL
         imageURLMobile
         minimumUsage
         minimumUsageLabel
         isNewCoupon
         isStacked
         stackID
         isHighlighted
         upperLeftSection {
           textAttributes {
             text
             color
             isBold
           }
           backgroundColor
         }
         promoInfo {
           promoType
           promoIcon
         }
       }
       tokopointsPaging {
         hasNext
       }
  }
}
