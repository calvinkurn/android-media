<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:paddingStart="16dp"
    android:paddingEnd="16dp"
    android:layout_marginTop="12dp"
    >

    <com.tokopedia.unifycomponents.LoaderUnify
        android:layout_width="44dp"
        android:layout_height="44dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:loader_type="circle"
        android:id="@+id/loader_order"
        />
    
    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_order_name"
        android:layout_width="0dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@id/loader_order"
        app:layout_constraintEnd_toEndOf="parent"
        app:loader_type="rect"
        app:layout_constraintTop_toTopOf="@id/loader_order"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_order_detail"
        android:layout_width="155dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@id/loader_order"
        app:loader_type="rect"
        app:layout_constraintTop_toBottomOf="@id/loader_order_name"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_shop_name"
        android:layout_width="0dp"
        android:layout_height="12dp"
        android:layout_marginTop="34dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:loader_type="rect"
        android:layout_marginEnd="32dp"
        app:layout_constraintTop_toBottomOf="@id/loader_order"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_shop_detail"
        android:layout_width="155dp"
        android:layout_height="12dp"
        android:layout_marginTop="8dp"
        app:loader_type="rect"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/loader_shop_name"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:layout_width="44dp"
        android:layout_height="44dp"
        app:layout_constraintStart_toStartOf="parent"
        app:loader_type="circle"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/loader_shop_detail"
        android:id="@+id/loader_product_one"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_product_one_name"
        android:layout_width="0dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@id/loader_product_one"
        app:layout_constraintEnd_toEndOf="parent"
        app:loader_type="rect"
        app:layout_constraintTop_toTopOf="@id/loader_product_one"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_product_one_detail"
        android:layout_width="155dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@id/loader_product_one"
        app:loader_type="rect"
        app:layout_constraintTop_toBottomOf="@id/loader_product_one_name"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_product_one_detail_subtitle"
        android:layout_width="155dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@id/loader_product_one"
        app:loader_type="rect"
        app:layout_constraintTop_toBottomOf="@id/loader_product_one_detail"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:layout_width="44dp"
        android:layout_height="44dp"
        app:layout_constraintStart_toStartOf="parent"
        app:loader_type="circle"
        android:layout_marginTop="24dp"
        app:layout_constraintTop_toBottomOf="@id/loader_product_one_detail_subtitle"
        android:id="@+id/loader_product_two"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_product_two_name"
        android:layout_width="0dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@id/loader_product_two"
        app:layout_constraintEnd_toEndOf="parent"
        app:loader_type="rect"
        app:layout_constraintTop_toTopOf="@id/loader_product_two"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_product_two_detail"
        android:layout_width="155dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@id/loader_product_two"
        app:loader_type="rect"
        app:layout_constraintTop_toBottomOf="@id/loader_product_two_name"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_product_two_detail_subtitle"
        android:layout_width="155dp"
        android:layout_height="12dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="4dp"
        app:layout_constraintStart_toEndOf="@id/loader_product_two"
        app:loader_type="rect"
        app:layout_constraintTop_toBottomOf="@id/loader_product_two_detail"
        />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/loader_banner"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:loader_type="rect"
        app:layout_constraintTop_toBottomOf="@id/loader_product_two_detail_subtitle"
        />

</androidx.constraintlayout.widget.ConstraintLayout>