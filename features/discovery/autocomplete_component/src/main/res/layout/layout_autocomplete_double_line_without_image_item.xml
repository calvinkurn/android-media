<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/autocompleteDoubleLineWithoutImageItem"
    android:layout_width="match_parent"
    android:layout_height="52dp"
    android:foreground="?attr/selectableItemBackground">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/autocompleteSuggestionDoubleLineWithoutImageTitle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:ellipsize="end"
        android:maxLines="1"
        tools:text="@string/autocomplete_suggestion_double_line_without_image_title"
        android:textColor="@color/Unify_N700_96"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="display_2"/>

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/autocompleteSuggestionDoubleLineWithoutImageSubtitle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:layout_marginBottom="8dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:ellipsize="end"
        android:maxLines="1"
        tools:text="@string/autocomplete_suggestion_double_line_without_image_subtitle"
        android:textColor="@color/Unify_G500"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/autocompleteSuggestionDoubleLineWithoutImageTitle"
        app:typographyType="display_3"
        app:typographyWeight="bold"/>

</androidx.constraintlayout.widget.ConstraintLayout>