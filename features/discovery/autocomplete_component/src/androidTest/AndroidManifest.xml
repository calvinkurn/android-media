<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:versionCode="1"
    android:versionName="1.0.0"
    package="com.tokopedia.autocompletecomponent">

    <application
        android:label="Auto Complete Instrumentation Test"
        tools:replace="android:label">

        <activity
            android:name=".AutoCompleteActivityStub"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">

            <intent-filter>
                <action android:name="com.tokopedia.internal.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:scheme="@string/internal_scheme"
                    android:host="@string/host_discovery"
                    android:pathPattern="@string/path_autocomplete"/>
            </intent-filter>
        </activity>

        <activity
            android:name=".universal.UniversalSearchActivityStub"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" >
            <intent-filter>
                <action android:name="com.tokopedia.internal.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:scheme="@string/internal_scheme"
                    android:host="@string/host_discovery"
                    android:path="@string/path_universal" />
            </intent-filter>
        </activity>

        <activity
            android:name=".initialstate.InitialStateActivityTest"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />

        <activity
            android:name=".suggestion.SuggestionActivityTest"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />

        <activity
            android:name=".AutoCompleteActivitySample"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />

    </application>

</manifest>