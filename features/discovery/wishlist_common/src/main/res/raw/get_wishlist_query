query wishlist($page: Int, $count: Int, $filter: WishlistFilter, $additionalParams: WishlistAdditionalParams){
  wishlist(page: $page, count: $count, filter: $filter, additionalParams: $additionalParams){
      has_next_page
      total_data
      items {
        category_breadcrumb
        id
        name
        url
        image_url
        raw_price
        condition
        available
        status
        price
        minimum_order
        rating
        review_count
        free_ongkir {
          is_active
          image_url
        }
        free_ongkir_extra {
          is_active
          image_url
        }
        wholesale_price {
          minimum
          maximum
          price
        }
        shop {
          id
          name
          url
          gold_merchant
          official_store
          status
          location

        }
        preorder
        badges {
          title
          image_url
        }
        labels {
          title
          color
        }
      }
      pagination {
        next_url
      }
  }
}
