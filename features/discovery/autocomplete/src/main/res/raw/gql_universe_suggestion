query universe_suggestion($params: String!) {
  universe_suggestion(param: $params) {
    data {
      id
      name
      items {
        template
        type
        applink
        url
        title
        subtitle
        icon_title
        icon_subtitle
        shortcut_image
        image_url
        url_tracker
        tracking {
          code
        }
      }
    }
  }
}