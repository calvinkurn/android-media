<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools">

    <com.tokopedia.unifycomponents.ContainerUnify
        android:id="@+id/inspirationCarouselListPostAtcContainer"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        android:visibility="visible"
        app:container_color="GREEN"
        app:layout_constraintBottom_toBottomOf="@id/inspirationCarouselListPostAtcProductSeparator"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/inspirationCarouselListPostAtcListGroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="visible"
        app:constraint_referenced_ids="inspirationCarouselListPostAtcProductAtc,
                inspirationCarouselListPostAtcHeaderTitle, inspirationCarouselListPostAtcHeaderBadge,
                inspirationCarouselListPostAtcHeaderSubtitle, inspirationCarouselListPostAtcHeaderClose,
                inspirationCarouselListPostAtcProduct, inspirationCarouselListPostAtcProductReimagine"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="visible" />

    <com.tokopedia.unifycomponents.ImageUnify
        android:id="@+id/inspirationCarouselListPostAtcProductAtc"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="12dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/inspirationCarouselListPostAtcHeaderTitle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginHorizontal="16dp"
        android:ellipsize="end"
        android:lines="1"
        android:maxEms="12"
        app:layout_constraintEnd_toStartOf="@+id/inspirationCarouselListPostAtcHeaderClose"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toEndOf="@+id/inspirationCarouselListPostAtcProductAtc"
        app:layout_constraintTop_toTopOf="@+id/inspirationCarouselListPostAtcProductAtc"
        app:typographyType="display_1"
        app:typographyWeight="bold"
        tools:text="Hypebeast Banget Pokoknya" />

    <com.tokopedia.unifycomponents.ImageUnify
        android:id="@+id/inspirationCarouselListPostAtcHeaderBadge"
        android:layout_width="16dp"
        android:layout_height="16dp"
        android:layout_marginTop="4dp"
        android:contentDescription="@string/content_desc_icon_subtitle"
        app:layout_constraintBottom_toBottomOf="@+id/inspirationCarouselListPostAtcProductAtc"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintStart_toStartOf="@+id/inspirationCarouselListPostAtcHeaderTitle"
        app:layout_constraintTop_toBottomOf="@+id/inspirationCarouselListPostAtcHeaderTitle" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/inspirationCarouselListPostAtcHeaderSubtitle"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="2dp"
        android:layout_marginEnd="16dp"
        android:ellipsize="end"
        android:lines="1"
        android:textColor="@color/Unify_NN600"
        app:layout_constraintBottom_toBottomOf="@+id/inspirationCarouselListPostAtcHeaderBadge"
        app:layout_constraintEnd_toStartOf="@+id/inspirationCarouselListPostAtcHeaderClose"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintLeft_toRightOf="@id/inspirationCarouselListPostAtcHeaderBadge"
        app:layout_constraintStart_toEndOf="@+id/inspirationCarouselListPostAtcHeaderBadge"
        app:layout_constraintTop_toTopOf="@+id/inspirationCarouselListPostAtcHeaderBadge"
        app:layout_goneMarginLeft="@dimen/home_component_header_margin_default"
        app:layout_goneMarginRight="@dimen/home_component_header_margin_default"
        app:layout_goneMarginTop="@dimen/home_component_header_subtitle_top_padding"
        app:typographyType="display_3"
        tools:text="Berakhir dalam"
        tools:visibility="visible" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/inspirationCarouselListPostAtcHeaderClose"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginEnd="16dp"
        app:icon_name="CLOSE"
        app:layout_constraintBottom_toBottomOf="@+id/inspirationCarouselListPostAtcHeaderSubtitle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/inspirationCarouselListPostAtcHeaderTitle" />

    <com.tokopedia.carouselproductcard.CarouselProductCardView
        android:id="@+id/inspirationCarouselListPostAtcProduct"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingBottom="12dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inspirationCarouselListPostAtcHeaderSubtitle"
        tools:visibility="gone" />

    <com.tokopedia.carouselproductcard.reimagine.CarouselProductCardView
        android:id="@+id/inspirationCarouselListPostAtcProductReimagine"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingBottom="12dp"
        android:layout_marginTop="12dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inspirationCarouselListPostAtcHeaderSubtitle"
        tools:visibility="visible" />

    <View
        android:id="@+id/inspirationCarouselListPostAtcProductSeparator"
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:layout_marginBottom="10dp"
        android:background="@color/Unify_NN50"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="@id/searchBroadMatchBarrier"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="parent"
        tools:visibility="visible" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/searchBroadMatchBarrier"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="inspirationCarouselListPostAtcProductReimagine,
        inspirationCarouselListPostAtcSuccessMessageHide,
        inspirationCarouselListPostAtcProduct" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/inspirationCarouselListPostAtcHideGroup"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:constraint_referenced_ids="inspirationCarouselListPostAtcSuccesHide,
                inspirationCarouselListPostAtcCancelHide, inspirationCarouselListPostAtcSuccessMessageHide"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:visibility="gone" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/inspirationCarouselListPostAtcSuccesHide"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="12dp"
        android:ellipsize="end"
        android:lines="1"
        android:textColor="@color/Unify_NN950"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="paragraph_3"
        android:text="@string/search_inspiration_carousel_post_atc_success_hide" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/inspirationCarouselListPostAtcCancelHide"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginTop="12dp"
        android:paddingHorizontal="16dp"
        android:ellipsize="end"
        android:lines="1"
        android:text="@string/search_inspiration_carousel_post_atc_cancel_hide"
        android:textColor="@color/Unify_GN500"
        android:visibility="gone"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="display_3"
        app:typographyWeight="bold"
        tools:visibility="visible" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/inspirationCarouselListPostAtcSuccessMessageHide"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:paddingVertical="32dp"
        android:layout_marginHorizontal="16dp"
        android:layout_centerVertical="true"
        android:ellipsize="end"
        android:lines="1"
        android:paddingHorizontal="16dp"
        android:text="@string/search_inspiration_carousel_post_atc_success_messsage_hide"
        android:textColor="@color/Unify_NN950"
        android:textAlignment="center"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/inspirationCarouselListPostAtcSuccesHide"
        app:typographyType="display_2"
        app:typographyWeight="bold"
        tools:visibility="visible" />

</androidx.constraintlayout.widget.ConstraintLayout>