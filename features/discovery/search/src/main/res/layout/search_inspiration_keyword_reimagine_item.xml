<?xml version="1.0" encoding="utf-8"?>
<com.tokopedia.unifycomponents.CardUnify2
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/searchInspirationKeywordReimagineCard"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="8dp"
    android:background="?attr/selectableItemBackground"
    app:card_type="border"
    app:cardCornerRadius="12dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="6dp"
        android:layout_marginVertical="8dp">

        <com.tokopedia.unifycomponents.ImageUnify
            android:id="@+id/searchInspirationKeywordReimagineIcon"
            android:layout_width="16dp"
            android:layout_height="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            android:src="@drawable/ic_product_navigation_search_grayscale_24"/>

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/searchInspirationKeywordReimagineKeyword"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:ellipsize="end"
            android:maxLines="2"
            android:textColor="@color/Unify_NN950_96"
            android:visibility="visible"
            android:layout_marginStart="4dp"
            app:layout_constraintBottom_toBottomOf="@+id/searchInspirationKeywordReimagineIcon"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/searchInspirationKeywordReimagineIcon"
            app:layout_constraintTop_toTopOf="@+id/searchInspirationKeywordReimagineIcon"
            app:layout_goneMarginTop="@dimen/product_card_text_view_product_name_gone_margin_top"
            app:typographyType="small"
            tools:text="Product Name Product Name Product Name Product Name Product Name Product Name Product Name" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</com.tokopedia.unifycomponents.CardUnify2>