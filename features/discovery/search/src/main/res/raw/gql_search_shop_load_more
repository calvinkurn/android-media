      shop_rate_accuracy
      shop_rate_service
      shop_status
      products {
        id
        name
        url
        applink
        price
        price_format
        image_url
      }
      voucher {
        free_shipping
        cashback {
          cashback_value
          is_percentage
        }
      }
      shop_lucky
      reputation_image_uri
      reputation_score
      is_official
      ga_key
    }
    top_seller_data {
      shop_id
      shop_name
      shop_domain
      shop_url
      shop_applink
      shop_image
      shop_image_300
      shop_description
      shop_tag_line
      shop_location
      shop_total_transaction
      shop_total_favorite
      shop_gold_shop
      shop_is_owner
      shop_rate_speed
      shop_rate_accuracy
      shop_rate_service
      shop_status
      products {
        id
        name
        url
        applink
        price
        price_format
        image_url
      }
      voucher {
        free_shipping
        cashback {
          cashback_value
          is_percentage
        }
      }
      shop_lucky
      reputation_image_uri
      reputation_score
      is_official
      ga_key
    }
    top_official_seller_data {
      shop_id
      shop_name
      shop_domain
      shop_url
      shop_applink
      shop_image
      shop_image_300
      shop_description
      shop_tag_line
      shop_location
      shop_total_transaction
      shop_total_favorite
      shop_gold_shop
      shop_is_owner
      shop_rate_speed
      shop_rate_accuracy
      shop_rate_service
      shop_status
      products {
        id
        name
        url
        applink
        price
        price_format
        image_url
      }
      voucher {
        free_shipping
        cashback {
          cashback_value
          is_percentage
        }
      }
      shop_lucky
      reputation_image_uri
      reputation_score
      is_official
      ga_key
    }
  }
}