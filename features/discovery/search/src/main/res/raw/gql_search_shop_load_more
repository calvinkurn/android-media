    source
    total_shop
    search_url
    paging {
      uri_next
      uri_previous
    }
    tab_name
    shops {
      shop_id
      shop_name
      shop_domain
      shop_url
      shop_applink
      shop_image
      shop_image_300
      shop_description
      shop_tag_line
      shop_location
      shop_total_transaction
      shop_total_favorite
      shop_gold_shop
      shop_is_owner
