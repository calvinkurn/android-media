<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    android:id="@+id/cta_chevron_container"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <ImageView
        android:id="@+id/cta_chevron_border"
        android:layout_width="@dimen/home_component_header_cta_container"
        android:layout_height="@dimen/home_component_header_cta_container"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:src="@drawable/home_component_header_bg_header_cta"
        android:importantForAccessibility="no" />
    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/cta_chevron_icon"
        app:icon_name="CHEVRON_RIGHT"
        android:clickable="true"
        android:focusable="true"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_width="@dimen/home_component_header_cta_icon"
        android:layout_height="@dimen/home_component_header_cta_icon"
        android:contentDescription="@string/content_desc_header_cta"/>
</androidx.constraintlayout.widget.ConstraintLayout>