query getMojitoEligibleSellerProduct($camp_id: Int!,
                      $rows: Int!,
                      $offset: Int!,
                      $shop_id: Int!,
                      $q: String,
                      $filter: Int){
    getMojitoEligibleSellerProduct(params:{
    camp_id:$camp_id
    rows:$rows
    offset:$offset
    shop_id:$shop_id
    q:$q
    filter:$filter
    }){
        data{
            submitted_count
            pending_count
            products{
                id
                shop_id
                pacman_time_unix
                status
                condition
                flag
                price_level
                star_rating
                returnable
                city_id
                count_view
                count_review
                count_talk
                count_transaction
                server_id
                child_category_id
                currency_id
                menu_id
                position
                cluster_id
                price
                rating
                review_score
                product_score
                bestmatch_score4
                bestmatch_score4_fixed
                bestmatch_score5
                bestmatch_score5_fixed
                bestmatch_score6
                bestmatch_score6_fixed
                original_price
                discounted_price
                name
                name_sort
                keyword
                city_name
                url_path
                update_time
                create_time
                location
                image_path
                image_name
                department_id
                shipping
                brand {
                  ID
                }
                shop {
                  id
                  name
                  domain
                  gold_merchant_expired
                  shop_score
                  shop_reputation
                  is_official
                  rate_cancel
                }
                campaign {
                  campaign_id
                  start_date
                  end_date
                  discount_percentage
                  original_price
                  discounted_price
                  criteria {
                    criteria_id
                    price_min
                    price_max
                    stock_min
                    rating_min
                    rating_max
                    cashback_minimum
                    cashback_maximum
                    discount_percentage_min
                    discount_percentage_max
                    submission_max
                    submission_count
                    exclude_preorder
                    exclude_wholesale
                    min_order_min
                    min_order_max
                  }
                  stock
                  product_status
                  image_url
                  is_eligible
                  message
                  submitted_count
                  pending_count
                }
                is_valid
                product_status
            }
        }
    }
}