query getMojitoEligibleSellerProduct($cid: Int!, $source: String!,
                      $rows: Int,
                      $start: Int,
                      $shop_id: String,
                      $q: String){
   getMojitoPostProductList(
     params:{cid:$cid, source:$source,
       shop_id:$shop_id, rows:$rows, start:$start, q:$q}
   ){
   header {
     total_data
   }
    data{
        products{
         id
         name
         image_url
         price
         department_id
         campaign{
           campaign_id
           original_price
           discount_percentage
           discounted_price
           original_price
           custom_stock
           original_custom_stock
           stock_sold_percentage
           product_map_status
           status_id
           admin_status
           max_order
         }
       }
    }
   }
}