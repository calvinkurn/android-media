<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="LayoutIconPullRefreshView">
        <attr name="color_type" format="enum">
            <enum name="white" value="0" />
            <enum name="green" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="DynamicChannelHeaderRevampView">
        <attr name="color_mode" format="enum">
            <enum name="normal" value="0" />
            <enum name="inverted" value="1" />
        </attr>
        <attr name="cta_mode" format="enum">
            <enum name="seeAll" value="0" />
            <enum name="reload" value="1" />
            <enum name="close" value="2" />
        </attr>
    </declare-styleable>
</resources>