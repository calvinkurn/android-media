<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#08000000" />
            <padding
                android:bottom="3px"
                android:left="3px"
                android:right="0px"
                android:top="0px" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="#10000000" />
            <padding
                android:bottom="2px"
                android:left="2px"
                android:right="0px"
                android:top="0px" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="#11000000" />
            <padding
                android:bottom="1px"
                android:left="1px"
                android:right="0px"
                android:top="0px" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="#13000000" />
            <padding
                android:bottom="1px"
                android:left="1px"
                android:right="0px"
                android:top="0px" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/white" />
        </shape>
    </item>
</layer-list>