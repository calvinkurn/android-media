<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">
    <com.tokopedia.empty_state.EmptyStateUnify
        android:id="@+id/error_state"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:unifyEmptyStateCTAFullWidth="true"
        app:unifyEmptyStateDescription="@string/wishlist_error_description"
        app:unifyEmptyStateImageDrawable="@drawable/ic_global_error_totopolis"
        app:unifyEmptyStateOrientation="horizontal"
        app:unifyEmptyStatePrimaryCTAText="@string/wishlist_try_again"
        app:unifyEmptyStateTitle="@string/default_title_error"
        app:unifyEmptyStateType="page"
        android:visibility="visible"
        tools:visibility="visible"/>
</FrameLayout>