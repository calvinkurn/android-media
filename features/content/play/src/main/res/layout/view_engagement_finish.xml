<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="@dimen/play_engagement_finish_width"
    android:layout_height="@dimen/play_engagement_finish_height"
    android:background="@drawable/bg_play_engagement"
    >

    <com.tokopedia.unifycomponents.ImageUnify
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:scaleType="fitStart"
        app:srcCompat="@drawable/bg_play_engagement_decor"
        />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/tv_engagement_finish_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:text="Game Selesai!"
        android:textColor="@color/Unify_Static_White"
        app:typographyType="body_3"
        app:typographyWeight="bold"
        />

</FrameLayout>