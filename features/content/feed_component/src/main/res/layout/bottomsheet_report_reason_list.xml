<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/report_option_text1"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/unify_space_12"
        android:text="@string/feed_component_report_option_text1"
        android:textColor="@color/Unify_N700_96"
        app:layout_constraintEnd_toStartOf="@id/report_option_text1_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:typographyType="heading_5" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/report_option_text1_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:icon_name="CHEVRON_RIGHT"
        app:layout_constraintBottom_toBottomOf="@id/report_option_text1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintTop_toTopOf="@id/report_option_text1" />

    <View
        android:id="@+id/divider1"
        android:layout_width="match_parent"
        android:layout_height="@dimen/feed_component_dp_0.5"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:background="@color/Unify_N75"
        app:layout_constraintStart_toStartOf="@id/report_option_text1"
        app:layout_constraintTop_toBottomOf="@id/report_option_text1" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/report_option_text2"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/unify_space_12"
        android:text="@string/feed_component_report_option_text2"
        android:textColor="@color/Unify_N700_96"
        app:layout_constraintEnd_toStartOf="@id/report_option_text2_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/report_option_text1"
        app:typographyType="heading_5" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/report_option_text2_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:icon_name="CHEVRON_RIGHT"
        app:layout_constraintBottom_toBottomOf="@id/report_option_text2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintTop_toTopOf="@id/report_option_text2" />

    <View
        android:id="@+id/divider2"
        android:layout_width="match_parent"
        android:layout_height="@dimen/feed_component_dp_0.5"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:background="@color/Unify_N75"
        app:layout_constraintStart_toStartOf="@id/report_option_text2"
        app:layout_constraintTop_toBottomOf="@id/report_option_text2" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/report_option_text3"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/unify_space_12"
        android:text="@string/feed_component_report_option_text3"
        android:textColor="@color/Unify_N700_96"
        app:layout_constraintEnd_toStartOf="@id/report_option_text3_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/report_option_text2"
        app:typographyType="heading_5" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/report_option_text3_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:icon_name="CHEVRON_RIGHT"
        app:layout_constraintBottom_toBottomOf="@id/report_option_text3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintTop_toTopOf="@id/report_option_text3" />

    <View
        android:id="@+id/divider3"
        android:layout_width="match_parent"
        android:layout_height="@dimen/feed_component_dp_0.5"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:background="@color/Unify_N75"
        app:layout_constraintStart_toStartOf="@id/report_option_text3"
        app:layout_constraintTop_toBottomOf="@id/report_option_text3" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/report_option_text4"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/unify_space_12"
        android:text="@string/feed_component_report_option_text4"
        android:textColor="@color/Unify_N700_96"
        app:layout_constraintEnd_toStartOf="@id/report_option_text4_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/report_option_text3"
        app:typographyType="heading_5" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/report_option_text4_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:icon_name="CHEVRON_RIGHT"
        app:layout_constraintBottom_toBottomOf="@id/report_option_text4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintTop_toTopOf="@id/report_option_text4" />

    <View
        android:id="@+id/divider4"
        android:layout_width="match_parent"
        android:layout_height="@dimen/feed_component_dp_0.5"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:background="@color/Unify_N75"
        app:layout_constraintStart_toStartOf="@id/report_option_text4"
        app:layout_constraintTop_toBottomOf="@id/report_option_text4" />

    <com.tokopedia.unifyprinciples.Typography
        android:id="@+id/report_option_text5"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/unify_space_12"
        android:layout_marginBottom="@dimen/spacing_lvl4"
        android:text="@string/feed_component_report_option_text5"
        android:textColor="@color/Unify_N700_96"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@id/report_option_text5_icon"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/report_option_text4"
        app:typographyType="heading_5" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/report_option_text5_icon"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:icon_name="CHEVRON_RIGHT"
        app:layout_constraintBottom_toBottomOf="@id/report_option_text5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1"
        app:layout_constraintTop_toTopOf="@id/report_option_text5" />

</androidx.constraintlayout.widget.ConstraintLayout>