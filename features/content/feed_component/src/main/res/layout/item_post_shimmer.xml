<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingBottom="@dimen/spacing_lvl5">

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer1"
        android:layout_width="@dimen/layout_lvl4"
        android:layout_height="@dimen/layout_lvl4"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/unify_space_12"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:loader_type="circle" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer2"
        android:layout_width="@dimen/layout_lvl8"
        android:layout_height="@dimen/feed_component_dp_10"
        android:layout_marginStart="@dimen/unify_space_12"
        app:layout_constraintBottom_toBottomOf="@id/shimmer1"
        app:layout_constraintStart_toEndOf="@id/shimmer1"
        app:layout_constraintTop_toTopOf="@id/shimmer1"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer3"
        android:layout_width="@dimen/unify_space_0"
        android:layout_height="@dimen/unify_space_0"
        android:layout_marginTop="@dimen/unify_space_12"
        app:layout_constraintDimensionRatio="1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shimmer1"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer4"
        android:layout_width="@dimen/layout_lvl3"
        android:layout_height="@dimen/layout_lvl3"
        android:layout_marginTop="@dimen/unify_space_12"
        app:layout_constraintStart_toStartOf="@id/shimmer1"
        app:layout_constraintTop_toBottomOf="@id/shimmer3"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer5"
        android:layout_width="@dimen/layout_lvl3"
        android:layout_height="@dimen/layout_lvl3"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/unify_space_12"
        app:layout_constraintStart_toEndOf="@id/shimmer4"
        app:layout_constraintTop_toBottomOf="@id/shimmer3"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer6"
        android:layout_width="@dimen/layout_lvl3"
        android:layout_height="@dimen/layout_lvl3"
        android:layout_marginTop="@dimen/unify_space_12"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shimmer3"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer7"
        android:layout_width="@dimen/layout_lvl9"
        android:layout_height="@dimen/feed_component_dp_10"
        android:layout_marginTop="@dimen/spacing_lvl4"
        app:layout_constraintStart_toStartOf="@id/shimmer1"
        app:layout_constraintTop_toBottomOf="@id/shimmer4"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer8"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="@dimen/feed_component_dp_10"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl4"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shimmer7"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer9"
        android:layout_width="@dimen/layout_lvl0"
        android:layout_height="@dimen/feed_component_dp_10"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shimmer8"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer10"
        android:layout_width="@dimen/feed_component_dp_200"
        android:layout_height="@dimen/feed_component_dp_10"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/spacing_lvl3"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shimmer9"
        app:loader_type="rect" />

    <com.tokopedia.unifycomponents.LoaderUnify
        android:id="@+id/shimmer11"
        android:layout_width="@dimen/layout_lvl8"
        android:layout_height="@dimen/feed_component_dp_10"
        android:layout_marginStart="@dimen/spacing_lvl4"
        android:layout_marginTop="@dimen/unify_space_12"
        android:layout_marginEnd="@dimen/spacing_lvl4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/shimmer10"
        app:loader_type="rect" />

</androidx.constraintlayout.widget.ConstraintLayout>