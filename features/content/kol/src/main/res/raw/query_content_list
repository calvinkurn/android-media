query ContentList($limit: Int, $cursor: String, $shopID: Int, $userID: Int, $source: String){
  feed_content_post(limit:$limit, cursor:$cursor, shopID:$shopID, userID:$userID, source:$source){
    posts{
      id
      createTime
      description
      content{
        type
        URL
        tags{
          id
          type
          link
          captionEng
          captionInd
        }
      }
      author{
        id
        name
        thumbnail
        url
      }
      interaction{
        isLiked
        commentCount
        likeCount
        showComment
        showLike
      }
    }
    lastCursor
    error
  }
}