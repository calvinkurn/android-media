<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RatingBarReview">
        <attr name="numstars" format="integer" />
        <attr name="rating" format="integer" />
        <attr name="progress_bar_percentage" format="float"/>
        <attr name="total_review" format="integer"/>
    </declare-styleable>

    <declare-styleable name="NestedScrollableHost">
        <attr name="lockScroll" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="ReviewProductItemFilterView">
        <attr name="counter_rating" format="integer" />
        <attr name="isActive" format="boolean"/>
        <attr name="isAll" format="boolean"/>
        <attr name="isWithPhoto" format="boolean"/>
    </declare-styleable>

    <declare-styleable name="FeedFloatingButton">
        <attr name="fab_text" format="string" />
    </declare-styleable>

    <declare-styleable name="PlayLikeBubbleView">
        <attr name="bouncing" format="boolean"/>
        <attr name="maxShot" format="integer"/>
    </declare-styleable>
</resources>