<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_feed"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/content_common_space_1">

    <com.tokopedia.unifycomponents.ImageUnify
        android:id="@+id/image_content"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:adjustViewBounds="true"
        android:clickable="true"
        android:focusable="true"
        android:foreground="?attr/selectableItemBackground"
        android:scaleType="centerCrop"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="H,1:1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:unify_image_type="rect"
        tools:ignore="UnusedAttribute" />

    <com.tokopedia.iconunify.IconUnify
        android:id="@+id/icon_type"
        android:layout_width="22dp"
        android:layout_height="22dp"
        android:layout_gravity="top|end"
        android:layout_marginHorizontal="@dimen/unify_font_8"
        android:layout_marginTop="6dp"
        app:icon_color_light_enable="@color/Unify_Static_White"
        app:icon_color_night_enable="@color/Unify_Static_White"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:icon_name="SELECT_MULTIPLE" />

</androidx.constraintlayout.widget.ConstraintLayout>