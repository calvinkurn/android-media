query ProfileHeader ($userIDTarget: Int!) {
    bymeProfileHeader(userIDTarget: $userIDTarget) {
       profile{
          id
          name
          isKol
          avatar
          affiliateName
          link
          isFollowed
          totalFollowing{
            formatted
            number
          }
          totalFollower{
            formatted
            number
          }
       }
    }
    affiliatePostQuota() {
        formatted
        number
        format
    }
}
