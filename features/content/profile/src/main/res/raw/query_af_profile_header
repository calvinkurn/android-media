query ProfileHeader ($userIDTarget: Int!) {
    bymeProfileHeader(userIDTarget: $userIDTarget) {
       profile{
          id
          name
          isKol
          isAffiliate
          avatar
          affiliateName
          link
          isFollowed
          totalFollowing{
            formatted
            number
          }
          totalFollower{
            formatted
            number
          }
       }
    }
}
