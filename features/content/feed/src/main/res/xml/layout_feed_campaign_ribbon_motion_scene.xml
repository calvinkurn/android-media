<KeyFrameSet>
    <KeyPosition
        app:motionTarget="@+id/ty_feed_campaign_ribbon_title"
        app:framePosition="99"
        app:keyPositionType="parentRelative"
        app:percentY="-1" />
</KeyFrameSet>
    </Transition>
    <Transition
        app:constraintSetEnd="@+id/availability_state"
        app:constraintSetStart="@+id/initial_title_with_icon"
        app:duration="600">
        <KeyFrameSet></KeyFrameSet>
    </Transition>
    <Transition
        app:constraintSetEnd="@+id/initial_title_with_timer_and_icon"
        app:constraintSetStart="@+id/initial_title_with_icon"
        app:duration="600">
        <KeyFrameSet></KeyFrameSet>
    </Transition>
    <Transition
        app:constraintSetEnd="@+id/start_in_state"
        app:constraintSetStart="@+id/initial_title_with_icon"
        app:duration="600">
        <KeyFrameSet>

            <KeyPosition
                app:framePosition="1"
                app:keyPositionType="parentRelative"
                app:motionTarget="@+id/timer_feed_campaign_ribbon"
                app:percentX=".4" />
