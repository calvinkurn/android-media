query onboardingRecommendations(
  $ids: [Int!]!,
  $cursor: String,
  $limit: Int,
  $source: String) {
  feedUserOnboardingRecommendations(
    ids: $ids,
    cursor: $cursor,
    limit: $limit,
    source: $source) {
    meta {
      assets {
        title
        instruction
        buttonCta
        shopDescription
        profileDescription
      }
      minFollowed
      nextCursor
      source
    }
    data {
      header {
        avatar
        avatarTitle
        avatarWeblink
        avatarApplink
        avatarBadgeImage
        avatarDescription
        followCta {
          textTrue
          textFalse
        }
      }
      media {
        id
        type
        appLink
        webLink
        thumbnail
        thumbnailLarge
      }
    }
  }
}