query FeedDetail($userID: Int!, $detailID: String!, $pageDetail: Int!, $limitDetail: Int!) {
  feed(userID: $userID, detailID: $detailID, pageDetail: $pageDetail, limitDetail: $limitDetail) {
    __typename
    data {
      __typename
      id
      create_time
      type
      cursor
      source {
        __typename
        type
        shop {
          __typename
          id
          name
          avatar
          isOfficial
          isGold
          badge
          url
          shopLink
          shareLinkDescription
          shareLinkURL
        }
      }
      content {
        __typename
        type
        total_product
        products {
          __typename
          id
          name
          price
          price_original
          price_original_int
          image
          wholesale {
            __typename
            qty_min_fmt
          }
          tags {
            linkType
            text
            type
          }
          freereturns
          preorder
          cashback
          url
          productLink
          wishlist
          rating
          countReview
        }
        status_activity
      }
      meta {
        __typename
        has_next_page
      }
    }
    token
  }
}
