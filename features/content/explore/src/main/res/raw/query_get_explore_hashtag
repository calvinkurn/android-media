query GetExploreData ($limit: Int!, $cursor: String!, $idcategory: Int!, $search: String!){
  get_discovery_kol_data(limit: $limit, cursor: $cursor, idcategory: $idcategory, search: $search) {
    error
    postKol {
      id
      description
      content {
        type
        imageurl
      }
      userName
      userInfo
      userPhoto
      userUrl
      userId
    }
    lastCursor
    }
}