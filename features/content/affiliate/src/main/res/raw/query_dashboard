query GetDashboard($limit: Int!) {
  affiliatedProduct(limit:$limit) {
    products {
      id
      image
      name
      isActive
      totalSold
      totalClick
      commission
    }
    pagination {
      nextCursor
    }
  }
  affiliateStats() {
    totalCommission
    productClick
    profileView
    productSold
  }
  affiliateCheck() {
    isAffiliate
    status
  }
  affiliatePostQuota() {
    formatted
    number
  }
}