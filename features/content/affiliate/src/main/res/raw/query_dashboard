query GetDashboard() {
  affiliatedProduct {
    products {
      id
      image
      name
      isActive
      totalSold
      totalClick
      commission
      productCommission
    }
    pagination{
      nextCursor
    }
  }
  affiliateStats{
    totalCommission
    productClick
    profileView
    productSold
  }
  affiliateCheck{
    isAffiliate
    status
  }
  affiliatePostQuota{
    formatted
    number
  }
  balance {
    seller_usable
    buyer_usable
  }
}