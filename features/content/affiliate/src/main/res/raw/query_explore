query getExploreAffiliate($keyword: String!, $cursor: String!, $filterKey: String!, $filterValue: String!, $sortKey: String!, $sortAsc: Boolean! ) {
  topadsExploreAffiliateProduct(keyword:$keyword,filter:[{key:$filterKey,value:$filterValue}],nextCursor:$cursor,sort:{key:$sortKey,asc:$sortAsc}) {
    products {
      id
      image
      name
      isActive
      totalSold
      totalClick
      commission
    }
    pagination{
      nextCursor
    }
  }
}