query GetDashboard($userID:Int!) {
  affiliateStats{
    totalCommission
    productClick
    profileView
    productSold
    productCount
  }
  affiliateCheck{
    isAffiliate
    status
  }
  affiliatePostQuota{
    formatted
    number
  }
  balance {
    seller_usable
    buyer_usable
  }
  bymeProfileHeader(userIDTarget:$userID) {
    profile {
      avatar
      link
      affiliateName
    }
  }
}