query GetAffiliatedProductTxList($affiliatedProductID: String, $next: String){
   affiliatedProductTxList(affiliatedProductID: $affiliatedProductID , next: $next) {
       history {
         itemSent
         affCommission
         affCommissionFormatted
         affInvoice
         affInvoiceURL
         txTimeFormatted
         txTime
         tkpdInvoice
         tkpdInvoiceURL
         tkpdCommission
         tkpdCommissionFormatted
         netCommission
         netCommissionFormatted
       }
       next
       has_next
   }
}