query getExploreAffiliate($keyword: String, $nextCursor: String, $filter: [TopadsExploreAffiliateFilter]) {
  topadsExploreAffiliateProduct(keyword:$keyword, nextCursor:$nextCursor, filter:$filter){
    products {
      adId
      productId
      image
      name
      commission
    }
    pagination{
      nextCursor
    }
  }
}