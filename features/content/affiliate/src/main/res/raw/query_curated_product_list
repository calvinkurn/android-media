query GetCuratedProductList($type: Int, $cursor: String, $sort: Int) {
  affiliatedProduct(type: $type, cursor:$cursor, sortBy: $sort) {
    products {
      id
      image
      name
      isActive
      totalSold
      totalClick
      commission
      productCommission
      createPostAppLink
      productRating
      reviewCount
    }
    pagination {
      nextCursor
    }
    subtitles {
      key
      text
    }
  }
}