query liveReport($channelID: String!){
  broadcasterReportLiveSummaries(channelID: $channelID){
    channel{
      channelID
      metrics{
    	addToCart
    	removeFromCart
    	wishList
    	removeWishList
    	paymentVerified
    	followShop
    	unFollowShop
    	likeChannel
    	unLike
    	visitShop
    	visitPDP
    	visitChannel
      }
    }
  }
}
