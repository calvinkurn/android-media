<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical">


        <TextView
            android:id="@+id/no_result"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/title_no_result"
            android:visibility="gone" />

        <com.tokopedia.core.customwidget.SwipeToRefresh
            android:id="@+id/swipe_refresh_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <com.tokopedia.core.customView.SimpleListView
                android:id="@+id/prod_list"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:cacheColorHint="#FF0000FF"
                android:checkMark="?android:attr/listChoiceIndicatorMultiple"
                android:divider="@android:color/transparent"
                android:listSelector="@android:color/transparent" />

        </com.tokopedia.core.customwidget.SwipeToRefresh>

        <ImageView
            android:id="@+id/blur_image"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:choiceMode="singleChoice"
            android:divider="@android:color/transparent"
            android:dividerHeight="0dp" />

        <!--<com.tkpd.library.ui.floatbutton.FabSpeedDial-->
            <!--android:id="@+id/fab_speed_dial"-->
            <!--android:layout_width="wrap_content"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_alignParentRight="true"-->
            <!--android:layout_alignParentBottom="true"-->
            <!--app:touchGuardDrawable="@drawable/background_blur"-->
            <!--app:fabGravity="bottom_end"-->
            <!--app:fabMenu="@menu/menu_add_product_instoped"-->
            <!--app:fabBackgroundTint="@color/green_500"-->
            <!--app:miniFabBackgroundTint="@color/mini_fab_title_background_tint"-->
            <!--app:miniFabDrawableTint="@android:color/white"-->
            <!--app:miniFabTitleTextColor="@android:color/white" />-->

        <android.support.design.widget.FloatingActionButton
            android:id="@+id/fab"
            android:src="@drawable/ic_add_black_48dp_00"
            app:fabSize="normal"
            app:backgroundTint="@color/tkpd_main_green"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/activity_horizontal_margin"
            android:layout_alignParentBottom="true"
            android:layout_alignParentRight="true"
            android:layout_alignParentEnd="true"
            app:layout_anchorGravity="bottom|right|end"/>

    <!--<include layout="@layout/fab_button" />-->

</RelativeLayout>