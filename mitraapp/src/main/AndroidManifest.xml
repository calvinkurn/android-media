<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.mitra">

    <application
        android:name=".MitraApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true">
        <activity
            android:name="com.tokopedia.mitra.welcome.MitraWelcomeActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.White">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".digitalcategory.presentation.activity.MitraDigitalCategoryActivity"
            android:theme="@style/Theme.White" />
        <activity android:name=".digitalcategory.presentation.activity.MitraDigitalChooserActivity"
            android:theme="@style/Theme.White" />
        <activity android:name=".homepage.activity.MitraParentHomepageActivity" />
        <activity android:name=".applink.MitraDeeplinkActivity" />
    </application>

</manifest>