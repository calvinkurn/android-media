query GetProfileKol($userID: Int!, $limit: Int!, $cursor: String) {
    get_profile_kol_data(userID: $userID, cursor: $cursor, limit: $limit) {
		error
		profileKol {
			followers
			followed
			id
			info
			bio
			name
			photo
		}
		postKol {
			id
			description
			commentCount
			likeCount
			isLiked
			isFollowed
			createTime
			userName
			userPhoto
			userInfo
			content {
				imageurl
				tags {
					id
					type
					url
					link
					price
					caption
				}
			}
		}
		lastCursor
	}
}