query SearchProduct(
 $params: String
) {
searchProduct(params: $params) {
  query
   source
   shareUrl
   isFilter
  count
  redirection {
     redirect_url
     department_id
   }
   suggestion {
     currentKeyword
     suggestion
     suggestionCount
     instead
     insteadCount
     text
     query
   }
   products {
     id
     name
     childs
     url
     image_url
     image_url_700
     price
     whole_sale_price {
      quantity_min
      quantity_max
      price
     }
     courier_count
     condition
     category_id
     category_name
     category_breadcrumb
     department_id
     department_name
     labels {
      title
      color
     }
     badges {
       title
       image_url
     }
     is_featured
     rating
     count_review
     original_price
     discount_expired_time
     discount_start_time
     discount_percentage
     sku
     stock
     ga_key
     is_preorder
     shop {
       id
       name
       url
       is_gold_shop
       location
       city
       reputation
       clover
       is_official
     }
   }
   catalogs {
     id
     name
     price
     price_min
     price_max
     price_raw
     price_min_raw
     price_max_raw
     count_product
     description
     image_url
     url
     department_id
   }
 }
}