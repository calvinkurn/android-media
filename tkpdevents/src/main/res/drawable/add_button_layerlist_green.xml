<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <size
                android:width="24dp"
                android:height="24dp" />
            <solid android:color="@color/green_nob" />
            <corners android:radius="4dp" />
        </shape>
    </item>

    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="8dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <size
                android:width="24dp"
                android:height="24dp" />
            <solid android:color="@color/green_nob" />
            <corners android:radius="0dp" />
        </shape>
    </item>

    <item android:gravity="center">
        <shape android:shape="rectangle">
            <size
                android:width="10dp"
                android:height="2dp" />
            <solid android:color="@color/white" />

        </shape>
    </item>

    <item android:gravity="center">
        <shape android:shape="rectangle">
            <size
                android:width="2dp"
                android:height="10dp" />
            <solid android:color="@color/white" />

        </shape>
    </item>
</layer-list>