<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.tokopedia.tokocash">

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">
        <meta-data android:name="android.support.VERSION" android:value="26.1.0"
            tools:replace="android:value" />

        <activity
            android:name=".qrpayment.presentation.activity.SuccessPaymentQRActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".qrpayment.presentation.activity.NominalQrPaymentActivity"
            android:label="@string/title_input_nominal"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent"
            android:windowSoftInputMode="adjustResize" />
        <activity
            android:name=".activation.presentation.activity.ActivateTokoCashActivity"
            android:label="@string/tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".activation.presentation.activity.SuccessActivateTokoCashActivity"
            android:label="@string/tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".historytokocash.presentation.activity.HistoryTokoCashActivity"
            android:label=""
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".historytokocash.presentation.activity.HomeTokoCashActivity"
            android:label="@string/tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".historytokocash.presentation.activity.DetailTransactionActivity"
            android:label="@string/tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".historytokocash.presentation.activity.WaitingTransactionActivity"
            android:label="@string/tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".historytokocash.presentation.activity.HelpHistoryDetailActivity"
            android:label="@string/tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".historytokocash.presentation.activity.MoveToSaldoActivity"
            android:label="@string/tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".historytokocash.presentation.activity.ThankYouMoveToSaldoActivity"
            android:label="@string/tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
        <activity
            android:name=".accountsetting.presentation.activity.AccountSettingActivity"
            android:label="@string/tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White.PopupTransparent" />
    </application>

</manifest>
