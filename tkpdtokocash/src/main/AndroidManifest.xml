<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tokopedia.tokocash" >

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <activity
            android:name=".historytokocash.presentation.activity.HistoryTokoCashActivity"
            android:label="@string/title_menu_history_tokocash"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White"/>
        <activity android:name=".qrpayment.presentation.activity.CustomScannerTokoCashActivity"
            android:label="@string/title_scan_qr"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White"/>
        <activity android:name=".qrpayment.presentation.activity.SuccessPaymentQRActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White"/>
        <activity android:name=".qrpayment.presentation.activity.NominalQrPaymentActivity"
            android:windowSoftInputMode="adjustResize"
            android:label="@string/title_input_nominal"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.White"/>
    </application>

</manifest>
