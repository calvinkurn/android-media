<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/main_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="center"
    android:layout_marginBottom="@dimen/margin_small"
    android:background="@drawable/cards_ui_2"
    android:orientation="vertical" >

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="@dimen/margin_small" >

        <LinearLayout
            android:id="@+id/error_area"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/margin_small"
            android:background="#FFADAD"
            android:orientation="vertical"
            android:padding="@dimen/padding_small"
            android:visibility="gone" >

            <TextView
                android:id="@+id/error1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/margin_vs"
                android:textColor="@color/black"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/error2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:textColor="@color/label_color" />
        </LinearLayout>

        <TextView
            android:id="@+id/shop_name"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/error_area"
            android:maxWidth="@dimen/store_name"
            android:singleLine="true"
            android:text="Shop Name Ellipsis"
            android:textColor="@color/black"
            android:textSize="@dimen/fontm"
            android:textStyle="bold" />

        <LinearLayout
            android:id="@+id/edit_delete_area"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_below="@id/error_area"
            android:layout_centerVertical="true"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/edit"
                android:layout_width="@dimen/icon_prod_pad"
                android:layout_height="@dimen/icon_prod_pad"
                android:layout_marginRight="@dimen/med_dp"
                android:src="@drawable/ic_action_edit"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/delete"
                android:layout_width="@dimen/icon_prod_pad"
                android:layout_height="@dimen/icon_prod_pad"
                android:src="@drawable/ic_action_delete"
                android:visibility="gone" />

            <!-- <TextView -->
            <!-- android:shopId="@+shopId/edit" -->
            <!-- android:layout_width="wrap_content" -->
            <!-- android:layout_height="wrap_content" -->
            <!-- android:text="@string/title_edit_prod" -->
            <!-- android:layout_marginRight="@dimen/margin_small" -->
            <!-- android:textSize="@dimen/fonts" -->
            <!-- /> -->


            <!-- <TextView -->
            <!-- android:shopId="@+shopId/delete" -->
            <!-- android:layout_width="wrap_content" -->
            <!-- android:layout_height="wrap_content" -->
            <!-- android:text="@string/title_delete" -->
            <!-- android:textSize="@dimen/fonts" /> -->
        </LinearLayout>
    </RelativeLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:background="@color/greyish_bg" />
    <!-- <LinearLayout -->
    <!-- android:shopId="@+shopId/edit_delete_area" -->
    <!-- android:layout_height="match_parent" -->
    <!-- android:layout_width="wrap_content" -->
    <!-- android:orientation="horizontal" -->
    <!-- android:padding="10dp" > -->


    <!-- <TextView -->
    <!-- android:shopId="@+shopId/edit" -->
    <!-- android:layout_height="wrap_content" -->
    <!-- android:layout_width="160dp" -->
    <!-- android:text="@string/title_edit_prod" -->
    <!-- android:padding="10dp"/> -->


    <!-- <TextView -->
    <!-- android:shopId="@+shopId/delete" -->
    <!-- android:layout_height="wrap_content" -->
    <!-- android:layout_width="160dp" -->
    <!-- android:text="@string/title_delete" -->
    <!-- android:padding="10dp"/> -->


    <!-- </LinearLayout> -->

    <LinearLayout
        android:id="@+id/listview_prod"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical" >

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/greyish_bg" />

        <RelativeLayout
            android:id="@+id/detail_info_but"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/border_cart_1"
            android:paddingBottom="@dimen/padding_vs"
            android:paddingTop="@dimen/padding_vs" >

            <TextView
                android:id="@+id/text_detail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_centerVertical="true"
                android:layout_marginLeft="@dimen/margin_small"
                android:text="@string/title_details_info"
                android:textColor="@color/label_color"
                android:textSize="@dimen/fonts"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/chevron_sign"
                android:layout_width="@dimen/large_dp"
                android:layout_height="@dimen/large_dp"
                android:layout_alignParentRight="true"
                android:layout_marginRight="@dimen/margin_small"
                android:background="@drawable/overflow_selected"
                android:padding="@dimen/padding_vs"
                android:src="@drawable/ic_chevron_down" />
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0.5dp"
            android:background="@color/greyish_bg" />

        <RelativeLayout
            android:id="@+id/detail_info"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/margin_small"
            android:paddingLeft="@dimen/margin_small"
            android:paddingRight="@dimen/margin_small" >

            <LinearLayout
                android:id="@+id/list_detail_transaction"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical" >

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:text="@string/title_shipping_to"
                    android:textColor="@color/label_color"
                    android:textSize="@dimen/fonts" />

                <TextView
                    android:id="@+id/shipping_address"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_marginBottom="@dimen/margin_vs"
                    android:text="Address Dest."
                    android:textColor="@color/black"
                    android:textSize="@dimen/fonts" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@id/shipping_address"
                    android:text="@string/title_shipping_agency"
                    android:textColor="@color/label_color"
                    android:textSize="@dimen/fonts" />

                <TextView
                    android:id="@+id/shipping_agency"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_below="@id/shipping_address"
                    android:layout_marginBottom="@dimen/margin_vs"
                    android:text="JNE - YES"
                    android:textColor="@color/black"
                    android:textSize="@dimen/fonts" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@id/shipping_agency"
                    android:text="@string/title_use_insurance"
                    android:textColor="@color/label_color"
                    android:textSize="@dimen/fonts" />

                <!-- <Spinner -->
                <!-- android:shopId="@+shopId/spinner_insurance" -->
                <!-- android:layout_height="wrap_content" -->
                <!-- android:layout_width="wrap_content" -->
                <!-- android:layout_alignParentRight="true" -->
                <!-- style="@android:style/Widget.Holo.Light.Spinner" -->
                <!-- android:layout_marginBottom="@dimen/margin_vs" -->
                <!-- android:layout_below="@shopId/shipping_agency" -->
                <!-- /> -->

                <TextView
                    android:id="@+id/insurance"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_below="@id/shipping_agency"
                    android:layout_marginBottom="@dimen/margin_vs"
                    android:text="Tidak"
                    android:textColor="@color/black"
                    android:textSize="@dimen/fonts" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentLeft="true"
                    android:layout_below="@id/insurance"
                    android:text="@string/title_left_stock"
                    android:textColor="@color/label_color"
                    android:textSize="@dimen/fonts" />

                <!-- <Spinner -->
                <!-- android:shopId="@+shopId/spinner_remaining_stock" -->
                <!-- android:layout_height="wrap_content" -->
                <!-- android:layout_width="wrap_content" -->
                <!-- android:layout_alignParentRight="true" -->
                <!-- android:layout_below="@shopId/spinner_insurance" -->
                <!-- /> -->
                <!-- <Spinner -->
                <!-- android:shopId="@+shopId/spinner_remaining_stock" -->
                <!-- android:layout_width="wrap_content" -->
                <!-- android:layout_height="wrap_content" -->
                <!-- android:textSize="@dimen/fonts" -->
                <!-- style="@android:style/Widget.Holo.Light.Spinner" -->
                <!-- android:layout_alignParentRight="true" -->
                <!-- android:layout_below="@shopId/spinner_insurance" -->
                <!-- android:layout_marginBottom="@dimen/margin_vs" -->
                <!-- /> -->

                <TextView
                    android:id="@+id/remaining_stock"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentRight="true"
                    android:layout_below="@id/insurance"
                    android:text="Batalkan Pemesanan"
                    android:textColor="@color/black"
                    android:textSize="@dimen/fonts" />

                <!-- android:textSize="@dimen/fonts" -->
                <!-- android:textColor="@color/href_link" -->


                <!-- android:layout_marginBottom="@dimen/margin_small" -->


                <!-- android:popupBackground="@color/white" -->

            </LinearLayout>

            <View
                android:id="@+id/list_separator"
                android:layout_width="match_parent"
                android:layout_height="0.5dp"
                android:layout_below="@+id/list_detail_transaction"
                android:layout_marginBottom="@dimen/margin_small"
                android:layout_marginTop="@dimen/margin_small"
                android:background="@color/greyish_bg" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/list_separator"
                android:text="@string/title_total_item"
                android:textColor="@color/label_color"
                android:textSize="@dimen/fonts" />

            <TextView
                android:id="@+id/total_weight"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@id/list_separator"
                android:layout_marginBottom="@dimen/margin_vs"
                android:text="2"
                android:textColor="@color/black"
                android:textSize="@dimen/fonts" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/total_weight"
                android:text="@string/title_sub_total"
                android:textColor="@color/label_color"
                android:textSize="@dimen/fonts" />

            <TextView
                android:id="@+id/sub_total"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@id/total_weight"
                android:layout_marginBottom="@dimen/margin_vs"
                android:text="Rp 20.000,-"
                android:textColor="@color/black"
                android:textSize="@dimen/fonts" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/sub_total"
                android:text="@string/title_shipping_cost"
                android:textColor="@color/label_color"
                android:textSize="@dimen/fonts" />

            <TextView
                android:id="@+id/shipping_cost"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@id/sub_total"
                android:layout_marginBottom="@dimen/margin_vs"
                android:text="Rp 20.000,-"
                android:textColor="@color/black"
                android:textSize="@dimen/fonts" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/shipping_cost"
                android:layout_marginBottom="@dimen/margin_vs"
                android:text="@string/title_insurance_cost"
                android:textColor="@color/label_color"
                android:textSize="@dimen/fonts" />

            <TextView
                android:id="@+id/insurance_price"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentRight="true"
                android:layout_below="@id/shipping_cost"
                android:layout_marginBottom="@dimen/margin_vs"
                android:text="Rp 20.000,-"
                android:textColor="@color/black"
                android:textSize="@dimen/fonts" />
        </RelativeLayout>
    </LinearLayout>

    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/margin_small"
        android:padding="@dimen/padding_small" >

        <TextView
            android:id="@+id/title_total"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentLeft="true"
            android:text="@string/title_total"
            android:textColor="@color/black"
            android:textSize="@dimen/fonts"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/total_price"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:text="Rp 20.000,-"
            android:textColor="@color/black"
            android:textSize="@dimen/fonts"
            android:textStyle="bold" />
    </RelativeLayout>

</LinearLayout> <!-- <?xml version="1.0" encoding="utf-8"?> -->
