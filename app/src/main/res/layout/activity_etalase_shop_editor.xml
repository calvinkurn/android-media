<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".EtalaseShopEditor">

    <include
        android:id="@+id/include_no_result"
        layout="@layout/error_no_result"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="gone" />

    <com.tokopedia.tkpd.customadapter.LazyListView
        android:id="@+id/listview_etalase"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/margin_small"
        android:divider="@android:color/transparent"
        android:dividerHeight="@dimen/margin_small" />
</LinearLayout>