query wishlist($user_id: Int, $page: Int, $count: Int){
  wishlist(user_id: $user_id, page: $page, count: $count){

      count
      has_next_page
      total_data
      items {
        id
        name
        url
        image
        price
        price_formatted
        prev_price
        minimum_order
        wholesale_price {
          minimum
          maximum
          price
        }
        condition
        shop {
          id
          name
          url
          reputation {
            score
            set
            level
            image
          }
          official_store
          gold_merchant
          status
          location
        }
        badges {
          title
          image_url
        }
        labels {
          title
          color
        }
        available
        status
        preorder
        rating
        review_count
        wishlist
        default_child_id
        is_ncf
      }
      pagination {
        next_url
      }
  }
}
