query wishlist($user_id: Int, $page: Int, $count: Int){
  wishlist(user_id: $user_id, page: $page, count: $count){
    count
    total_data
    has_next_page
    items{
      id
      name
      url
      image_url
      raw_price
      price
      prev_price
      available
      status
      condition
      preorder
      rating
      review_count
      minimum_order
      shop {
        id
        shop_name
        shop_url
        name
        url
        location
        official_store
        gold_merchant
        status
      }
      wishlist
    }
  }
}
