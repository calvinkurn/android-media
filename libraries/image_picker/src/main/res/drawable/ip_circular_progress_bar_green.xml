<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/bg_circular_progress">
        <shape
            android:shape="ring"
            android:thickness="@dimen/dp_3"
            android:useLevel="false">

            <solid android:color="@color/Unify_N100"/>
        </shape>
    </item>

    <item android:id="@+id/circular_progress">
        <shape
            android:shape="ring"
            android:thickness="@dimen/dp_3"
            android:useLevel="true">

            <solid android:color="@color/Unify_G400"/>
        </shape>
    </item>


</layer-list>