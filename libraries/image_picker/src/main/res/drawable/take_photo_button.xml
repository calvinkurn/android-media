<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:left="@dimen/dp_4" android:right="@dimen/dp_4"
        android:top="@dimen/dp_4"
        android:bottom="@dimen/dp_4">
        <selector>
            <item android:state_pressed="true" >
                <shape
                    android:shape="oval">
                    <solid android:color="@color/white_65" />
                </shape>
            </item>
            <item>
                <shape
                    android:shape="oval">
                    <solid android:color="#F0757575" />
                </shape>
            </item>
        </selector>

    </item>

    <item>
        <shape
            android:shape="oval">
            <solid android:color="@android:color/transparent" />
            <stroke android:color="@color/white_65" android:width="@dimen/dp_4" />
        </shape>
    </item>


</layer-list>
