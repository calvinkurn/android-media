query flightSuggestion($input: String!) {
    flightSuggestion(input: $input) {
        suggestions {
            airports {
                id
                name {
                  key
                  value
                }
            }
            cityName {
                key
                value
            }
            cityID
            code
            countryID
            countryName {
                key
                value
            }
            name {
                key
                value
            }
        }
	}
}