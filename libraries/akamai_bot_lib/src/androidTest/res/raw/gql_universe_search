query universe_search($navsource: String , $shopID: Int, $q: String ){
    universe_search(navsource: $navsource, shopID : $shopID,q: $q){
            data{
                id
                name
                items {
                    location
                    imageURI
                    applink
                    url
                    keyword
                    recom
                    sc
                    isOfficial
                    post_count
                    affiliate_username
                }
            }
       }
}