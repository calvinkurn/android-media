mutation addShopLocation($name:String!, $address:String!, $districtId:Int!, $cityId:Int!,
                         $stateId:Int!, $postalCode:Int!, $email:String, $phone:String, $fax:String){
  addShopLocation(input: {
      name: $name,
      address: $address,
      districtId: $districtId,
      cityId: $cityId,
      stateId: $stateId,
      postalCode: $postalCode,
      email: $email,
      phone: $phone,
      fax: $fax
    }) {
      success
      message
    }
}
