query umrahTravelAgentBySlug($slugName:String!){
  umrahTravelAgentBySlug(slugName : $slugName){
    id
    slugName
    name
    location
    address
    phone
    email
    logoUrl
    permissionOfHajj
    permissionOfUmrah
    pilgrimsPerYear
    establishedSince
    description
    products{
        id
        title
        banner
        slugName
        originalPrice
        slashPrice
        downPaymentPrice
        durationDays
        departureDate
        returningDate
        ui{
              travelDates
              travelDurations
              hotelStars
              variant
              transitCity
              availableSeat
            }
        airlines{
              name
              date
              departureCity{
                name
              }
              arrivalCity{
                name
              }
              logoUrl
            }
    }
    ui {
        establishedSince
        pilgrimsPerYear
    }
  }
}