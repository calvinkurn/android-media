{
  "readme": "Digital checkout default flow, without fintech product and subscription",
  "mode": "exact",
  "query": [
    {
      "event": "begin_checkout",
      "eventCategory": "digital - checkout",
      "eventAction": "view checkout",
      "eventLabel": "{{.+ - .+}}",
      "checkout_step": "1",
      "checkout_option": "cart page loaded",
      "currentSite": "tokopediadigital",
      "sessionIris": "{{.+}}",
      "items": [
        {
          "cart_id": "{{.+}}",
          "quantity": "{{\\d*.\\d*}}",
          "category_id": "{{.*}}",
          "item_id": "{{.+}}",
          "item_name": "{{.+}}",
          "item_brand": "{{.+}}",
          "item_variant": "{{.+}}",
          "item_category": "{{.+}}",
          "price": "{{\\d*.\\d*|\\d.\\d+E\\d}}",
          "shop_id": "{{.+}}",
          "shop_name": "{{.+}}",
          "shop_type": "{{.+}}"
        }
      ]
    }
  ]
}