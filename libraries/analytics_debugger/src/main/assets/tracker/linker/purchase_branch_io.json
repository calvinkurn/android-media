{
  "readme": "Purchase IO Tracker",
  "mode": "exact",
  "query": [
    {
      "buoList": [],
      "customProperties": {
        "nameValuePairs": {
          "paymentID": "{{\\w+}}",
          "productType": "{{\\w+}}",
          "userId": "{{\\w+}}",
          "new_buyer": "{{true|false}}",
          "monthly_new_buyer": "{{true|false}}"
        }
      },
      "eventName": "marketplace_first_txn",
      "isStandardEvent": "{{true|false}}",
      "standardProperties": {
        "nameValuePairs": {
          "transaction_id": "{{\\d+}}",
          "currency": "{{\\w+}}",
          "shipping": "{{\\d+}}",
          "revenue": "{{\\d+}}"
        }
      }
    }
  ]
}
