{
  "readme": "Product Detail - click add to basket login",
  "mode": "exact",
  "query": [
    {
      "eventCategory": "product detail page",
      "eventAction": "impression - modular component",
      "event": "view_item",
      "categoryId": "{{productId : \\d{1,}}}",
      "productId": "{{\\d{1,}}}",
      "sessionIris": "{{.*}}",
      "eventLabel": "{{.*}}",
      "layout": "{{layout:.*;catName:.*;catId:\\d{1,}}}",
      "component": "{{comp:.*;temp:.*;elem:.*;cpos:\\d{1,}}};",
      "promotions": [
        {
          "item_id": "{{.*}}",
          "creative_url": "{{.*}}",
          "creative_name": "{{layout:.*;comp:.*;temp:.*}};",
          "category": "{{.*}}",
          "item_name": "{{.*}}",
          "promo_code": "{{.*}}",
          "promo_id": "{{.*}}",
          "creative_slot": "{{\\d{1,}}}"
        }
      ]
    },
    {
      "eventCategory": "product detail page",
      "eventAction": "click - choose product variant",
      "event": "clickPDP",
      "productId": "{{\\d{1,}}}",
      "sessionIris": "{{.*}}",
      "eventLabel": "{{level : \\d{1,};variant_title : .*;variant_value: .*;variant : \\d{1,}}};",
      "layout": "{{layout:.*;catName:.*;catId:\\d{1,}}};",
      "component": "{{comp:.*;temp:.*;elem:.*;cpos:\\d{1,}}};"
    },
    {
      "eventCategory": "product detail page",
      "eventAction": "click - tambah ke keranjang on pdp",
      "event": "add_to_cart",
      "layout": "{{.*}}",
      "productId": "{{\\d{1,}}}",
      "user_id": "{{.*}}",
      "isLoggedInStatus": "{{true|false}}",
      "shopId": "{{\\d{1,}}}",
      "shopType": "{{.*}}",
      "items": [
        {
          "item_category": "{{.*}}",
          "item_brand": "none / other",
          "dimension54": "{{.*}}",
          "dimension80": "{{.*}}",
          "dimension81": "{{.*}}",
          "dimension45": "{{\\d{1,}}}",
          "dimension83": "none / other",
          "dimension40": "{{.*}}",
          "dimension79": "{{\\d{1,}}}",
          "item_id": "{{\\d{1,}}}",
          "quantity": "{{\\d*.\\d*|\\d.\\d+E\\d}}",
          "price": "{{\\d*.\\d*|\\d.\\d+E\\d}}",
          "item_variant": "{{.*}}",
          "item_name": "{{.*}}",
          "dimension82": "{{.*}}",
          "dimension38": "{{.*}}"
        }
      ],
      "eventLabel": "{{.*}}"
    }
  ]
}