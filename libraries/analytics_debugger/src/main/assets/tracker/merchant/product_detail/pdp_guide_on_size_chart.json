{
  "readme": "Product Detail - click guide on the size chart",
  "mode": "exact",
  "query": [
    {
      "eventCategory": "product detail page",
      "eventAction": "{{.*}}",
      "event": "view_item",
      "categoryId": "{{productId : \\d{1,}}}",
      "productId": "{{\\d{1,}}}",
      "sessionIris": "{{.{1,}}}",
      "eventLabel": "{{.*}}",
      "layout": "{{layout:.*;catName:.*;catId:\\d{1,};}}",
      "component": "{{comp:.*;temp:.*;elem:.*;cpos:\\d{1,};}}",
      "promotions": [
        {
          "item_id": "{{.*}}",
          "creative_url": "{{.*}}",
          "creative_name": "{{layout:.*;comp:.*;temp:.*;}}",
          "category": "{{.*}}",
          "item_name": "{{.*}}",
          "promo_code": "{{.*}}",
          "promo_id": "{{.*}}",
          "creative_slot": "{{\\d{1,}}}"
        }
      ]
    },
    {
      "event": "clickPDP",
      "eventCategory": "product detail page",
      "eventAction": "{{.*}}",
      "eventLabel": "{{.*}};",
      "productId": "{{\\d{1,}}}",
      "layout": "{{layout:.*;catName:.*;catId:\\d{1,};}}",
      "sessionIris": "{{.{1,}}}",
      "component": "{{comp:.*;temp:.*;elem:.*;cpos:\\d{1,};}}"
    }
  ]
}