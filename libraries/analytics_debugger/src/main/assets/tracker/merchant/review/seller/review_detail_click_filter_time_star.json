{
  "readme": "Review Detail - click filter time and star",
  "mode": "exact",
  "query": [
    {
      "event" : "clickReview",
      "eventCategory" : "review detail page",
      "eventAction" : "click - time filter",
      "eventLabel" : "{{.*}}",
      "shopId": "{{\\d{1,}}}",
      "productId" : "{{\\d{1,}}}",
      "screenName" : "review detail page",
    },
    {
      "event" : "clickReview",
      "eventCategory" : "review detail page",
      "eventAction" : "click - apply time filter",
      "eventLabel" : "{{filterBy:.*;}}",
      "shopId": "{{\\d{1,}}}",
      "productId" : "{{\\d{1,}}}",
      "screenName" : "review detail page"
    },
    {
      "event" : "clickReview",
      "eventCategory" : "review detail page",
      "eventAction" : "click - star filter",
      "eventLabel" : "{{star:.*}}",
      "isActive" : "{{true|false}}",
      "shopId": "{{\\d{1,}}}",
      "productId" : "{{\\d{1,}}}",
      "screenName" : "review detail page"
    }
  ]
}