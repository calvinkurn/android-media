{
  "readme": "Flight Homepage Tracking",
  "mode": "exact",
  "query": [
    {
      "event": "genericFlightEvent",
      "eventAction": "select origin",
      "eventCategory": "digital - flight",
      "eventLabel": "{{.*}}",
      "sessionIris": "{{.*}}"
    },
    {
      "event": "genericFlightEvent",
      "eventAction": "select destination",
      "eventCategory": "digital - flight",
      "eventLabel": "{{.*}}",
      "sessionIris": "{{.*}}"
    },
    {
      "event": "genericFlightEvent",
      "eventAction": "select flight class",
      "eventCategory": "digital - flight",
      "eventLabel": "{{.*}}",
      "sessionIris": "{{.*}}"
    },
    {
      "event": "genericFlightEvent",
      "eventAction": "select Passenger",
      "eventCategory": "digital - flight",
      "eventLabel": "{{\\d* .* - \\d* .* - \\d* .*}}",
      "sessionIris": "{{.*}}"
    },
    {
      "event": "clickSearch",
      "eventAction": "click search flight",
      "eventCategory": "digital - flight",
      "eventLabel": "{{flight - .*-.* - .* - \\d*-\\d*-\\d* - .* - .*}}",
      "sessionIris": "{{.*}}"
    },
    {
      "event": "genericFlightEvent",
      "eventAction": "select trip type",
      "eventCategory": "digital - flight",
      "eventLabel": "{{.*}}",
      "sessionIris": "{{.*}}"
    },
    {
      "event": "view_item",
      "eventAction": "banner impression",
      "eventCategory": "digital - flight",
      "eventLabel": "{{flight - \\d* - .*}}",
      "sessionIris": "{{.*}}",
      "promotions": [
        {
          "item_id": "{{.*}}",
          "item_name": "{{.* - slider banner}}",
          "creative_slot": "{{\\d*}}",
          "creative_name": "{{.*}}",
          "creative_url": "{{.*}}"
        }
      ]
    },
    {
      "event": "select_content",
      "eventAction": "click banner",
      "eventCategory": "digital - flight",
      "eventLabel": "{{flight - \\d* - .*}}",
      "sessionIris": "{{.*}}",
      "promotions": [
        {
          "item_id": "{{.*}}",
          "item_name": "{{.* - slider banner}}",
          "creative_slot": "{{\\d*}}",
          "creative_name": "{{.*}}",
          "creative_url": "{{.*}}"
        }
      ]
    }
  ]
}