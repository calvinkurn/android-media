<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_websocket_detail_log_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/spacing_lvl4"
            android:layout_marginTop="@dimen/spacing_lvl4"
            android:layout_marginEnd="@dimen/spacing_lvl4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:typographyType="heading_3"
            app:typographyWeight="bold"
            tools:text="Contoh Event Name" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_websocket_detail_log_date_time"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_lvl2"
            android:ellipsize="end"
            android:maxLines="1"
            android:textColor="@color/black_55"
            app:layout_constraintEnd_toEndOf="@id/tv_websocket_detail_log_title"
            app:layout_constraintStart_toStartOf="@id/tv_websocket_detail_log_title"
            app:layout_constraintTop_toBottomOf="@id/tv_websocket_detail_log_title"
            app:typographyType="body_3"
            tools:text="11 Nov 2021 12:12:12.123" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_websocket_detail_log_channel_id_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_lvl3"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="@string/websocket_log_channel_id_label"
            app:layout_constraintEnd_toStartOf="@id/tv_websocket_detail_log_channel_id"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintStart_toStartOf="@id/tv_websocket_detail_log_title"
            app:layout_constraintTop_toBottomOf="@id/tv_websocket_detail_log_date_time"
            app:typographyType="body_3"
            app:typographyWeight="bold" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_websocket_detail_log_channel_id"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/spacing_lvl3"
            android:ellipsize="end"
            android:maxLines="1"
            app:layout_constraintBottom_toBottomOf="@id/tv_websocket_detail_log_channel_id_label"
            app:layout_constraintEnd_toEndOf="@id/tv_websocket_detail_log_title"
            app:layout_constraintHorizontal_weight="2"
            app:layout_constraintStart_toEndOf="@id/tv_websocket_detail_log_channel_id_label"
            app:layout_constraintTop_toTopOf="@id/tv_websocket_detail_log_channel_id_label"
            app:typographyType="body_3"
            tools:text="123123" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_websocket_detail_log_gc_token_label"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_lvl2"
            android:ellipsize="end"
            android:maxLines="1"
            android:text="@string/websocket_log_gc_token_label"
            app:layout_constraintEnd_toStartOf="@id/tv_websocket_detail_log_gc_token"
            app:layout_constraintHorizontal_weight="1"
            app:layout_constraintStart_toStartOf="@id/tv_websocket_detail_log_title"
            app:layout_constraintTop_toBottomOf="@id/tv_websocket_detail_log_channel_id"
            app:typographyType="body_3"
            app:typographyWeight="bold" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_websocket_detail_log_gc_token"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/spacing_lvl3"

            app:layout_constraintEnd_toEndOf="@id/tv_websocket_detail_log_title"
            app:layout_constraintHorizontal_weight="2"
            app:layout_constraintStart_toEndOf="@id/tv_websocket_detail_log_gc_token_label"
            app:layout_constraintTop_toTopOf="@id/tv_websocket_detail_log_gc_token_label"
            app:typographyType="body_3"
            tools:text="" />

        <com.tokopedia.unifyprinciples.Typography
            android:id="@+id/tv_websocket_detail_log_message"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/spacing_lvl3"
            android:layout_marginBottom="@dimen/spacing_lvl3"
            android:textColor="@color/black_55"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="@id/tv_websocket_detail_log_title"
            app:layout_constraintStart_toStartOf="@id/tv_websocket_detail_log_title"
            app:layout_constraintTop_toBottomOf="@id/tv_websocket_detail_log_gc_token"
            app:typographyType="body_3"
            tools:text="This is message" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</ScrollView>