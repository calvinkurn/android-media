query OfficialStoreFeaturedShop($categoryAliasID: Int) {
   OfficialStoreFeaturedShop(categoryAliasID: $categoryAliasID, device: 4, size: 4){
      totalShops
      header{
        ctaText
        title
        link
      }
      shops{
        id
        name
        url
        logoUrl
        imageUrl
        additionalInformation
        featuredBrandId
      }
    }
}