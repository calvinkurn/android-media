query SearchProfile(
 $params: String
) {
aceSearchProfile(params: $params) {
    query
    source
    count
    count_text
    has_next
    profiles {
      id
      name
      avatar
      username
      followed
      iskol
      isaffiliate
      followers
      post_count
    }
    top_profile {
      id
      name
      avatar
      username
      followed
      iskol
      isaffiliate
      followers
      post_count
    }
  }
}