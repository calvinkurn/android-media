query GetShopRating($shopId: Int!){
  ShopRatingQuery(shopId: $shopId) {
    ratingScore
    starLevel
    totalReview
    detail {
      fiveStar {
        rate
        totalReview
        percentageWord
        percentage
      }
      fourStar {
        rate
        totalReview
        percentageWord
        percentage
      }
      threeStar {
        rate
        totalReview
        percentageWord
        percentage
      }
      twoStar {
        rate
        totalReview
        percentageWord
        percentage
      }
      oneStar {
        rate
        totalReview
        percentageWord
        percentage
      }
    }
  }
}