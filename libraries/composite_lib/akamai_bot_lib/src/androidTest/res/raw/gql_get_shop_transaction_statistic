query getShopTransactionStatistic($shopID: Int!){
     shopTransactionStatistic(shopID: $shopID) {
          shop_tx_stats {
            shop_tx_has_transaction
            shop_tx_has_transaction_6_month
            shop_tx_has_transaction_3_month
            shop_tx_has_transaction_1_month
            shop_tx_show_percentage_6_month
            shop_tx_show_percentage_3_month
            shop_tx_show_percentage_1_month
            shop_tx_success_rate_6_month
            shop_tx_success_rate_3_month
            shop_tx_success_rate_1_month
            shop_tx_success_6_month_fmt
            shop_tx_success_3_month_fmt
            shop_tx_success_1_month_fmt
          }
        }
 }