query OfficialStoreAllBrands($categoryId: Int!, $device: Int!, $offset: Int!, $query: String!, $size: Int!, $sort: Int!, $firstLetter: String!) {
  OfficialStoreAllBrands(categoryId: $categoryId, device: $device, offset: $offset, query: $query, size: $size, sort: $sort, firstLetter: $firstLetter) {
    brands {
      id
      name
      appsUrl
      defaultUrl
      logoUrl
      isNew
      exclusiveLogoURL
    }
    totalBrands
  }
}