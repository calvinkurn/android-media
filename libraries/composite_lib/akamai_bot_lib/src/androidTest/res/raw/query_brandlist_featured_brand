query OfficialStoreFeaturedShop($categoryAliasID: Int) {
    OfficialStoreFeaturedShop(categoryAliasID: $categoryAliasID, device: 4, size: 24) {
        shops{
            id
            name
            url
            logoUrl
            imageUrl
            additionalInformation
            featuredBrandId
        }
        totalShops
        header{
            title
            ctaText
            link
        }
    }
}