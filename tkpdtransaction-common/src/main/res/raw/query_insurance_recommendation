query getTransactionalProducts($digitalProducts: InsuretechDigitalProductRequestInput!) {
  insuretechGetTransactionalProducts(InsuretechProductData: $digitalProducts) {
    shops{
      shop_id
      items{
        product_id
        digital_product{
          digital_product_id
          type_id
          price_per_product
          total_price
          opt_in
          is_product_level
          is_seller_money
          is_application_needed
          is_purchase_protection
          product_info{
            title
            sub_title
            description
            section_title
            ticker_text
            icon_url
            app_link_url
            link_name
            web_link_html
          }
          application_details{
            id
            label
            place_holder
            type
            required
            value
            values{
              id
              value
            }
            validations{
              type
              value
              error_message
            }
          }
        }
      }
    }
  }
}