<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableLayoutStyleable">
        <attr name="expandable_layout_duration" format="integer"/>
        <attr name="expandable_layout_expanded" format="boolean"/>
        <attr name="expandable_layout_defaultChildIndex" format="integer"/>
        <attr name="expandable_layout_defaultPosition" format="dimension"/>
        <attr name="expandable_layout_orientation" format="enum">
            <enum name="horizontal" value="0"/>
            <enum name="vertical" value="1"/>
        </attr>
        <attr name="expandable_layout_interpolator" format="enum">
            <enum name="accelerateDecelerate" value="0"/>
            <enum name="accelerate" value="1"/>
            <enum name="anticipate" value="2"/>
            <enum name="anticipateOvershoot" value="3"/>
            <enum name="bounce" value="4"/>
            <enum name="decelerate" value="5"/>
            <enum name="fastOutLinearIn" value="6"/>
            <enum name="fastOutSlowIn" value="7"/>
            <enum name="linear" value="8"/>
            <enum name="linearOutSlowIn" value="9"/>
            <enum name="overshoot" value="10"/>
        </attr>
    </declare-styleable>
    <declare-styleable name="DoubleText">
        <attr name="topText" format="string" />
        <attr name="bottomText" format="string" />
        <attr name="android:ems" />
    </declare-styleable>
</resources>