<resources>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="TextAppearance.Title.Collapsed" parent="android:TextAppearance">
        <item name="android:textColor">@color/black_70</item>
        <item name="android:textSize">16sp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="TextAppearance.Title.Expanded" parent="android:TextAppearance">
        <item name="android:textColor">@color/black_70</item>
        <item name="android:textSize">24sp</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>

    <style name="TransactionToogleButton" parent="Theme.AppCompat.Light">
        <item name="colorControlActivated">#42b549</item>
        <item name="colorSwitchThumbNormal">#f1f1f1</item>
    </style>



    <style name="TextSmall">
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextSmallNormal" parent="TextSmall">
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="TextSmallNormalBlack38" parent="TextSmallNormal">
        <item name="android:textColor">@color/black_38</item>
    </style>

    <style name="TextSmallNormalBlack70" parent="TextSmallNormal">
        <item name="android:textColor">@color/black_70</item>
    </style>

    <style name="TextSmallMedium" parent="TextSmall">
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="TextSmallMediumBlack38" parent="TextSmallMedium">
        <item name="android:textColor">@color/black_38</item>
    </style>

    <style name="TextSmallMediumBlack70" parent="TextSmallMedium">
        <item name="android:textColor">@color/black_70</item>
    </style>



    <style name="TextMid">
        <item name="android:textSize">14sp</item>
    </style>

    <style name="TextMidNormal" parent="TextMid">
        <item name="android:fontFamily">sans-serif</item>
    </style>

    <style name="TextMidMedium" parent="TextMid">
        <item name="fontFamily">sans-serif-medium</item>
    </style>

    <style name="TextMidNormalBlack70" parent="TextMidNormal">
        <item name="android:textColor">@color/black_70</item>
    </style>

</resources>
