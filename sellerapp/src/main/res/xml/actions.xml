<?xml version="1.0" encoding="utf-8"?>
<actions>
    <action intentName="actions.intent.GET_ORDER">
        <fulfillment
            fulfillmentMode="actions.fulfillment.SLICE"
            urlTemplate="content://com.tokopedia.seller.action.slices/order{?orderDate}">
            <parameter-mapping
                intentParameter="order.orderDate"
                urlParameter="orderDate"/>
        </fulfillment>
    </action>
    <action intentName="actions.intent.GET_THING">
        <fulfillment
            fulfillmentMode="actions.fulfillment.DEEPLINK"
            urlTemplate="sellerapp://seller-search{?keyword}">
            <parameter-mapping
                intentParameter="thing.name"
                required="true"
                urlParameter="keyword"/>
        </fulfillment>
        <fulfillment
            fulfillmentMode="actions.fulfillment.DEEPLINK"
            urlTemplate="sellerapp://seller-search">
        </fulfillment>
    </action>
</actions>