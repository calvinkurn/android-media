<?xml version="1.0" encoding="utf-8"?>
<actions>
    <action intentName="actions.intent.GET_ORDER">
        <fulfillment
            fulfillmentMode="actions.fulfillment.SLICE"
            urlTemplate="content://com.tokopedia.seller.action.slices/order">
        </fulfillment>
    </action>
    <action intentName="action.intent.GET_REVIEW">
        <fulfillment
            fulfillmentMode="actions.fulfillment.SLICE"
            urlTemplate="content://com.tokopedia.seller.action.slices/stars{?rating}">
            <parameter-mapping
                intentParameter="review.reviewRating.ratingValue"
                required="true"
                urlParameter="rating"/>
        </fulfillment>
        <fulfillment
            fulfillmentMode="actions.fulfillment.DEEPLINK"
            urlTemplate="tokopedia-android-internal://marketplace/stars"/>
    </action>
</actions>