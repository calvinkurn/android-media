<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/margin_padding_large"
    android:background="@color/white"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="@dimen/tnc_header_height"
        android:gravity="center"
        android:text="@string/syarat_amp_ketentuan"
        android:textColor="@color/black_70"
        android:textSize="@dimen/fontl" />

    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/thin_line"
        android:layout_marginBottom="@dimen/margin_sm"
        android:background="@android:color/darker_gray" />

    <com.tokopedia.abstraction.base.view.webview.TkpdWebView
        android:id="@+id/webview"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</LinearLayout>