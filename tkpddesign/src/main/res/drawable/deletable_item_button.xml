<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <stroke android:width="1dp" android:color="@color/Unify_N100"/>
            <solid android:color="@color/Unify_N50"/>
        </shape>
    </item>
    <item android:top="8dp" android:left="8dp" android:right="8dp" android:bottom="8dp">
        <rotate android:fromDegrees="45" android:toDegrees="45" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="line">
                <stroke android:width="2dp" android:color="@color/Unify_N150"/>
            </shape>
        </rotate>
    </item>
    <item android:top="8dp" android:left="8dp" android:right="8dp" android:bottom="8dp">
        <rotate android:fromDegrees="315" android:toDegrees="315" android:pivotX="50%" android:pivotY="50%">
            <shape android:shape="line">
                <stroke android:width="2dp" android:color="@color/Unify_N150"/>
            </shape>
        </rotate>
    </item>
</layer-list>
