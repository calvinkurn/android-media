<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="PinEditText">
        <attr name="AnimationType" format="enum">
            <enum name="po" value="0" />
            <enum name="fromBottom" value="1" />
            <enum name="none" value="-1" />
        </attr>
        <attr name="CharacterMask" format="string" />
        <attr name="RepeatedHint" format="string" />
        <attr name="LineStroke" format="dimension" />
        <attr name="LineStrokeSelected" format="dimension" />
        <attr name="CharacterSpacing" format="dimension" />
        <attr name="TextBottomPadding" format="dimension" />
        <attr name="LineColors" format="color" />
        <attr name="BackgroundDrawable" format="reference" />
        <attr name="BackgroundIsSquare" format="boolean" />
    </declare-styleable>
</resources>