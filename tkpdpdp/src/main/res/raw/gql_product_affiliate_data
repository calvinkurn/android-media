query topAdsPDPAffiliate($productId:[Int]!, $shopId:Int!, $userId:Int!){
  topAdsPDPAffiliate(productId:$productId, shopId:$shopId, userId:$userId, includeUI:false){
    data{
      affiliate{
        AdId
        ProductId
        AffiliatedByUser
        CommissionPercent
        CommissionPercentDispay
        CommissionValue
        CommissionValueDisplay
        UniqueURL
      }
    }
  }
}